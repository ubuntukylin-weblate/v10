msgstr ""
"སྒེའུ་ཁུང་གསར་པས་ཇི་ལྟར་བྱས་ནས་མདོ་ཚེག་ཐོབ་ལེན་བྱེད་པ་ཚོད་འཛིན་བྱེད་དགོས།"

msgid ""
"Coordinate expression contained unknown operator at the start of this text: "
"\"%s\""
msgstr ""
"གནས་ཚད་མཚོན་ཚུལ་ལ་ཡིག་དེབ་འདི་འགོ་ཚུགས་སར་ངེས་མེད་ཀྱི་རྩིས་རྒྱག་མཚོན་རྟགས་འདུས་པ།：“%s”"

msgid "Coordinate expression contains character '%s' which is not allowed"
msgstr "གནས་ཚད་མཚོན་ཚུལ་ནང་དུ་མི་ཆོག་པའི་ཡིག་རྟགས“%s”ཚུད་ཡོད།"

msgid ""
"Coordinate expression contains floating point number '%s' which could not be"
" parsed"
msgstr "གནས་ཚད་མཚོན་ཚུལ་ནང་དབྱེ་ཞིབ་བྱེད་ཐབས་བྲལ་པའི་འཕྱོ་ཚེག་གྲངས་%sཚུད་ཡོད།"

msgid "Coordinate expression contains integer '%s' which could not be parsed"
msgstr "གནས་ཚད་མཚོན་ཚུལ་ནང་དབྱེ་ཞིབ་བྱེད་ཐབས་བྲལ་པའི་ཧྲིལ་གྲངས་%sཚུད་ཡོད།"

msgid "Coordinate expression doesn't seem to have any operators or operands"
msgstr "གནས་ཚད་མཚོན་ཚུལ་ལ་རྩིས་རྒྱག་རྟགས་སམ་བཀོལ་སྤྱོད་གྲངས་གང་ཡང་མེད་པ་འདྲ།"

msgid "Coordinate expression ended with an operator instead of an operand"
msgstr ""
"གནས་ཚད་མཚོན་ཚུལ་ནི་རྩིས་རྒྱག་རྟགས་ཀྱིས་མཇུག་སྡུད་པ་ལས་བཀོལ་སྤྱོད་གྲངས་ཀྱིས་མཇུག་སྡུད་པ་མ་ཡིན།"

msgid "Coordinate expression had a close parenthesis with no open parenthesis"
msgstr "གནས་ཚད་མཚོན་ཚུལ་ལ་ཟུམ་པའི་གུག་རྟགས་ཤིག་ཡོད་པ་ལས་ཕྱེ་པའི་གུག་རྟགས་མེད།"

msgid ""
"Coordinate expression had an open parenthesis with no close parenthesis"
msgstr "གནས་ཚད་མཚོན་ཚུལ་ལ་ཕྱེ་བའི་གུག་རྟགས་ཤིག་ཡོད་པ་ལས་ཟུམ་པའི་གུག་རྟགས་མེད།"

msgid "Coordinate expression had an operand where an operator was expected"
msgstr ""
