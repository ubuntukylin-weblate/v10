msgid ""
"\"%s\" found in configuration database is not a valid value for keybinding "
"\"%s\"\n"
msgstr ""
"གཞི་གྲངས་མཛོད་སྡེབ་སྒྲིག་ཁྲོད་རྙེད་པའི%sནི་མཐེབ་གནོན་ཚན་སྡེབ་%sཡི་གོ་ཆོད་ཐང་མ་རེད།"

msgid ""
"\"%s\" found in configuration database is not a valid value for mouse button"
" modifier\n"
msgstr ""
"གཞི་གྲངས་མཛོད་སྡེབ་སྒྲིག་ཁྲོད་རྙེད་པའི%sནི་ཙིག་རྟགས་མཐེབ་ཀྱི་རྒྱན་མཐེབ་ཀྱི་གོ་ཆོད་ཐང་མ་རེད།"

msgid "\"%s\" is not a valid value for focus attribute"
msgstr "%sནི་focusངོ་བོའི་ནུས་ལྡན་གྱི་ཐང་མིན།"

msgid "\"%s\" is not a valid value for resize attribute"
msgstr "%sནི་resizeངོ་བོའི་ནུས་ལྡན་གྱི་གྲངས་ཐང་མིན།"

msgid "\"%s\" is not a valid value for state attribute"
msgstr "s%ནི་Sateངོ་བོའི་ནུས་ལྡན་གྱི་གྲངས་ཐང་མིན།"

msgid "%d coordinate expressions parsed in %g seconds (%g seconds average)\n"
msgstr ""
"སྐར་ཆ་%2$g ཡི་ནང་གནས་ཚད་མཚོན་ཚུལ་%1$d སྒྲིག་གཅོད་བྱས།（ཆ་སྙོམས་སྐར་ཆ་%3$g ）\n"
"\""

msgid "%d stored in GSettings key %s is out of range %d to %d\n"
msgstr ""
"MateConfཁྲོད་དུ་ཉར་གསོག་བྱས་པའི་ %2$sཡི་ཐང་%1$dདེ་%3$d ནས་%4$d "
"ཡི་ཁྱབ་ཁོངས་ནས་བརྒལ་བ།\n"

msgid "%d x %d"
msgstr "%d x %d"

msgid "%g milliseconds to draw one window frame"
msgstr "ཧའོ་སྐར་ཆ་g%སྤྱད་ནས་སྒེའུ་ཁུང་གི་སྒྲོམ་གཞི་ཞིག་འབྲི་བ།"

