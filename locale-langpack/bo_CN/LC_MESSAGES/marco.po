msgstr "%s（རིམ་བརྒལ་སྤྱོད་མཁན་བྱས་ནས།）"

msgid "%s (on %s)"
msgstr "\"%s། ( %sལ)\""

msgid "<%s> geometry \"%s\" has not been defined"
msgstr "\"<%s> དབྱིབས་རྩིས་བཀོད་པ“%s”ད་དུང་མཚན་ཉིད་བཞག་མེད།\""

msgid "<%s> must specify either a geometry or a parent that has a geometry"
msgstr ""
"<%s>ངེས་པར་དུ་དབྱིབས་རྩིས་ཀྱི་བཀོད་པ་ཞིག་གམ་དབྱིབས་རྩིས་ཀྱི་བཀོད་པ་ཡོད་པའི་ཕ་རིམ་ཞིག་དམིགས་འཛུགས་བྱེད་དགོས།"

msgid "<%s> name \"%s\" used a second time"
msgstr "ཡང་བསྐྱར་<%s> མིང་གི་%sབེད་སྤྱོད་བྱས་པ།"

msgid "<%s> parent \"%s\" has not been defined"
msgstr "<%s> ཕ་རིམ་པའི%sལ་ད་དུང་མཚན་ཉིད་བཞག་མེད།"

msgid "<%s> specified twice for this theme"
msgstr "དེར་བརྟེན་བརྗོད་དོན་གཙོ་བོས་<%s>ཐེངས་གཉིས་ལ་དམིགས་འཛུགས་བྱས།"

