msgid "%s Properties"
msgstr "%s གཏོགས་གཤིས།"

msgid "%s is currently using this computer"
msgstr "%s ཡིས་རྩིས་འཁོར་འདི་སྤྱོད་བཞིན་པ།"

msgid ""
"%s is the only administrator on this computer. Deleting this account would "
"lock you out of administrating the system."
msgstr ""
"%s ནི་རྩིས་འཁོར་སྟེང་གི་དོ་དམ་པ་གཅིག་པུ་ཡིན་པས། "
"རྩིས་ཐོ་འདི་སུབ་ཚེ་ཁྱེད་ཀྱིས་རྒྱུད་ཁོངས་དོ་དམ་མི་ཐུབ།"

msgid ""
"%s is the only administrator on this computer. Revoking administration "
"rights for this account would lock you out of administrating the system."
msgstr ""
"%s ནི་རྩིས་འཁོར་སྟེང་གི་དོ་དམ་པ་གཅིག་པུ་ཡིན་པས། "
"སྤྱོད་མཁན་འདི་ཡི་དོ་དམ་ཆས་དབང་ཚད་དོར་ཚེ་ཁྱེད་ཀྱིས་རྒྱུད་ཁོངས་དོ་དམ་མི་ཐུབ།"

msgid ""
"%s will no longer be able to perform administrative tasks. This account "
"won't be allowed to get administration rights back on its own."
msgstr ""
"%s ཡིས་དོ་དམ་ལས་འགན་ལག་བསྟར་མི་ཐུབ་པས། "
"རྩིས་ཐོ་འདི་ལ་དོ་དམ་དབང་ཚད་ཡང་བསྐྱར་ཐོབ་མི་ཐུབ།"

msgid "<b>Essid:</b> %s "
msgstr "<b>Essid：</b>%s"

msgid "A group name must be specified."
msgstr "ངེས་པར་དུ་ཚོ་ཆུང་གི་མིང་དམིགས་འཛུགས་བྱེད་དགོས།"

msgid ""
"A user with this name already exists. Please choose a different name for the"
" new user."
msgstr ""
"མིང་འདི་སྤྱོད་པའི་སྤྱོད་མཁན་གནས་ཟིན་འདུག་པས། "
"སྤྱོད་མཁན་གསར་པར་མིང་མི་འདྲ་བ་ཞིག་འདེམས་དགོས།"

msgid "Access /proc filesystem"
msgstr "/proc ཡིག་ཆའི་རྒྱུད་ཁོངས་ལ་ལྟ་སྤྱོད།"

msgid "Access external storage devices automatically"
msgstr "ཕྱིའི་གསོག་འཇོག་སྒྲིག་ཆས་ལ་རང་འགུལ་ངང་ལྟ་སྤྱོད་བྱེད་པ།"

msgid "Access point name:"
msgstr "ཚེག་གི་མིང་ལ་ལྟ་སྤྱོད།:"

msgid "Account _type:"
msgstr "རྩིས་ཐོའི་རིགས་གྲས། (_T)："

msgid "Account data"
msgstr "རྩིས་ཐོའི་གཞི་གྲངས།"

msgid "Account information resolver"
msgstr "རྩིས་ཐེམ་གྱི་ཆ་འཕྲིན་ཞིབ་འགྲེལ་ཆས།"

msgid "Actions scheduler"
msgstr "འགུལ་སྟངས་འཆར་གཞི།"

msgid "Add NTP Server"
msgstr "ཞབས་ཞུ་ཆས NTP སྣོན་པ།"

msgid "Add a shared path, modifies it if it already exists"
msgstr "མཉམ་སྤྱོད་བརྒྱུད་ལམ་སྣོན་པ། གལ་ཏེ་ཡོད་ཚེ་བཟོ་བཅོས་བྱེད་དགོས།"

msgid "Add allowed hosts"
msgstr "ཆོག་པའི་གཙོ་འཁོར་སྣོན་པ།"

msgid "Add or remove users and groups"
msgstr ""

msgid "Address:"
msgstr "ཤག་གནས།："

msgid "Administer the system"
msgstr "དོ་དམ་རྒྱུད་ཁོངས།"

msgid "Administrator"
msgstr "དོ་དམ་པ།"

msgid "Administrator account cannot be deleted"
msgstr "དོ་དམ་པའི་རྩིས་ཐོ་སུབ་མི་ཐུབ་པ།"

msgid "Administrator group can not be deleted"
msgstr "དོ་དམ་པའི་ཚོ་ཆུང་སུབ་མི་ཐུབ་པ།"

msgid "Advanced"
msgstr "མཐོ་རིམ།"

msgid "Advanced Settings"
msgstr "མཐོ་རིམ་སྒྲིག་འགོད།"

msgid "Advanced settings:"
msgstr "མཐོ་རིམ་སྒྲིག་འགོད།:"

msgid "Aliases"
msgstr "མིང་གཞན།"

msgid "Aliases:"
msgstr "མིང་གཞན།"

msgid "Allowed Hosts"
msgstr "ཆོག་པའི་གཙོ་འཁོར།"

msgid "Allowed host/network"
msgstr "ཆོག་པའི་གཙོ་འཁོར / དྲ་རྒྱ།"

msgid "Allowed hosts:"
msgstr "ཆོག་པའི་གཙོ་འཁོར།："

msgid "Allows applications to use printers"
msgstr "ཉེར་སྤྱོད་བྱ་རིམ་གྱིས་པར་འཁོར་སྤྱད་ཆོག་པ།"

msgid "Allows users to login graphically"
msgstr "སྤྱོད་མཁན་གྱིས་རིས་དབྱིབས་ཚུལ་གྱིས་ཐོ་འགོད་པ།"

msgid ""
"An error occurred while checking for authorizations: %s\n"
"You may report this as a bug."
msgstr ""

msgid "An unknown error occurred."
msgstr "མི་ཤེས་པའི་ནོར་འཁྲུལ་བྱུང་། "

msgid "Analyzes your incoming mail for virus"
msgstr "ཁྱེད་ལ་འབྱོར་བའི་ཡིག་ཟམ་ནང་གི་ནད་དུག་ལ་བཤར་འབེབས་པ།"

msgid "Antivirus"
msgstr "ནད་དུག་ལ་ལྡོག་པ།"

msgid "Application server"
msgstr "ཞབས་ཞུ་ཆས་བཀོལ་སྤྱོད།"

msgid "Applying changes to user settings..."
msgstr "བཟོ་བཅོས་དེ་སྤྱོད་མཁན་གྱི་སྒྲིག་འགོད་ལ་བཀོལ་སྤྱོད་བྱེད་པ།"

msgid "Are you sure you want to deactivate %s?"
msgstr "ཁྱེད་ཀྱིས %s སྤྱོད་འགོག་བྱེད་ངེས་ཡིན་ནམ།"

msgid "Are you sure you want to delete group \"%s\"?"
msgstr "ཚོ་ཆུང “%s” སུབ་ངེས་ཡིན་ནམ།"

msgid "Are you sure you want to delete this share?"
msgstr "མཉམ་སྤྱོད་འདི་སུབ་ངེས་ཡིན་ནམ།"

msgid "Asked on login"
msgstr "ཐོ་འགོད་སྐབས་འདྲི་རྩད་བྱེད་པ།"

msgid "Assign a random password by default"
msgstr "སྐབས་བསྟུན་གསང་ཨང་ཞིག་སོར་བཞག་བགོ་བཤའ་བྱེད་པ།"

msgid "Audio settings management"
msgstr "སྒྲ་ཟློས་སྒྲིག་འགོད་དོ་དམ།"

msgid "Authentication failed"
msgstr "ར་སྤྲོད་ཕམ་པ།"

msgid "Automated crash reports support"
msgstr "རྩ་ཐོར་གྱི་རྒྱབ་སྐྱོར་རང་འགུལ་ངང་སྙན་ཞུ་བྱེད་པ།"

msgid "Automatic UID/GID"
msgstr "རང་འགུལ UID/GID"

msgid "Automatic configuration (DHCP)"
msgstr "རང་འགུལ་སྡེབ་སྒྲིག (DHCP)"

msgid "Basic Settings"
msgstr "རྩ་བའི་སྒྲིག་འགོད།"

msgid "Be able to get administrator privileges"
msgstr "དོ་དམ་པའི་དབང་ཚད་ཐོབ་ཐུབ།"

msgid "Blocks undesired network access to your computer"
msgstr ""
"ཕྱི་རོལ་གྱིས་ཁྱེད་ཀྱི་རྩིས་འཁོར་ལ་བྱེད་པའི་ངེས་མེད་དྲ་རྒྱའི་ལྟ་སྤྱོད་སྒྲིབ་པ།"

msgid "Bluetooth device management"
msgstr "སོ་སྔོན་གྱི་སྒྲིག་ཆས་དོ་དམ།"

msgid "Braille display management"
msgstr "Braille དོ་དམ་མངོན་པ།"

msgid "Burn CDs / DVDs"
msgstr "CD/DVD རྐོ་ཕབ།"

msgid "CPU Frequency manager"
msgstr "CPU ཡི་ཟློས་ཕྱོད་དོ་དམ་ཆས།"

msgid ""
"Can change anything on the system, including installing and upgrading "
"software."
msgstr ""
"སྒྲིག་འཇུག་དང་མཉེན་ཆས་རིམ་སྤར་ཚུད་པའི་རྒྱུད་ཁོངས་སྟེང་གི་ཡོད་ཚད་ལ་བཟོ་བཅོས་བྱས་ཆོག"

msgid ""
"Can perform common tasks. Can't install software or change settings "
"affecting all users."
msgstr ""
"སྤྱིར་བཏང་གི་ལས་འགན་ལག་བསྟར་མི་ཐུབ་པ། "
"མཉེན་ཆས་སྒྲིག་འཇུག་བྱས་མི་ཆོག་པའམ་ཡང་ན་བཟོ་བཅོས་ཀྱིས་སྤྱོད་མཁན་ཚང་མའི་སྒྲིག་འགོད་ལ་ཤུགས་རྐྱེན་ཐེབས་མི་ཆོག"

msgid "Can't delete the only administrator account"
msgstr "དོ་དམ་པའི་རྩིས་ཐོ་གཅིག་པུ་སུབ་མི་རུང་།"

msgid "Can't revoke administration rights"
msgstr "དོ་དམ་པའི་དབང་ཚད་ཕྱིར་འཐེན་བྱེད་མི་ཐུབ་པ།"

msgid "Change Advanced User Settings"
msgstr "མཐོ་རིམ་སྤྱོད་མཁན་གྱི་སྒྲིག་འགོད་བཟོ་བཅོས།"

msgid "Change User Account Type"
msgstr "སྤྱོད་མཁན་གྱི་རྩིས་ཐོའི་རིགས་གྲས་བཟོ་བཅོས།"

msgid "Change User Name"
msgstr "སྤྱོད་མཁན་གྱི་མིང་བསྒྱུར་བ།"

msgid "Change User Password"
msgstr "སྤྱོད་མཁན་གྱི་གསང་བརྡ་བཟོ་བཅོས།"

msgid "Change _Host name"
msgstr "གཙོ་འཁོར་མིང་བཟོ་བཅོས། (_H)"

msgid "Change system time, date, and timezone"
msgstr "རྒྱུད་ཁོངས་ཀྱི་དུས་ཚོད་དང་། ཚེས་གྲངས། དུས་ཁུལ་བཅས་ལ་བཟོ་བཅོས་བྱེད་པ།"

msgid "Changing advanced settings for:"
msgstr "ལ་མཐོ་རིམ་སྒྲིག་འགོད་བཟོ་བཅོས།："

msgid "Changing interface configuration"
msgstr "མཐུད་ཁའི་སྡེབ་སྒྲིག་བཟོ་བཅོས།"

msgid "Changing network location"
msgstr "དྲ་རྒྱའི་གནས་ས་བཟོ་བཅོས།"

msgid "Changing user account type for:"
msgstr "ལ་སྤྱོད་མཁན་རྩིས་ཐོའི་རིགས་གྲས་བཟོ་བཅོས།:"

msgid "Changing user name for:"
msgstr "གཤམ་གྱི་སྤྱོད་མཁན་གྱི་སྤྱོད་མཁན་མིང་བཟོ་བཅོས།:"

msgid "Changing user password for:"
msgstr "གཤམ་གྱི་སྤྱོད་མཁན་གྱི་སྤྱོད་མཁན་གསང་བརྡ་བཟོ་བཅོས།:"

msgid ""
"Check that it is correctly typed and that it is correctly supported by your "
"system."
msgstr ""
"ནང་འཇུག་ཡང་དག་མིན་ཞིབ་བཤེར་བྱེད་པ་མ་ཟད། "
"ཁྱེད་ཀྱི་རྒྱུད་ཁོངས་ཀྱིས་སྒྲིག་ཆས་འདིར་རྒྱབ་སྐྱོར་བྱེད་ཐུབ་མིན་ཞིབ་བཤེར་བྱེད་དགོས།"

msgid ""
"Check that the device is not busy and that is correctly attached to the "
"computer."
msgstr ""
"སྒྲིག་ཆས་བྲེལ་བ་ཚ་པོ་མེད་པ་མ་ཟད་རྩིས་འཁོར་ལ་འབྲེལ་མཐུད་ཡང་དག་མིན་གཏན་འཁེལ་བྱེད་དགོས།"

msgid "Check that this character is not used."
msgstr "ཡིག་རྟགས་འདི་སྤྱད་མེད་པ་ཞིབ་བཤེར་བྱེད་དགོས།"

