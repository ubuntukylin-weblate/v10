msgid "A graphical tool to analyze disk usage."
msgstr "སྡུད་སྡེར་བཀོལ་སྤྱོད་གནས་ཚུལ་དབྱེ་ཞིབ་བྱ་བྱེད་ཀྱི་རིས་དབྱིབས་ལག་ཆ་ཞིག"

msgid "A list of URIs for partitions to be excluded from scanning."
msgstr "ཕྱིར་ཕུད་པའི་བཤར་འབེབས་ཁུལ་དབྱེURI རེའུ་མིག"

msgid ""
"A simple application which can scan either specific folders (local or "
"remote) or volumes and give a graphical representation including each "
"directory size or percentage."
msgstr ""

msgid "Active Chart"
msgstr "བྱེད་སྒོའི་རི་མོ།"

msgid "Apparent sizes are shown instead."
msgstr "རིགས་འདེད་ཀྱི་ཆེ་ཆུང་མངོན་པར་འགྱུར།"

msgid "Baobab"
msgstr "Baobab"

msgid "Check folder sizes and available disk space"
msgstr "ཡིག་ཁུག་གི་ཆེ་ཆུང་དང་སྤྱོད་རུང་སྡུད་སྡེར་བར་སྟོང་ལ་ཞིབ་བཤེར་བྱེད།"

msgid "Close"
msgstr "ཁ་རྒྱག་པ།"

msgid "Computer"
msgstr "རྩིས་འཁོར།"

msgid "Contents"
msgstr "ནང་དོན།"

