"\n"
msgstr ""

msgid "%d (default)"
msgstr "%d། (སོར་བཞག)"

msgid "%d (disabled)"
msgstr "%d། (འགོག་སྤྱོད)"

msgid "%d (enabled, prefer public IP)"
msgstr "%d། (སྤྱོད་འགོ་བརྩམས་ཟིན། སྤྱིའི IP སྔོན་འགྲོ)"

msgid "%d (enabled, prefer temporary IP)"
msgstr "%d། (སྤྱོད་འགོ་བརྩམས་ཟིན། སྐབས་ཐོག IP སྔོན་འགྲོ)"

msgid "%d (key)"
msgstr "%d། (key)"

msgid "%d (no)"
msgstr "%d། (མིན)"

msgid "%d (passphrase)"
msgstr "%d། (གསང་ཨང་།)"

msgid "%d (unknown)"
msgstr "%d། (ངེས་མེད།)"

msgid "%d (yes)"
msgstr "%d། (ཡིན)"

msgid "%d. DNS server address is invalid"
msgstr "%d。DNS ཞབས་ཞུ་ཆས་ཀྱི་ཤག་གནས་ནུས་མེད་རེད།"

msgid "%d. IP address has 'label' property with invalid type"
msgstr "%d。IP ཤག་གནས་ནུས་མེད་རིགས་རྣམ་གྱི “lable”(གདོང་འཛར)གཏོགས་གཤིས།"

msgid "%d. IP address has invalid label '%s'"
msgstr "%d。IP ཤག་གནས་ལ་ནུས་མེད་ཀྱི་གདོང་འཛར་ཡོད། “%s”"

msgid "%d. IP address is invalid"
msgstr "%d。IP ཤག་གནས་ནུས་མེད།"

msgid "%d. IPv4 address has invalid prefix"
msgstr "%d。IPv4 ཤག་གནས་སྔོན་འཇུག་ནུས་མེད།"

msgid "%d. IPv4 address is invalid"
msgstr "%d。IPv4 ཤག་གནས་ནུས་མེད།"

msgid "%d. IPv6 address has invalid prefix"
msgstr "\"%d。IPv6 ཤག་གནས་ལ་ས་མེདཀྱི་སྔོན་འཇུག་ཡོད།"

msgid "%d. IPv6 address is invalid"
msgstr "%d。IPv6 ཤག་གནས་ནུས་མེད།"

msgid "%d. route cannot be a default route"
msgstr "%d。སོར་བཞག་ལམ་འཚོལ་ཡིན་མི་ཆོག"

msgid "%d. route has invalid prefix"
msgstr "%d。ལམ་འཚོལ་སྔོན་འཇུག་ནུས་མེད།"

msgid "%d. route is invalid"
msgstr "%d。ལམ་འཚོལ་ནུས་མེད།"

msgid "%s Network"
msgstr "%s དྲ་རྒྱ།"

msgid "%s is already running (pid %ld)\n"
msgstr ""

msgid "%s is incompatible with static WEP keys"
msgstr "%s འགུལ་མེད WEP གསང་ལྡེ་དང་འཆམ་མཐུན་མིན།"

msgid "%s.  Please use --help to see a list of valid options.\n"
msgstr ""

msgid "%u MHz"
msgstr "%u MHz"

msgid "%u Mb/s"
msgstr "%u Mb/s"

msgid "%u Mbit/s"
msgstr "%u Mbit/s"

msgid "'%d' is not a valid channel"
msgstr "%d' ནུས་མེད་ཀྱི་རྒྱུ་ལམ་ཡིན།"

msgid "'%d' is not a valid value for the property (should be <= %d)"
msgstr "%d' ནུས་མེད་ཀྱི་གཏོགས་གཤིས་གྲངས་ཐང་ཡིན།(སྤྱིར་ཡིན་ན <= %d)"

msgid "'%d' is not valid; use <%d-%d>"
msgstr "%d' ནུས་མེད། སྤྱོད་པ། <%d-%d>"

msgid "'%d' is out of valid range <128-16384>"
msgstr "%d' ཁྱབ་ཁོངས་ལས་བརྒལ་བ། <128-16384>"

msgid "'%d' value is out of range <0-3>"
msgstr "%d' ཁྱབ་ཁོངས་ལས་བརྒལ་བ། <0-3>"

msgid "'%ld' is not a valid channel"
msgstr "“%ld” ནུས་མེད་ཀྱི་རྒྱུ་ལམ་ཡིན།"

msgid "'%lld' is not valid; use <%lld-%lld>"
msgstr "“%lld” ནུས་མེད། སྤྱོད་པ།  <%lld-%lld>"

msgid "'%s' can only be used with '%s=%s' (WEP)"
msgstr "%s'  '%s=%s' ཁོ་ནར་མཉམ་སྡེབ་བྱས་ནས་སྤྱོད་ཆོག(WEP)"

msgid "'%s' connection requires '%s' or '%s' setting"
msgstr "“%s” འབྲེལ་མཐུད་བྱེད་པར “%s” དང་ཡང་ན “%s” སྒྲིག་བཀོད་དགོས།"

msgid "'%s' connections require '%s' in this property"
msgstr "“%s” འབྲེལ་མཐུད་བྱེད་པར་གཏོགས་གཤིས་འདིའི་ནང་ “%s”ཡོད་དགོས།"

msgid "'%s' contains invalid char(s) (use [A-Za-z._-])"
msgstr "%s' ནུས་མེད་ཡིག་རྟགས་འདུས། (སྤྱོད་པ། [A-Za-z._-])"

msgid "'%s' has to be alone"
msgstr "“%s” ངེས་པར་དུ་ཁེར་རྐྱང་དུ་གནས་དགོས།"

msgid "'%s' is ambiguous (%s x %s)"
msgstr "%s' དོན་མང་ཡོད།(%s x %s)"

msgid "'%s' is neither an UUID nor an interface name"
msgstr "%s'  UUID མིན་ལ་མཐུད་ཁའི་མིང་ཡང་མིན།"

msgid "'%s' is not a DCB app priority"
msgstr "%s'  DCB བཀོལ་སྤྱོད་སྔོན་ཐོབ་རིམ་པ་མིན།"

msgid "'%s' is not a VPN connection profile"
msgstr "%s'  VPN འབྲེལ་མཐུད་སྡེབ་སྒྲིག་ཡིག་ཆ་མིན།"

msgid "'%s' is not a name of any exiting profile"
msgstr "“%s”སྡེབ་སྒྲིག་ཡིག་ཆའི་མིང་མངོན་པ་གང་ཡང་མིན།"

msgid "'%s' is not a number"
msgstr "%s' གྲངས་ཀ་ཞིག་མིན།"

msgid "'%s' is not a valid DCB flag"
msgstr "%s' ནུས་མེད་ཀྱི DCB མཚོན་རྟགས་ཡིན།"

msgid "'%s' is not a valid Ethernet MAC"
msgstr "%s' ནུས་མེད་ཀྱི་ཨེ་ཐེར་དྲ་རྒྱ MAC ཤག་གནས་ཡིན།"

msgid "'%s' is not a valid Ethernet port value"
msgstr "%s' ནུས་མེད་ཀྱི་ཨེ་ཐེར་དྲ་རྒྱ་སྣེ་ཁ་གྲངས་ཐང་ཡིན།"

msgid "'%s' is not a valid IBoIP P_Key"
msgstr "%s' ནུས་མེད་ཀྱི IBoIP P_Key ཡིན།"

msgid "'%s' is not a valid IPv4 address for '%s' option"
msgstr "%2$s' འདེམས་ཚན་ལ་མཚོན་ན། '%1$s' ནི་ནུས་མེད་ཀྱི IPv4 ཤག་གནས་ཡིན།"

msgid "'%s' is not a valid InfiniBand MAC"
msgstr "%s' ནུས་མེད་ཀྱི InfiniBand MAC ཤག་གནས་ཡིན།"

msgid "'%s' is not a valid MAC"
msgstr "%s' ནུས་མེད་ཀྱི MAC ཤག་གནས་ཡིན།"

msgid "'%s' is not a valid MAC address"
msgstr "%s' ནུས་མེད་ཀྱི MAC ཤག་གནས་ཡིན།"

msgid "'%s' is not a valid PSK"
msgstr "%s' ནུས་མེད་ཀྱི PSKཡིན།"

msgid "'%s' is not a valid UUID"
msgstr "%s' ནུས་མེད་ཀྱི UUIDཡིན།"

msgid "'%s' is not a valid Wi-Fi mode"
msgstr "%s' ནུས་མེད་ཀྱི Wi-Fi རྣམ་པ་ཡིན།"

msgid "'%s' is not a valid band"
msgstr "%s' ནུས་མེད་ཀྱི་ཟློས་དུམ་ཡིན།"

msgid "'%s' is not a valid channel"
msgstr "%s' ནུས་མེད་ཀྱི་རྒྱུ་ལམ་ཡིན།"

msgid "'%s' is not a valid channel; use <1-13>"
msgstr "%s' ནུས་མེད་ཀྱི་རྒྱུ་ལམཡིན།  <1-13> སྤྱོད་པ།"

msgid "'%s' is not a valid duplex value"
msgstr "%s' ནུས་མེད་ཀྱི་བསྐྱར་སྤྱོད་གྲངས་ཐང་ཡིན།"

msgid "'%s' is not a valid flag number; use <0-%d>"
msgstr "%s' ནུས་མེད་ཀྱི་མཚོན་རྟགས་ཨང་ཡིན།  <0-%d> སྤྱོད་པ།"

msgid "'%s' is not a valid hex character"
msgstr "%s' ནུས་མེད་ཀྱི་བཅུ་དྲུག་གོང་འགྲིལ་ལུགས་ཡིག་རྟགས་ཡིན།"

msgid "'%s' is not a valid interface name"
msgstr "%s' ནུས་མེད་ཀྱི་མཐུད་ཁའི་མིང་ཡིན།"

msgid "'%s' is not a valid interface name for '%s' option"
msgstr "%2$s' འདེམས་ཚན་ལ་མཚོན་ན། '%1$s' ནི་ནུས་མེད་ཀྱི་མཐུད་ཁའི་མིང་ཡིན།"

msgid "'%s' is not a valid number (or out of range)"
msgstr "%s' ནུས་མེད་ཀྱི་གྲངས་ཐང་ཡིན། (ཡང་ན་ཁྱབ་ཀོངས་ལས་བརྒལ།)"

msgid "'%s' is not a valid team configuration or file name."
msgstr "%s' ནུས་མེད་ཁྱུ་ཚོ་སྒྲིག་འགོད་དང་ཡང་ན་ཡིག་ཆའི་མིང་ཡིན།"

msgid "'%s' is not a valid value for '%s'"
msgstr "%2$s' ལ་མཚོན་ན '%1$s' ནི་ནུས་མེད་གྲངས་ཐང་ཡིན།"

msgid "'%s' is not a valid value for the property"
msgstr "%s' གཏོགས་གཤིས་འདིར་མཚོན་ན་ནུས་ལྡན་གྲངས་ཐང་མིན།"

msgid "'%s' is not a valid value; use -1, 0 or 1"
msgstr "“%s” ནུས་ལྡན་གྲངས་ཐང་མིན།  -1、0 ཡང་ན 1 སྤྱོད་པ།"

msgid "'%s' is not valid"
msgstr "“%s” ནུས་མེད།"

msgid "'%s' is not valid (the format is: ip[/prefix] [next-hop] [metric])"
msgstr "“%s” ནུས་མེད། (རྣམ་གཞག་ནི：ip[/prefix] [next-hop] [metric])"

msgid "'%s' is not valid master; use ifname or connection UUID"
msgstr ""
"%s' ནུས་མེད་ཀྱི་འབྲེལ་མཐུད་གཙོ་བོ་ཡིན།   ifname ཡང་ན་འབྲེལ་མཐུད་ཀྱི UUID "
"སྤྱོད་པ།"

msgid "'%s' is not valid; 2 or 3 strings should be provided"
msgstr "“%s” ནུས་མེད།   ཡིག་རྟགས་ཕྲེང་བ 2 དང་ཡང་ན 3 མཁོ་འདོན་བྱེད་དགོས།"

msgid "'%s' is not valid; use 0, 1, or 2"
msgstr "“%s” ནུས་མེད།  0, 1, ཡང་ན 2སྤྱོད་པ།"

msgid "'%s' is not valid; use <option>=<value>"
msgstr "“%s” ནུས་མེད།   <option>=<value> སྤྱོད་པ།"

msgid "'%s' is not valid; use [%s] or [%s]"
msgstr "“%s” ནུས་མེད།  [%s]ཡང་ན[%s]སྤྱོད་པ།"

msgid "'%s' is not valid; use [e, o, n]"
msgstr "“%s” ནུས་མེད།  [e, o, n]སྤྱོད་པ།"

msgid "'%s' length is invalid (should be 5 or 6 digits)"
msgstr "“%s” རིང་ཚད་ནུས་མེད། (གནས་གྲངས 5 ཡང་ན 6 ཡིན་དགོས།)"

msgid "'%s' not a number between 0 and %u (inclusive)"
msgstr "“%s”  0 དང་ %u། (འདུས་པ) བར་གྱི་གྲངས་ཀ་ཞིག་མིན།"

msgid "'%s' not a number between 0 and %u (inclusive) or %u"
msgstr "“%s”  0 དང་ %u། (འདུས་པ) བར་གྱི་གྲངས་ཀ་ཞིག་དང་ཡང་ན %uམིན།"

msgid "'%s' not among [%s]"
msgstr "“%s”  [%s] ནང་དུ་མེད།"

msgid "'%s' not among [0 (unknown), 1 (key), 2 (passphrase)]"
msgstr "“%s”  [0(ངེས་མེད) 1(གསང་ལྡེ) 2(གསང་ཨང་)] ནང་དུ་མེད།"

msgid ""
"'%s' not compatible with %s '%s', please change the key or set the right %s "
"first."
msgstr ""
"“%s”  %s “%s” དང་འཆམ་མཐུན་མིན།  གསང་ལྡེ་བཟོ་བཅོས་དང་ཡང་ན་ཡང་དག་པའི %s "
"དབང་ཚད་སྒྲིག་བཀོད་བྱེད་རོགས།"

msgid "'%s' option is empty"
msgstr "“%s” འདེམས་ཚན་སྟོང་པ་ཡིན།"

msgid "'%s' option is only valid for '%s=%s'"
msgstr "“%s” འདེམས་ཚན་གྱིས “%s=%s” ཁོ་ནར་ནུས་པ་ཡོད།"

msgid "'%s' option is only valid with mode '%s'"
msgstr "“%s” འདེམས་ཚན་རྣམ་པ “%s” ཁོ་ནར་ནུས་པ་ཡོད།"

msgid "'%s' option requires '%s' option to be set"
