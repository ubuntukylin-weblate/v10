msgid "(seconds)"
msgstr "（སྐར་ཆ།）"

msgid "A correct role is required"
msgstr "ཡང་དག་པའི་འཁྲབ་བྱའི་མི་སྣ་དམིགས་འཛུགས་བྱེད་དགོས།"

msgid "A destination directory and the package names to download are required"
msgstr ""
"དམིགས་བྱའི་དཀར་ཆག་དང་ཕབ་ལེན་བྱ་དགོས་པའི་མཉེན་ཆས་ཀྱི་མིང་དམིགས་འཛུགས་བྱེད་དགོས།"

msgid "A distribution name is required"
msgstr "འགྲེམས་སྤེལ་པར་གཞིའི་མིང་དམིགས་འཛུགས་བྱ་དགོས།"

msgid "A filename to install is required"
msgstr "སྒྲིག་འཇུག་བྱེད་དགོས་པའི་ཡིག་ཆའི་མིང་དམིགས་འཛུགས་བྱ་དགོས།"

msgid "A licence identifier (eula-id) is required"
msgstr "ཆོག་འཐུས་ངོ་རྟགས་（eula-id）དམིགས་འཛུགས་བྱ་དགོས།"

msgid "A output directory or file name is required"
msgstr "ཕྱིར་འདོན་དཀར་ཆག་གམ་ཡིག་ཆའི་མིང་དམིགས་འཛུགས་བྱེད་དགོས།"

msgid "A pack with the same name already exists, do you want to overwrite it?"
msgstr "མིང་གཅིག་པའི་ཞབས་ཞུ་ཁུག་གནས་ཟིན་པས། ཁྱེད་ཀྱིས་དེ་ལ་བཀབ་རྒྱུ་ཡིན་ནམ།"

msgid "A package name is required"
msgstr "མཉེན་ཆས་ཁུག་མའི་མིང་དམིགས་འཛུགས་བྱེད་དགོས།"

msgid "A package name to install is required"
msgstr "སྒྲིག་འཇུག་བྱེད་དགོས་པའི་མཉེན་ཆས་ཁུག་མའི་མིང་དམིགས་འཛུགས་བྱེད་དགོས།"

msgid "A package name to remove is required"
msgstr "སུབ་དགོས་པའི་མཉེན་ཆས་ཁུག་མའི་མིང་དམིགས་འཛུགས་བྱེད་དགོས།"

msgid "A package name to resolve is required"
msgstr "དབྱེ་ཞིབ་བྱེད་དགོས་པའི་མཉེན་ཆས་ཁུག་མའི་མིང་དམིགས་འཛུགས་བྱེད་དགོས།"

msgid "A package provide string is required"
msgstr "མཉེན་ཆས་ཁུག་མ་ལས་ཞིབ་བརྗོད་འདོན་སྤྲོད་དམིགས་འཛུགས་བྱེད་དགོས།"

msgid "A repo name, parameter and value are required"
msgstr "མཉེན་ཆས་མཛོད་ཁང་མིང་། ཞུགས་གྲངས་དང་གྲངས་ཐང་དམིགས་འཛུགས་བྱེད་དགོས།"

msgid "A repository name is required"
msgstr "མཉེན་ཆས་མཛོད་ཁང་མིང་དམིགས་འཛུགས་བྱེད་དགོས།"

msgid "A search term is required"
msgstr "བཤེར་འཚོལ་ཚིག་དམིགས་འཛུགས་བྱེད་དགོས།"

msgid "A search type is required, e.g. name"
msgstr "བཤེར་འཚོལ་རིགས་རྣམ་དམིགས་འཛུགས་བྱེད་དགོས། དཔེར་ན་མིང་ལྟ་བུ།"

msgid "A type, key_id and package_id are required"
msgstr ""
"རིགས་རྣམ། "
"གསང་ལྡེ་ངོ་རྟགས་（key_id）དང་མཉེན་ཆས་ཁུག་མའི་ངོ་རྟགས་（package_id）དམིགས་འཛུགས་བྱེད་དགོས།"

msgid "Accept EULA"
msgstr "མཐའ་མའི་སྤྱོད་མཁན་གྱི་ཆོག་འཐུས་གྲོས་མཐུན（EULA）དང་ལེན་བྱེད་པ།"

msgid "Accepting EULA"
msgstr "མཐའ་མའི་སྤྱོད་མཁན་གྱི་ཆོག་འཐུས་གྲོས་མཐུན（EULA）དང་ལེན་བྱེད་པ།"

msgid "Affected packages:"
msgstr "ཤུགས་རྐྱེན་ཐེབས་པའི་མཉེན་ཆས་ཁུག་མ།："

msgid "Affected packages: None"
msgstr "ཤུགས་རྐྱེན་ཐེབས་པའི་མཉེན་ཆས་ཁུག་མ།：མེད།"

msgid "Agreement"
msgstr "གྲོས་ཆིངས།"

msgid "An action, e.g. 'update-packages' is required"
msgstr ""
"བྱ་འགུལ་དམིགས་འཛུགས་བྱེད་དགོས། དཔེར་ན “update-"
"packages（མཉེན་ཆས་ཁུག་མ་གསར་སྒྱུར།）”"

msgid "An upgrade type is required, e.g. 'minimal', 'default' or 'complete'"
msgstr ""

msgid "Application restart required by:"
msgstr "ཉེར་སྤྱོད་རིམ་བྱ་རིམ་གསར་དུ་འགོ་སློང་བའི་རེ་བ་ཞུ་་པའི་མཉེན་ཆས་ཁུག་མ།:"

msgid "Attempting to rebind device"
msgstr "སྒྲིག་ཆས་བསྐྱར་དུ་བསྡམས་བཅད་བྱེད་སྟ་གོན་བྱེད་བཞིན་པ།"

msgid "Authentication is required to accept a EULA"
msgstr ""
"མཐའ་མའི་སྤྱོད་མཁན་ཆོག་འཐུས་གྲོས་ཆིངས་བཏང་ལེན་བྱེད་ན་（EULA）ར་སྤྲོད་བྱེད་དགོས།"

msgid ""
"Authentication is required to cancel a task that was not started by yourself"
msgstr ""
"ཁྱེད་ཀྱིས་འགོ་སློང་བྱས་པ་མ་ཡིན་པའི་ལས་འགན་ཞིག་དོར་བར་ར་སྤྲོད་བྱེད་དགོས།"

msgid "Authentication is required to change software source parameters"
msgstr "མཉེན་ཆས་ཁུངས་ཀྱི་ཞུགས་གྲངས་བཟོ་བཅོས་བྱེད་པར་ར་སྤྲོད་བྱེད་དགོས།"

msgid "Authentication is required to clear the offline updates message"
msgstr "སྐུད་བྲལ་བརྡ་ཐོ་གསར་སྒྱུར་གཙང་སེལ་བྱེད་པར་ར་སྤྲོད་བྱེད་དགོས།"

msgid ""
"Authentication is required to consider a key used for signing packages as "
"trusted"
msgstr ""
"མཉེན་ཆས་ཁུག་མ་མིང་རྟགས་བཀོད་པར་སྤྱད་པའི་གསང་ལྡེ་ནི་ཡིད་ཆེས་བྱས་ཆོག་པའི་ར་སྤྲོད་བྱེད་དགོས།"

msgid "Authentication is required to install a package"
msgstr "མཉེན་ཆས་ཐུམ་སྒྲིག་འཇུག་པར་ར་སྤྲོད་བྱེད་དགོས།"

msgid "Authentication is required to install an untrusted package"
msgstr "ཡིད་མ་ཆེས་བའི་མཉེན་ཆས་ཐུམ་སྒྲིག་འཇུག་བྱེད་པ་ར་སྤྲོད་བྱེད་དགོས།"

msgid "Authentication is required to refresh the system sources"
msgstr "རྒྱུད་ཁོངས་ཁུམས་གསར་འདོན་བྱེད་པར་ར་སྤྲོད་བྱེད་དགོས།"

msgid "Authentication is required to reload the device with a new driver"
msgstr "སྐུལ་སློང་གསར་པས་སྒྲིག་ཆས་གསར་དུ་ནང་འཇུག་བྱེད་པར་ར་སྤྲོད་བྱེད་དགོས།"

msgid "Authentication is required to remove packages"
msgstr "མཉེན་ཆས་ཐུམ་སུབ་པར་ར་སྤྲོདབྱེད་དགོས།"

msgid "Authentication is required to repair the installed software"
msgstr "སྒྲིག་འཇུག་བྱས་ཟིན་པའི་མཉེན་ཆས་ཉམས་གསོ་བྱེད་པར་ར་སྤྲོད་བྱེད་དགོས།"

msgid ""
"Authentication is required to set the network proxy used for downloading "
"packages"
msgstr ""
"མཉེན་ཆས་ཐུམ་ཕབ་ལེན་ལ་སྤྱོད་པའི་དྲ་རྒྱའི་ལས་ཚབ་སྒྲིག་འགོད་བྱེད་པར་ར་སྤྲོད་བྱེད་དགོས།"

msgid "Authentication is required to trigger offline updates"
msgstr "སྐུད་བྲལ་གསར་སྒྱུར་འཕྲད་འདོན་བྱེད་པར་ར་སྤྲོད་བྱེད་དགོས།"

msgid "Authentication is required to update packages"
msgstr "མཉེན་ཆས་ཐུམ་གསར་སྒྱུར་བྱེད་པར་ར་སྤྲོད་བྱེད་དགོས།"

msgid "Authentication is required to upgrade the operating system"
msgstr "བཀོལ་སྤྱོད་རྒྱུད་ཁོངས་རིམ་སྤོར་བྱེད་པར་ར་སྤྲོད་བྱེད་དགོས།"

msgid "Available"
msgstr "སྤྱོད་རུང་།"

msgid "Blocked"
msgstr "སྒྲིབ་ཡོལ།"

msgid "Both options selected."
msgstr "རྣམ་གྲངས་གཉིས་ཀ་འདེམ་པ།"

msgid "Bug fix "
msgstr "ཆད་སྐྱོན་སླར་གསོ།"

msgid "Bugzilla"
msgstr "Bugzilla（ཆད་སྐྱོན་མཇུག་སྒྲིལ་མཁན།）"

msgid "CVE"
msgstr "སྤྱི་པའི་སྐྱོན་ཆ་དང་ཕྱིར་མངོན།（CVE）"

msgid "Cancel foreign task"
msgstr "སྤྱོད་མཁན་གཞན་གྱི་ལས་འགན་དོར་བ།"

msgid "Canceling"
msgstr "ལེན་པ།"

msgid "Cancelling"
msgstr "ལེན་བཞིན་པ།"

msgid "Category"
msgstr "རིགས།"

msgid "Change software source parameters"
msgstr "མཉེན་ཆས་ཁུངས་ཀྱི་ཞུགས་གྲངས་བཟོ་བཅོས།"

msgid "Changes"
msgstr "བཟོ་བཅོས།"

msgid "Checking applications in use"
msgstr "སྤྱོད་བཞིན་པའི་བཀོལ་སྤྱོད་བྱ་རིམ་ཞིབ་བཤེར་བྱེད་བཞིན་པ།"

msgid "Checking libraries in use"
msgstr "སྤྱོད་བཞིན་པའི་མཛོད་ཞིབ་བཤེར་བྱེད་བཞིན་པ།"

msgid "Checking signatures"
msgstr "མིང་རྟགས་ཞིབ་བཤེར་་བྱེད་བཞིན་པ།"

msgid "Cleaned up"
msgstr "གཙང་བཤེར་བྱས་ཟིན་པ།"

msgid "Cleaning up"
msgstr "གཙང་བཤེར་བྱེད་བཞིན་པ།"

msgid "Cleaning up packages"
msgstr "མཉེན་ཆས་ཐུམ་གཙང་བཤེར་བྱེད་བཞིན་ཡོད།"

msgid "Clear offline update message"
msgstr "སྐུད་བྲལ་བརྡ་ཐོ་གསར་སྒྱུར་གཙང་བཤེར།"

msgid "Command failed"
msgstr "བཀའ་བརྡ་ཕམ་པ།"

msgid "Command line"
msgstr "བཀའ་བརྡའི་ཕྲེང་།"

msgid "Committing changes"
msgstr "བཟོ་བཅོས་ཟིན་ཐོར་འགོད་བཞིན་པ།"

msgid "Copying files"
msgstr "ཡིག་ཆ་འདྲ་བཟོ་བྱེད་པ།"

msgid "Could not disable the debugging sources: %s"
msgstr "ཚོད་སྒྲིག་ཁུངས་འགོག་སྤྱོད་བྱེད་ཐུབ་མེད།：%s"

msgid "Could not find dependent packages: %s"
msgstr "བརྟེན་བྱའི་མཉེན་ཆས་ཐུམ་རྙེད་མ་བྱུང་།：%s"

msgid "Could not install packages: %s"
