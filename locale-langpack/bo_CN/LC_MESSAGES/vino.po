msgid " or "
msgstr " ཡང་ན།"

msgid "%s's remote desktop on %s"
msgstr "%s ནི་ %s ཡི་རྒྱང་རིང་ཅོག་ངོས་སུ་ཡོད།"

msgid "'%s' disconnected"
msgstr "“%s”འབྲེལ་མཐུད་མཚམས་བཅད་ཟིན།"

msgid "'%s' is remotely controlling your desktop."
msgstr "“%s”རྒྱང་ནས་ཁྱེད་ཀྱི་ཅོག་ངོས་ཚོད་འཛིན་བྱེད་བཞིན་པ།"

msgid "'%s' rejected the desktop sharing invitation."
msgstr "“%s”ཅོག་ངོས་མཉམ་སྤྱོད་གདན་ཞུ་དང་ལེན་མ་བྱས་པ།"

msgid "- Updates Vino password"
msgstr "- Vino གསང་ཨང་གསར་བསྒྱུར།"

msgid "- VNC Server for GNOME"
msgstr "- GNOME ཡི་ VNC ཞབས་ཞུ་ཆས།"

msgid "A user on the computer '%s' is remotely controlling your desktop."
msgstr ""
"རྩིས་འཁོར་“%s”སྟེང་གནས་པའི་སྤྱོད་མཁན་གྱིས་ཁྱེད་རང་གི་ཅོག་ངོས་ལ་རྒྱང་ནས་ཚོད་འཛིན་བྱེད་བཞིན་པ།"

msgid "A user on the computer '%s' is remotely viewing your desktop."
msgstr ""
"རྩིས་འཁོར་“%s”སྟེང་གནས་པའི་སྤྱོད་མཁན་གྱིས་ཁྱེད་རང་གི་ཅོག་ངོས་ལ་རྒྱང་ནས་ལྟ་སྤྱོད་བྱེད་བཞིན་པ།"

msgid ""
"A user on the computer '%s' is trying to remotely view or control your "
"desktop."
msgstr ""
"རྩིས་འཁོར་“%s”སྟེང་གི་སྤྱོད་མཁན་ཞིག་གིས་ཁྱེད་ཀྱི་ཅོག་ངོས་ལ་རྒྱང་ནས་ལྟ་སྤྱོད་དམ་ཚོད་འཛིན་བྱེད་པར་ཚོད་ལྟ་བྱེད་བཞིན་པ།"

msgid "Al_ways"
msgstr "རྟག་པར་(_W)"

msgid "All remote users will be disconnected. Are you sure?"
msgstr ""
"རྒྱང་རིང་གི་སྤྱོད་མཁན་ཡོངས་ཀྱི་བྲེལ་མཐུད་མཚམས་གཅོད་པར་འགྱུར། "
"ཁྱེད་ཀྱིས་གཏན་ཁེལ་ལམ།"

msgid "Allow other users to _view your desktop"
msgstr "མི་གཞན་གྱིས་ཁྱེད་ཀྱི་ཅོག་ངོས་ལ་ལྟ་ཆོག་པ།(_V)"

msgid "Allowed authentication methods"
msgstr "རུང་བའི་ར་སྤྲོད་ཐབས་ལམ།"

msgid "Alternative port number"
msgstr "སྣེ་ཁའི་ཨང་ཚབ་བྱེད།"

msgid "An error has occurred:"
msgstr "ནོར་འཁྲུལ་ཞིག་བྱུང་བ།："

msgid "Another user is controlling your desktop"
msgstr "གཞན་གྱིས་ཁྱེད་ཀྱི་ཅོག་ངོས་ལ་ཚོད་འཛིན་བྱེད་བཞིན་པ།"

msgid "Another user is viewing your desktop"
msgstr "མི་གཞན་དག་གིས་ཁྱེད་ཀྱི་ཅོག་ངོས་ལ་ལྲ་བཞིན་འདུག"

msgid "Application does not accept documents on command line"
msgstr "ཉེར་སྤྱོད་བྱ་རིམ་དེ་བཀའ་བརྡ་འཕྲེད་དུ་ཡིག་ཚགས་མི་ལེན།"

msgid "Are you sure you want to disconnect '%s'?"
msgstr "ཁྱེད་ཀྱིས་“%s”ཡི་འབྲེལ་མཐུད་མཚམས་གཅོད་པ་གཏན་འཁེལ་ལམ།"

msgid "Are you sure you want to disconnect all clients?"
msgstr "ཁྱེད་ཀྱིས་སྤྱོད་མཁན་ཡོངས་ཀྱི་འབྲེལ་མཐུད་མཚམས་གཅོད་པ་གཏན་འཁེལ་ལམ།"

msgid "Automatically _configure UPnP router to open and forward ports"
msgstr ""
" UPnP ལམ་འཚོལ་ཆས་སྒོ་འབྱེད་དང་བརྒྱུད་བསྐུར་སྣེ་ཁ་རང་འགུལ་སྡེབ་སྒྲིག(_C)"

msgid "Can't pass document URIs to a 'Type=Link' desktop entry"
msgstr "ཡིག་ཚགས་ཀྱི་URL དེ“Type=Link”ཡི་ཅོག་ངོས་ཚན་ལ་བརྒྱུད་མི་ཐུབ།"

msgid "Cancelled"
msgstr "ལེན་པ།"

msgid "Changing Vino password.\n"
msgstr " Vino གསང་ཨང་བཟོ་བཅོས་བྱེད་བཞིན་པ།\n"

msgid "Checking the connectivity of this machine..."
msgstr "རྩིས་འཁོར་འདིའི་དྲ་སྦྲེལ་རྣམ་པ་་ཞིབ་བཤེར་བྱེད་བཞིན་པ།..."

msgid "Choose how other users can remotely view your desktop"
msgstr ""

msgid "Desktop Sharing"
msgstr "ཅོག་ངོས་མཉམ་སྤྱོད།"

msgid "Desktop Sharing Preferences"
msgstr "ཅོག་ངོས་མཉམ་སྤྱོད་སྔོན་འདེམས་ཚན་པ།"

msgid "Desktop sharing is enabled"
msgstr "ཅོག་ངོས་མཉམ་སྤྱོད་འགོ་བསླངས་ཟིན།"

msgid "Disable connection to session manager"
msgstr "གཏམ་གླེང་དོ་དམ་ཆས་ཀྱི་འབྲེལ་མཐུད་འགོག་སྤྱོད།"

msgid "Disconnect"
msgstr "འབྲེལ་མཐུད་མཚམས་གཅོད་པ།"

msgid "Disconnect %s"
msgstr "འབྲེལ་མཐུད་མཚམས་གཅོད་པ་ %s"

msgid "Disconnect all"
msgstr "འབྲེལ་མཐུད་ཡོངས་ཀྱི་མཚམས་གཅོད་པ།"

msgid "E-mail address to which the remote desktop URL should be sent"
msgstr ""
"རྒྱང་རིང་ཅོག་ངོས་ URL ཤག་གནས་ཀྱི་གློག་རྡུལ་ཡིག་སྒམ་ཤག་གནས་སྐྱེལ་གཏོང་།"

msgid ""
"ERROR: Maximum length of password is %d character. Please, re-enter the "
"password."
msgid_plural ""
"ERROR: Maximum length of password is %d characters. Please, re-enter the "
"password."
msgstr[0] ""
"ནོར་འཁྲུལ།： གསང་ཨང་ཆེས་རིང་ན་ཡིག་རྟགས་ %d ཡིན། ཡང་བསྐྱར་གསང་ཨང་ནང་འཇུག་བྱོས།"
msgstr[1] ""
"ནོར་འཁྲུལ།： གསང་ཨང་ཆེས་རིང་ན་ཡིག་རྟགས་ %d ཡིན། ཡང་བསྐྱར་གསང་ཨང་ནང་འཇུག་བྱོས།"

msgid "ERROR: You do not have enough permissions to change Vino password.\n"
msgstr ""
"ནོར་འཁྲུལ།： ཁྱེད་ལ་ Vino གསང་ཨང་བཅོས་བསྒྱུར་བྱེད་པའི་དབང་ཚད་འདང་ངེས་འདུག\n"

msgid "Enable remote access to the desktop"
msgstr "རྒྱང་རིང་ནས་ཅོག་ངོས་ལ་ལྟ་སྤྱོད་རུང་བ།"

msgid "Enter new Vino password: "
msgstr "Vino གསང་ཨང་གསར་པ་ནང་འཇུག་བྱོས།："

msgid "Error displaying help"
msgstr "རོགས་རམ་འཆར་སྐབས་ནོར་འཁྲུལ་བྱུང་བ།"

msgid "Error displaying preferences"
msgstr "སྔོན་འདེམས་ཚན་པ་འཆར་སྐབས་ནོར་འཁྲུལ་བྱུང་བ།"

msgid "Error initializing libnotify\n"
msgstr " libnotify འགོ་སྒྲིག་ནོར་འཁྲུལ་བྱུང་བ།\n"

msgid "Error while displaying notification bubble: %s\n"
msgstr "ལྦུ་བའི་བརྡ་ཐོ་འཆར་སྐབས་ནོར་འཁྲུལ་བྱུང་བ།：%s\n"

msgid "FILE"
msgstr "ཡིག་ཆ།"

msgid "Failed to open connection to bus: %s\n"
msgstr "མ་སྐུད་དང་འབྲེལ་མཐུད་ཁ་འབྱེད་ཕམ་པ།：%s\n"

msgid "File is not a valid .desktop file"
msgstr "ཡིག་ཆ་གོ་ཆོད་མིན་པ། desktopཡིག་ཆ།"

msgid "GNOME Desktop Sharing"
msgstr "GNOME ཅོག་ངོས་མཉམ་སྤྱོད།"

msgid "GNOME Desktop Sharing Server"
msgstr "GNOME ཅོག་ངོས་མཉམ་སྤྱོད་ཞབས་ཞུ་ཆས།"

msgid "ID"
msgstr "ID"

msgid ""
"If not set, the server will listen on all network interfaces.\n"
"\n"
"Set this if you want to accept connections only from some specific network interface. For example, eth0, wifi0, lo and so on."
msgstr ""

msgid ""
"If true, allows remote access to the desktop via the RFB protocol. Users on "
"remote machines may then connect to the desktop using a VNC viewer."
msgstr ""

msgid ""
"If true, do not use the XDamage extension of X.org. This extension does not "
"work properly on some video drivers when using 3D effects. Disabling it will"
" make Vino work in these environments, with slower rendering as a side "
"effect."
msgstr ""

msgid ""
"If true, remote users accessing the desktop are not allowed access until the"
" user on the host machine approves the connection. Recommended especially "
"when access is not password protected."
msgstr ""

msgid ""
"If true, remote users accessing the desktop are only allowed to view the "
"desktop. Remote users will not be able to use the mouse or keyboard."
msgstr ""

msgid ""
"If true, remote users accessing the desktop are required to support "
"encryption. It is highly recommended that you use a client which supports "
"encryption unless the intervening network is trusted."
msgstr ""

msgid ""
"If true, request that a UPnP-capable router should forward and open the port"
" used by Vino."
msgstr ""
"གལ་ཏེ་ trueཡིན་ན། UPnP-capable ལམ་འཚོལ་ཆས་ལ་ "
"Vinoབརྒྱུད་བསྐུར་རམ་བེད་སྤྱད་ནས་སྣེ་ཁ་ཁ་ཕྱེ་བའི་རེ་བ་འདོན།"

msgid "If true, show a notification when a user connects to the system."
msgstr ""
"གལ་ཏེ་ trueཡིན་ན། "
"སྤྱོད་མཁན་གྱིས་རྒྱུད་ཁོངས་དང་འབྲེལ་མཐུད་བྱེད་པའི་ཚེ་བརྡ་ཐོའི་ཆ་འཕྲིན་འཆར་བ།"

msgid ""
"If true, the screen will be locked after the last remote client disconnects."
msgstr ""
"གལ་ཏེ་ trueཡིན་ན། "
"བརྙན་ཡོལ་ཆེས་མཇུག་མཐའི་རྒྱང་རིང་མཁོ་མཁན་གྱི་འབྲེལ་མཐུད་བཅད་རྗེས་དམིགས་འཛིན་བྱ་སྲིད།"

msgid ""
"If true, the server will listen on another port, instead of the default "
"(5900). The port must be specified in the 'alternative-port' key."
msgstr ""

