msgstr "ཕག་ཉན་ཚབ་བྱེད་སྣེ་ཁ།"

msgid ""
"Lists the authentication methods with which remote users may access the desktop.\n"
"\n"
"There are two possible authentication methods; \"vnc\" causes the remote user to be prompted for a password (the password is specified by the vnc-password key) before connecting and \"none\" which allows any remote user to connect."
msgstr ""

msgid "Lock the screen when last user disconnect"
msgstr ""
"སྤྱོད་མཁན་ཆེས་རྗེས་མས་འབྲེལ་མཐུད་མཚམས་གཅོད་སྐབས་བརྙན་ཡོལ་དམིགས་འཛིན་བྱེད་པ།"

msgid "Network interface for listening"
msgstr "ཕག་ཉན་ལ་སྤྱད་བྱའི་དྲ་རྒྱའི་མཐུད་ཁ།"

msgid "Nobody can access your desktop."
msgstr "ཁྱེད་ཀྱི་ཅོག་ངོས་ལ་ལྟ་སྤྱོད་བྱེད་ཐུབ་པའི་མི་མེད།"

msgid "Not a launchable item"
msgstr "འདོན་སྤྱོད་རུང་བའི་ཚན་པ་མ་རེད།"

msgid "Notify on connect"
msgstr "འབྲེལ་མཐུད་སྐབས་སྣེ་སྟོན།"

msgid "One person is connected"
msgid_plural "%d people are connected"
msgstr[0] "སྤྱོད་མཁན་%d ཡིས་འབྲེལ་མཐུད་བྱས་ཟིན།"
msgstr[1] "སྤྱོད་མཁན་%d ཡིས་འབྲེལ་མཐུད་བྱས་ཟིན།"

msgid "One person is viewing your desktop"
msgid_plural "%d people are viewing your desktop"
msgstr[0] "མི་%d ཡིས་ཁྱེད་ཀྱི་ཅོག་ངོས་ལ་ལྟ་བཞིན་པ།"
msgstr[1] "མི་%d ཡིས་ཁྱེད་ཀྱི་ཅོག་ངོས་ལ་ལྟ་བཞིན་པ།"

msgid "Only allow remote users to view the desktop"
msgstr "རྒྱང་རིང་སྤྱོད་མཁན་ཁོ་ནས་ཅོག་ངོས་ལ་ལྟ་ཆོག་པ།"

msgid "Others can access your computer using the address %s."
msgstr ""
"མི་གཞན་གྱིས་ཤག་གནས %sའདི་སྤྱད་ནས་ཁྱེད་ཀྱི་རྩིས་འཁོར་ལ་ལྟ་སྤྱོད་བྱ་རུང་བ།"

msgid "Password required for \"vnc\" authentication"
msgstr "“vnc” ར་སྤྲོད་སྐབས་མཁོ་བའི་གསང་ཨང་།"

msgid "Prompt the user before completing a connection"
msgstr "འབྲེལ་མཐུད་ལེགས་གྲུབ་སྔོན་སྤྱོད་མཁན་ལ་སྣེ་སྟོན་བྱེད་པ།"

msgid "Received signal %d, exiting."
msgstr "བརྡ་རྟགས་ %dའབྱོར་བ། ཕྱིར་འབུད་བཞིན་པ།"

msgid "Remote desktop sharing password"
msgstr "རྒྱང་རིང་ཅོག་ངོས་མཉམ་སྤྱོད་གསང་ཨང་།："

msgid "Remote users are able to control your mouse and keyboard"
msgstr ""
"རྒྱང་རིང་སྤྱོད་མཁན་གྱིས་ཁྱེད་ཀྱི་ཙིག་རྟགས་དང་མཐེབ་གཞོང་ཚོད་འཛིན་ཐུབ་པ།"

msgid "Require encryption"
msgstr "གསང་སྡོམ་བྱེད་དགོས།"

msgid "Retype new Vino password: "
msgstr " Vino གསང་ཨང་གསར་པ་ཡང་བསྐྱར་ཐེངས་གཅིག་ནང་འཇུག་བྱེད་རོགས།："

msgid ""
"Run 'vino-passwd --help' to see a full list of available command line "
"options"
msgstr ""
" “vino-passwd "
"--help”འཁོར་སྐྱོད་བྱས་ནས་སྤྱོད་རུང་བཀའ་བརྡ་ཕྲེང་གི་འདེམས་ཚན་གྱི་རེའུ་མིག་ཧྲིལ་པོར་ལྟ་བ།"

msgid ""
"Run 'vino-server --help' to see a full list of available command line "
"options"
msgstr ""
" “vino-passwd "
"--help”འཁོར་སྐྱོད་བྱས་ནས་སྤྱོད་རུང་བཀའ་བརྡ་ཕྲེང་གི་འདེམས་ཚན་གྱི་རེའུ་མིག་ཧྲིལ་པོར་ལྟ་བ།"

msgid "Screen"
msgstr "བརྙན་ཡོལ།"

msgid "Security"
msgstr "བདེ་འཇགས།"

msgid "Session management options:"
msgstr "གླེང་མོལ་དོ་དམ་འདེམ་བྱང་།"

msgid "Share my desktop information"
msgstr "བདག་གི་ཅོག་ངོས་ཆ་འཕྲིན་མཉམ་སྤྱོད།"

msgid "Share your desktop with other users"
msgstr "ཁྱེད་ཀྱི་ཅོག་ངོས་མི་གཞན་དག་དང་མཉམ་སྤྱོད་བྱེད་པ།"

msgid "Sharing"
msgstr " མཉམ་སྤྱོད།"

msgid "Show Notification Area Icon"
msgstr "བརྡ་ཐོ་ཁུལ་གྱི་རིས་རྟགས་འཆར་བ།"

msgid "Show Vino version"
msgstr " Vino པར་གཞི་འཆར་བ།"

msgid "Show session management options"
msgstr "གླེང་མོལ་དོ་དམ་འདེམས་བྱང་འཆར་བ།"

msgid "Some of these preferences are locked down"
msgstr "སྔོན་འདེམས་ཚན་པ་ཁ་ཅིག་དམིགས་འཛིན་བྱས་ཟིན།"

msgid "Sorry, passwords do not match.\n"
msgstr "དགོངས་པ་མ་ཚོམ། གསང་ཨང་ཐེངས་གཉིས་གཅིག་པ་མ་རེད།\n"

msgid "Specify file containing saved configuration"
msgstr "ཉར་ཚགས་བྱས་ཟིན་པའི་སྡེབ་སྒྲིག་ཡིག་ཆ་འདུས་པ་དམིགས་འཛུགས།"

msgid "Specify session management ID"
msgstr "གླེང་མོལ་དོ་དམ་IDདམིགས་འཛུགས།"

msgid "Start in tube mode, for the ‘Share my Desktop’ feature"
msgstr ""
"“བདག་གི་ཅོག་ངོས་མཉམ་སྤྱོད་”བྱེད་ནུས་སྤྱོད་སྐབས་Tube "
"རྣམ་པའི་ཁྲོད་འགོ་སློང་དགོས།"

msgid "Starting %s"
msgstr " %sའགོ་སློང་བཞིན་པ།"

msgid ""
"The password which the remote user will be prompted for if the \"vnc\" authentication method is used. The password specified by the key is base64 encoded.\n"
"\n"
"The special value of 'keyring' (which is not valid base64) means that the password is stored in the GNOME keyring."
msgstr ""

msgid ""
"The port which the server will listen to if the 'use-alternative-port' key "
"is set to true. Valid values are in the range of 5000 to 50000."
msgstr ""

msgid "The remote user '%s' will be disconnected. Are you sure?"
msgstr ""
"རྒྱང་རིང་སྤྱོད་མཁན་“%s”ཡི་འབྲེལ་མཐུད་གཅོད་པར་འགྱུར། ཁྱེད་ཀྱིས་གཏན་ཁེལ་ལམ།"

msgid "The remote user from '%s' will be disconnected. Are you sure?"
msgstr ""
"“%s”ནས་འོང་པའི་རྒྱང་རིང་སྤྱོད་མཁན་གྱི་འབྲེལ་མཐུད་གཅོད་པར་འགྱུར། "
"ཁྱེད་ཀྱིས་གཏན་ཁེལ་ལམ།"

msgid "The router must have the UPnP feature enabled"
msgstr "ལམ་འཚོལ་ཆས་ཀྱིས་ངེས་པར་དུ་UPnPབྱེད་ནུས་སྤྱོད་འགོ་རྩོམ་དགོས།"

msgid "The screen on which to display the prompt"
msgstr "སྣེ་སྟོན་ཆ་འཕྲིན་གྱི་བརྙན་ཡོལ་འཆར་བ།"

msgid ""
"There was an error displaying help:\n"
"%s"
msgstr ""

msgid "There was an error showing the URL \"%s\""
msgstr "དྲ་གནས་“%s” འཆེ་སྐབས་ནོར་འཁྲུལ་ཞིག་བྱུང་བ།"

msgid ""
"This key controls the behavior of the status icon. There are three options: "
"\"always\" - the icon will always be present; \"client\" - the icon will "
"only be present when someone is connected (this is the default behavior); "
"\"never\" - the icon will not be present."
msgstr ""

msgid ""
"This key specifies the e-mail address to which the remote desktop URL should"
" be sent if the user clicks on the URL in the Desktop Sharing preferences "
"dialog."
msgstr ""

msgid "Unrecognized desktop file Version '%s'"
msgstr "ངོས་འཛིན་མི་ཐུབ་པའི་ཅོག་ངོས་ཡིག་ཆ་པར་གཞི་“%s”"

msgid "Unrecognized launch option: %d"
msgstr "ངོས་འཛིན་མི་ཐུབ་པའི་མགོ་སློང་འདེམས་ཚན།：%d"

msgid "VINO Version %s\n"
msgstr "VINO པར་གཞི།%s\n"

msgid "Waiting for '%s' to connect to the screen."
msgstr "“%s”བརྙན་ཡོལ་ལ་མཐུད་པར་ངང་སྒུགས།"

msgid "When the status icon should be shown"
msgstr "རྣམ་པའི་རིས་རྟགས་ག་དུས་འཆར་བ།"

msgid ""
"When true, disable the desktop background and replace it with a single block"
" of color when a user successfully connects."
msgstr ""
"གལ་ཏེ་ trueཡིན་ན། "
"སྤྱོད་མཁན་གྱི་འབྲེལ་མཐུད་ལེགས་གྲུབ་བྱུང་ཚེ་རང་འགུལ་ངང་ཅོག་ངོས་རྒྱབ་ལྗོངས་མདོག་རྐྱང་བར་བརྗེ་སྲིད།"

msgid "Whether a UPnP router should be used to forward and open ports"
msgstr " UPnP ལམ་འཚོལ་ཆས་སྤྱད་ནས་སྣེ་ཁ་བརྒྱུད་བསྐུར་དང་སྒོ་འབྱེད་བྱེད་མིན།"

msgid "Whether to disable the desktop background when a user is connected"
msgstr "སྤྱོད་མཁན་འབྲེལ་མཐུད་བྱས་ཚེ།ཅོག་ངོས་རྒྱབ་ལྗོངས་འགོག་སྤྱོད་བྱེད་མིན།"

msgid "Whether we should disable the XDamage extension of X.org"
msgstr "ང་ཚོས་ X.org ཡི་ XDamage རྒྱ་སྐྱེད་འགོག་སྤྱོད་བྱེད་འོས་མིན།"

msgid ""
"Your XServer does not support the XTest extension - remote desktop access "
"will be view-only\n"
msgstr ""
"ཁྱེད་ཀྱི་ XServer ཡིས་XTest རྒྱ་སྐྱེད་ལ་རྒྱབ་སྐྱོར་མི་བྱེད་པས་ - "
"རྒྱང་རིང་ཅོག་ངོས་ལྟ་སྤྱོད་ཀྱིས་ལྟ་ཙམ་ལས་གཞན་ཡི་རུང་།\n"

msgid "Your desktop is only reachable over the local network."
msgstr ""
"ཁྱེད་ཀྱི་ཅོག་ངོས་ལ་རང་སའི་དྲ་རྒྱའི་ཁྲོད་ཁོ་ན་ལས་ལྟ་སྤྱོད་བྱེད་མི་རུང་།"

msgid "Your desktop will be shared"
msgstr "ཁྱེད་ཀྱི་ཅོག་ངོས་མཉམ་སྤྱོད་བྱེད་པར་འགྱུར།"

msgid "_About"
msgstr "འབྲེལ་ཡོད།(_A)"

msgid "_Allow"
msgstr "རུང་བ།(_A)"

msgid "_Allow other users to control your desktop"
msgstr "སྤྱོད་མཁན་གཞན་པས་ཁྱེད་ཀྱི་ཅོག་ངོས་ལ་ཚོད་འཛིན་རུང་བ།(_A)"

msgid "_Help"
msgstr " རོགས་རམ།(_H)"

msgid "_Never"
msgstr "གཏན་ཏུ་མིན།(_N)"

msgid "_Only when someone is connected"
msgstr "མི་གཞན་གྱིས་འབྲེལ་མཐུད་བྱས་ཚེ་ཁོ་ནར་(_O)"

msgid "_Preferences"
msgstr "སྒྲིག་འགོད།(_P)"

msgid "_Refuse"
msgstr "ཟློག་པ།(_R)"

msgid "_Require the user to enter this password:"
msgstr "རྒྱང་རིང་སྤྱོད་མཁན་ལ་གསང་ཨང་འདི་ནང་འཇུག་བྱེད་པའི་རེ་བ་འདོན་པ།(_R)："

msgid "_You must confirm each access to this machine"
msgstr "ངེས་པར་དུ་འཕྲུལ་ཆས་འདིར་ལྟ་སྤྱོད་བྱེད་ཐེངས་རེར་གཏན་འཁེལ་བྱ་དགོས།（_Y）"

