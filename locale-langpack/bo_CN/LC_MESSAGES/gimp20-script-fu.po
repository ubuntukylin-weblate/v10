msgid "3D _Outline..."
msgstr "3D སྤྱི་ཁོག (_O)..."

msgid "3_D Truchet..."
msgstr "3D Truchet(_D)..."

msgid "Active colors"
msgstr "བྱ་བྱེད་ཁ་དོག"

msgid "Add 'cow spots' to the selected region (or alpha)"
msgstr "བདམས་པའི་ཁུལ་ཁོངས། ( ཡང་ན alpha) ལ“བ་གླང་གི་ཁྲ་ཐིག” སྣོན་པ།"

msgid "Add B_evel..."
msgstr "གསེག་ངོས་སྣོན་པ། (_E)..."

msgid ""
"Add Spirographs, Epitrochoids, and Lissajous Curves to the current layer"
msgstr ""
"མིག་སྔའི་རིས་རིམ་ལ་དུང་འཁྱིལ་དབྱིབས་ཀྱི་སྐར་སྤྲིན་ཐིག་དང་། "
"ཕྱིའི་འཁྱིལ་འཁོར་ཐིག རི་ས་རུ་འཁྱོག་ཐིག་བཅས་སྣོན་པ།"

msgid "Add _Border..."
msgstr "མཐའ་སྒྲོམ་སྣོན་པ། (_B)..."

msgid "Add a 'Predator' effect to the selected region (or alpha)"
msgstr "བདམས་པའི་ཁུལ་ཁོངས། ( ཡང་ན alpha ) ལ“འཛིན་མཁན”འབྲས་བུ\"སྣོན་པ།"

msgid "Add a Trace of Particles effect to the selected region (or alpha)"
msgstr ""
"བདམས་པའི་ཁུལ་ཁོངས། ( ཡང་ན alpha ) ལ་རིལ་རྡུལ་རྗེས་སྙེག་འབྲས་བུ་སྣོན་པ།"

msgid "Add a beveled border to an image"
msgstr "བརྙན་རིས།་ལ་གྲིལ་མཐའ་སྣོན་པ།"

msgid "Add a border around an image"
msgstr "བརྙན་རིས།་ཀྱི་མཐའ་སྐོར་དུ་མཐའ་སྒྲོམ་སྣོན་པ།"

msgid "Add a chipped woodcarving effect to the selected region (or alpha)"
msgstr ""
"བདམས་པའི་ཁུལ་ཁོངས། ( ཡང་ན alpha ) ལ་གྲུམ་འཐོར་ཤིང་བརྐོས་ཀྱི་འབྲས་བུ་སྣོན་པ།"

msgid ""
"Add a chrome effect to the selected region (or alpha) using a specified "
"(grayscale) stencil"
msgstr ""
"དམིགས་འཛུགས་བྱས་པའི། ( སྐྱ་ཚད ) པྲ་ཚིལ་པར་གཞི་ཡིས་བདམས་པའི་ཁུལ་ཁོངས། ( ཡང་ན "
"alpha ) ལ་ཁིར་འབྱུག་འབྲས་བུ་སྣོན་པ།"

msgid "Add a cloth-like texture to the selected region (or alpha)"
msgstr "བདམས་པའི་ཁུལ་ཁོངས། ( ཡང་ན alpha ) ལ་རས་འདྲ་བའི་ཁྲ་རིས་སྣོན་པ།"

msgid ""
"Add a comic-book effect to the selected region (or alpha) by outlining and "
"filling with a gradient"
msgstr ""
"སྤྱི་ཁོག་ཞིབ་འབྲི་དང་རིམ་འགྱུར་ཁ་སྐོང་གིས་བདམས་པའི་ཁུལ་ཁོངས། ( ཡང་ན alpha ) "
"ལ་འགུལ་རིས་དཔེ་དེབ་ཀྱི་འབྲས་བུ་སྣོན་པ།"

msgid "Add a drop shadow to the selected region (or alpha)"
msgstr "བདམས་པའི་ཁུལ་ཁོངས། ( ཡང་ན alpha ) ལ་འཕྲོ་གྲིབ་སྣོན་པ།"

msgid ""
"Add a frost effect to the selected region (or alpha) with an added drop "
"shadow"
msgstr ""
"བདམས་པའི་ཁུལ་ཁོངས། ( ཡང་ན alpha ) "
"ལ་འཕྲོ་གྲིབ་ཞོར་གཏོགས་ཀྱི་སད་རྒྱག་པའི་འབྲས་བུ་སྣོན་པ།"

msgid "Add a glowing hot metal effect to the selected region (or alpha)"
msgstr ""
"བདམས་པའི་ཁུལ་ཁོངས། ( ཡང་ན alpha ) "
"ལ་རབ་ཏུ་ཚ་བའི་ལྕགས་རིགས་ཀྱི་འབྲས་བུ་སྣོན་པ།"

msgid ""
"Add a gradient effect, a drop shadow, and a background to the selected "
"region (or alpha)"
msgstr ""
"བདམས་པའི་ཁུལ་ཁོངས། ( ཡང་ན alpha ) ལ་རིམ་འགྱུར་འབྲས་བུ་དང་། འཕྲོ་གྲིབ། "
"རྒྱབ་ལྗོངས་བཅས་སྣོན་པ།"

msgid "Add a guide at the orientation and position specified (in pixels)"
msgstr ""
"དམིགས་འཛུགས་བྱས་པའི་ཁ་ཕྱོགས་དང་གནས་སར། ( བརྙན་རྒྱུ་ལྟར ) "
"ཟུར་ལྟའི་ཐིག་སྣོན་པ།"

msgid ""
"Add a guide at the position specified as a percentage of the image size"
msgstr ""
"བརྙན་རིས།་ཆེ་ཆུང་གི་བརྒྱ་ཆའི་བསྡུར་ཚད་ཀྱིས་དམིགས་འཛུགས་བྱས་པའི་གནས་སར་ཟུར་ལྟའ"
"ི་ཐིག་ཅིག་སྣོན་པ།"

msgid "Add a jagged, fuzzy border to an image"
msgstr "བརྙན་རིས།་ལ་ཁ་ཤོ་ཡོད་པའི་རབ་རིབ་ཀྱི་མུ་འགྲམ་སྣོན་པ།"

msgid ""
"Add a metallic effect to the selected region (or alpha) with reflections and"
" perspective shadows"
msgstr ""
"ལྡོག་འཕྲོ་དང་བརྟོལ་མཐོང་གྲིབ་ནག་གིས་བདམས་པའི་ཁུལ་ཁོངས། ( ཡང་ན alpha ) "
"ལ་ལྕགས་རིགས་ཀྱི་འབྲས་བུ་སྣོན་པ།"

msgid "Add a perspective shadow to the selected region (or alpha)"
msgstr "བདམས་པའི་ཁུལ་ཁོངས( ཡང་ན alpha ) ལ་བརྟོལ་མཐོང་གྲིབ་ནག་སྣོན་པ།"

msgid "Add a shadow and a highlight to the selected region (or alpha)"
msgstr "བདམས་པའི་ཁུལ་ཁོངས། ( ཡང་ན alpha) ལ་གྲིབ་ནག་དང་མཐོ་གསལ་སྣོན་པ།"

msgid "Add a shiny look and bevel effect to the selected region (or alpha)"
msgstr ""
"བདམས་པའི་ཁུལ་ཁོངས། ( ཡང་ན alpha ) "
"ལ་བལྟས་ན་འོད་མདངས་ཡོད་པའི་གསེག་ཟུར་གྱི་འབྲས་བུ་སྣོན་པ།"

msgid "Add a simple chrome effect to the selected region (or alpha)"
msgstr ""
"བདམས་པའི་ཁུལ་ཁོངས། ( ཡང་ན alpha ) ལ་སྟབས་བདེའི་ཁིར་འབྱུག་འབྲས་བུ་སྣོན་པ།"

msgid "Add a slide-film like frame, sprocket holes, and labels to an image"
msgstr ""
"བརྙན་རིས།་ལ་སྒྲོན་བརྙན་འདྲ་བའི་བརྙན་རིས།་ལྷེབ་མ་དང་། སོ་ཁུང་། "
"གདོང་འཛར་བཅས་སྣོན་པ།"

msgid "Add a subtle translucent 3D effect to the selected region (or alpha)"
msgstr ""
"བདམས་པའི་ཁུལ་ཁོངས། ( ཡང་ན alpha ) "
"ལ་ཞིབ་ཚགས་ཀྱི་ཕྱེད་ནང་གསལ་རྩ་གསུམ་འབྲས་བུ་སྣོན་པ།"

msgid "Add an eerie glow around the selected region (or alpha)"
msgstr ""
"འདེམ་པའི་ཁུལ་ཁོངས། ( ཡང་ན alpha ) ཀྱི་མཐའ་སྐོར་དུ་ཁྱད་མཚར་འོད་འཕྲོ་སྣོན་པ།"

msgid "Add background"
msgstr "རྒྱབ་ལྗོངས་སྣོན་པ།"

msgid ""
"Add blended backgrounds, highlights, and shadows to the selected region (or "
"alpha)"
msgstr ""
"བདམས་པའི་ཁུལ་ཁོངས། ( ཡང་ན alpha ) ལ་མཉམ་བསྲེས་རྒྱབ་ལྗོངས་དང་། འོད་མཐོ། "
"གྲིབ་ནག་བཅས་སྣོན་པ།"

msgid "Add drop-shadow"
msgstr "འཕྲོ་གྲིབ་སྣོན་པ།"

msgid "Add glowing"
msgstr "འོད་འབར་སྣོན་པ།"

msgid ""
"Add gradients, patterns, shadows, and bump maps to the selected region (or "
"alpha)"
msgstr ""
"བདམས་པའི་ཁུལ་ཁོངས། ( ཡང་ན alpha ) ལ་རིམ་འགྱུར་དང་། དཔེ་རིས། གྲིབ་ནག "
"འབུར་ཀོང་སྦྱར་རིས་བཅས་སྣོན་པ།"

msgid "Add psychedelic outlines to the selected region (or alpha)"
msgstr ""
"འདེམ་པའི་ཁུལ་ཁོངས། ( ཡང་ན alpha ) མཐའ་སྐོར་དུ་འཕྲུལ་བའི་སྤྱི་ཁོག་སྣོན་པ།"

msgid "Add realistic looking coffee stains to the image"
msgstr "བརྙན་རིས།་ལ་མངོན་གསུམ་གྱི་མཐོང་ཚོར་འབྲས་བུའི་འཚིག་ཇའི་ཤུལ་སྣོན་པ།"

msgid "Add shadow"
msgstr "གྲིབ་ནག་སྣོན་པ།"

msgid "After glow"
msgstr "འོད་འབར་རྗེས།"

msgid "Airbrush"
msgstr "གཏོར་མདའ།"

msgid "Alien Glow"
msgstr "འོད་འཕྲོ་ཁྱད་མཚར།"

msgid "Alien _Glow..."
msgstr "འོད་འཕྲོ་ཁྱད་མཚར། (_G)"

msgid "Alien _Neon..."
msgstr "འཇའ་སྒྲོན་ཁྱད་མཚར། (_N)"

msgid ""
"All characters in the name are either white-spaces or characters which can "
"not appear in filenames."
msgstr ""
"མིང་འདོགས་ནང་གི་ཡིག་རྟགས་ཚང་མ་ནི་ཡིག་རྟགས་སྟོང་པ་ཡིན་པའམ་ཡང་ན་ཡིག་ཆའི་མིང་ནང་ཐོན་མི་ཆོག་པའི་ཡིག་རྟགས་ཡིན།"

msgid "Allow resizing"
msgstr "ཆེ་ཆུང་སྒྱུར་ཆོག་པ།"

msgid "Alpha to _Logo"
msgstr "འགྱུར་རྟགས། Alpha(_L)"

msgid "Amplitude"
msgstr "འདར་རྒྱ།"

msgid "Angle"
msgstr "ཟུར་ཚད།"

msgid "Antialias"
msgstr "མུ་འགྲམ་འཇམ་སྙོམས།"

msgid "Arrow"
msgstr "མདའ་རྩེ།"

msgid "Azimuth"
msgstr "གནས་ཕྱོགས་ཟུར།"

msgid "B_asic II..."
msgstr "གཞི་རྩ II (_A)..."

msgid "B_urn-In..."
msgstr "སྲེག་འཇུག (_U)..."

msgid "B_utton..."
msgstr "མཐེབ་གནོན། (_U)..."

msgid "Background"
msgstr "རྒྱབ་ལྗོངས།"

msgid "Background Color"
msgstr "རྒྱབ་ལྗོངས་མདོག"

msgid "Background Image"
msgstr "རྒྱབ་ལྗོངས་པར་རིས།།"

msgid "Background color"
msgstr "རྒྱབ་ལྗོངས་མདོག"

msgid "Background image"
msgstr "རྒྱབ་ལྗོངས་བརྙན་རིས།།"

msgid "Bar"
msgstr "ལྕུག་མ།"

msgid "Bar height"
msgstr "ལྕུག་མའི་མཐོ་ཚད།"

msgid "Bar length"
msgstr "ལྕུག་མའི་རིང་ཚད།"

msgid "Base color"
msgstr "གཞི་རྩའི་ཁ་དོག"

msgid "Behavior"
msgstr "བྱ་སྤྱོད།"

msgid "Bevel Highlight"
msgstr "གསེག་ངོས་མཐོ་གསལ།"

msgid "Bevel Shadow"
msgstr "གསེག་ངོས་གྲིབ་ནག"

msgid "Bevel height (sharpness)"
msgstr "གསེག་ངོས་མཐོ་ཚད། ( སྐྱེན་ཚད )"

msgid "Bevel width"
msgstr "གསེག་ངོས་ཞེང་ཚད།"

msgid "Black"
msgstr "ནག་པོ།"

msgid "Black on white"
msgstr "ལྡེབ་དཀར་ཡིག་ནག"

msgid "Blen_ded..."
msgstr "མཉམ་བསྲེས། (_D)..."

msgid "Blend Animation needs at least three source layers"
msgstr "མཉམ་བསྲེས་འགུལ་རིས་ལ་ཉུང་མཐར་ཡང་ཁུངས་རིས་རིམ་གསུམ་དགོས།"

msgid "Blend gradient (outline)"
