msgid "About"
msgstr "འབྲེལ་ཡོད།"

msgid "All Fonts"
msgstr "ཡིག་གཟུགས་ཚང་མ།"

msgid "Back"
msgstr "ཕྱིར་འཐེན།"

msgid "Copyright"
msgstr "པར་དབང་།"

msgid "Description"
msgstr " ཞིབ་བརྗོད།"

msgid "FONT-FILE OUTPUT-FILE"
msgstr "ཡིག་གཟུགས་ཡིག་ཆ། ཕྱིར་འདོན་ཡིག་ཆ།"

msgid "Font Viewer"
msgstr "ཡིག་གཟུགས་ལྟ་ཆས།"

msgid "GNOME Font Viewer"
msgstr "GNOME ཡིག་གཟུགས་ལྟ་ཆས།"

msgid ""
"GNOME Font Viewer also supports installing new font files downloaded in the "
".ttf and other formats. Fonts may be installed only for your use or made "
"available to all users on the computer."
msgstr ""

msgid ""
"GNOME Font Viewer shows you the fonts installed on your computer for your "
"use as thumbnails. Selecting any thumbnails shows the full view of how the "
"font would look under various sizes."
msgstr ""

msgid "Info"
msgstr " ཆ་འཕྲིན།"

msgid "Install"
msgstr "སྒྲིག་འཇུག"

msgid "Install Failed"
msgstr "སྒྲིག་འཇུག་ཕམ་པ།"

msgid "Installed"
msgstr "སྒྲིག་འཇུག་བྱས་ཟིན།"

msgid "Name"
msgstr " མིང་།"

msgid "Quit"
msgstr "ཕྱིར་འདོན་པ།"

msgid "Run '%s --help' to see a full list of available command line options."
msgstr ""
"“%s "
"--help”འཁོར་སྐྱོད་བྱས་ནས་སྤྱོད་རུང་བཀའ་བརྡའི་འདེམ་ཚན་གྱི་རེའུ་མིག་ཆ་ཚང་ལ་ལྟ་དགོས།"

msgid "SIZE"
msgstr "ཆེ་ཆུང་།"

msgid "Show the application's version"
msgstr "ཉེར་སྤྱོད་བྱ་རིམ་པར་གཞི་མངོན་པ།"

msgid "Style"
msgstr "ཉམས་འགྱུར།"

msgid "TEXT"
msgstr "ཡི་གེ"

msgid "Text to thumbnail (default: Aa)"
msgstr "བསྡུས་རིས་ཀྱིས་བཀོལ་བའི་ཡི་གེ་སྐྱེད་འགྲུབ། ( སོར་བཞག：Aa)"

msgid "This font could not be displayed."
msgstr "ཡིག་གཟུགས་མངོན་མི་ཐུབ་པ།"

msgid "Thumbnail size (default: 128)"
msgstr "བསྡུས་རིས་ཀྱི་ཆེ་ཆུང་། ( སོར་བཞག：128)"

msgid "Type"
msgstr " རིགས་རྣམ།"

msgid "Version"
msgstr "པར་གཞི།"

msgid "View fonts on your system"
msgstr ""

msgid "[FILE...]"
msgstr "[ཡིག་ཆ།...]"

msgid "fonts;fontface;"
msgstr "fonts;fontface;ཡིག་གཟུགས ;"

