msgstr "（མིག་སྔར།）UNIXགསང་ཨང་།"

msgid "...Sorry, your time is up!\n"
msgstr "...དགོངས་པ་མ་ཚོམ།ཁྱེད་རང་གི་དུས་ཚོད་རྫོགས་ཚར་འདུག\n"

msgid "...Time is running out...\n"
msgstr "..དུས་ཚོད་རྫོགས་ལ་ཉེ།....\n"

msgid "Access granted (last access was %ld seconds ago)."
msgstr "ལྟ་སྤྱོད་བྱས་ཆོག (ཐེངས་སྔོན་མའི་ལྟ་སྤྱོད་ནི་སྐར་ཆ་ %ld ཡི་སྔོན）"

msgid "Account locked due to %u failed logins"
msgstr "%u ཕམ་པའི་ཐོ་འཇུག་གི་རྐྱེན་གྱིས་རྩིས་ཐེམ་སྒྲོག་འཇུག་བྱས།"

msgid "Account temporary locked (%ld seconds left)"
msgstr "རྩིས་ཐེམ་གནས་སྐབས་སྒྲོག་བཙུད།"

msgid "Application needs to call libpam again"
msgstr "ཉེར་སྤྱོད་བྱ་རིམ་གྱིས་ཡང་བསྐྱར་libpamའདོན་སྤྱོད་བྱེད་དགོས་པ།"

msgid "Authentication error"
msgstr "ར་སྤྲོད་ནོར་འཁྲུལ།"

msgid "Authentication failure"
msgstr "བདེན་དཔང་ཕམ་པ།"

msgid "Authentication information cannot be recovered"
msgstr "ར་སྤྲོད་ཆ་འཕྲིན་སླར་གསོ་བྱེད་ཐབས་བྲལ།"

msgid "Authentication service cannot retrieve authentication info"
msgstr "ར་སྤྲོད་ཞབས་ཞུས་ར་སྤྲོད་ཆ་འཕྲིན་འཚོལ་ཐབས་བྲལ་བ།"

msgid "Authentication service cannot retrieve user credentials"
msgstr "ར་སྤྲོད་ཞབས་ཞུས་སྤྱོད་མཁན་གྱི་ཐོབ་ཐང་དཔང་ཡིག་འཚོལ་ཐབས་བྲལ་བ།"

msgid "Authentication token aging disabled"
msgstr "གཏན་འགོག་བྱས་པའི་ར་སྤྲོད་བཀའ་རྟགས་བྱང་བུ་དུས་ཡོལ་སྒྲིག་དྲས་རྒྱག།"

msgid "Authentication token expired"
msgstr "ར་སྤྲོད་བཀའ་རྟགས་ཀྱི་ནུས་པ་ཤོར་བ།"

msgid "Authentication token is no longer valid; new one required"
msgstr ""
"ར་སྤྲོད་བཀའ་རྟགས་ཨང་རྟགས་དུས་ལས་ཡོལ་ཟིན་པས། "
"ར་སྤྲོད་བཀའ་རྟགས་བྱང་བུ་གསར་པ་དགོས།"

msgid "Authentication token lock busy"
msgstr "ར་སྤྲོད་བཀའ་རྟགས་སྒྲོག་བྱང་བྲེལ་བ་ཆེ་བ།"

msgid "Authentication token manipulation error"
msgstr "ར་སྤྲོད་བཀའ་རྟགས་བཀོལ་སྤྱོད་ནོར་འཁྲུལ།"

msgid "BAD PASSWORD: %s"
msgstr "ནུས་མེད་གསང་ཨང་།%s"

msgid "Bad item passed to pam_*_item()"
msgstr "བརྒྱུད་སྤྲོད་pam_*_item() ལ་སླེབས་པའི་རྣམ་གྲངས་ལ་ནུས་པ་མེད་པ།"

msgid "Cannot make/remove an entry for the specified session"
msgstr "དམིགས་འཛུགས་བྱས་པའི་གླེང་མོལ་ལ་/འདོར་བྱང་གསར་འཛུགས་བྱ་ཐབས་བྲལ།"

msgid "Changing STRESS password for %s."
msgstr "s%ཆེད་STRESSགསང་ཨང་བཟོ་བཅོས་བྱེད་པ།"

msgid "Changing password for %s."
msgstr "s%ཆེད་STRESSགསང་ཨང་བཟོ་བཅོས་བྱེད་པ།"

msgid "Conversation error"
msgstr "བརྗེ་སྤོར་ནོར་འཁྲུལ།"

msgid "Conversation is waiting for event"
msgstr "བརྗེ་སྤོར་གྱིས་དོན་རྐྱེན་ལ་སྒུག་བཞིན་ཡོད།"

msgid "Creating directory '%s'."
msgstr "ཡིག་ཁུག་%sགསར་འཛུགས་བྱེད་བཞིན་ཡོད།"

