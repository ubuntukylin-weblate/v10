msgid "Error creating file"
msgstr "ཡིག་ཆ་བཟོ་སྐབས་ནོར་འཁྲུལ་ཐོན་པ།"

msgid "Error creating file. Please choose another location and retry."
msgstr ""
"ཡིག་ཆ་བཟོ་སྐབས་ནོར་འཁྲུལ་ཐོན་པ། གནས་ས་གཞན་གདམ་ནས་ཡང་བསྐྱར་ཚོད་ལེན་བྱེད་དགོས།"

msgid "Error loading the help page"
msgstr "རོགས་རམ་ལྡེབ་ངོས་འགེལ་སྐབས་ནོར་འཁྲུལ་ཐོན་པ།"

msgid "GNOME Screenshot"
msgstr "བཅས་རིས་ GNOME"

msgid ""
"GNOME Screenshot allows you to take screenshots even when it's not open: "
"just press the PrtSc button on your keyboard, and a snapshot of your whole "
"screen will be saved to your Pictures folder. Hold Alt while pressing PrtSc "
"and you will get a screenshot of only the currently selected window."
msgstr ""

msgid ""
"GNOME Screenshot is a simple utility that lets you take pictures of your "
"computer screen. Screenshots can be of your whole screen, any specific "
"application, or a selected rectangular area. You can also copy the captured "
"screenshot directly into the GNOME clipboard and paste it into other "
"applications."
msgstr ""

msgid "Grab a window instead of the entire screen"
msgstr "སྒེའུ་ཁུང་གཅོད་ལེན་ཡིན་པ་ལས་བརྙན་ཡོལ་ཆ་ཚང་མིན།"

msgid "Grab after a _delay of"
msgstr "བཅད་རིས་ཀྱི་ཕྱི་འགྱངས་དུས་ཚོད། (_D)"

msgid "Grab an area of the screen instead of the entire screen"
msgstr ""
"བརྙན་ཡོལ་གྱི་ཁུལ་ཁོངས་ཤིག་གཅོད་ལེན་བྱེད་པ་ལས་བརྙན་ཡོལ་ཆ་ཚང་གཅོད་ལེན་མི་བྱེད་པ།"

msgid ""
"Grab just the current window, rather than the whole desktop. This key has "
"been deprecated and it is no longer in use."
msgstr ""
"མིག་སྔའི་སྒེའུ་ཁུང་ཁོ་ན་གཅོད་ལེན་བྱེད་པ་ལས་ཅོག་ངོས་ཆ་ཚང་གཅོད་ལེན་མི་བྱེད། "
"འགག་རྩའི་ཨང་འདི་ནུས་ཤོར་ཟིན་པས་ཡང་བསྐྱར་སྤྱོད་མི་ཐུབ།"

msgid "Grab the current _window"
msgstr "མིག་སྔའི་སྒེའུ་ཁུང་གཅོད་ལེན། (_W)"

msgid "Grab the whole sc_reen"
msgstr "བརྙན་ཡོལ་ཆ་ཚང་གཅོད་ལེན། (_R)"

msgid "Help"
msgstr "རོགས་རམ།"

msgid "Include Border"
msgstr "མཐའ་སྒྲོམ་ཚུད་པ།"

msgid "Include ICC Profile"
msgstr "སྡེབ་སྒྲིག་ཡིག་ཆ ICC ཚུད་པ།"

msgid "Include Pointer"
msgstr "སྟོན་མདའ་ཚུད་པ།"

msgid "Include _pointer"
msgstr "སྟོན་མདའ་ཚུད་པ། (_P)"

msgid "Include the ICC profile of the target in the screenshot file"
msgstr "བཅད་རིས་ཀྱི་ནང་སྡེབ་སྒྲིག་ཡིག་ཆ ICC ཚུད་པ། "

msgid "Include the pointer in the screenshot"
msgstr "བཅད་རིས་ཀྱི་ནང་ཙིག་མདའི་སྟོན་མདའ་ཚུད་པ།"

msgid "Include the pointer with the screenshot"
msgstr "བཅད་རིས་ཀྱི་ནང་ཙིག་མདའི་སྟོན་མདའ་ཚུད་པ།"

msgid "Include the window _border"
msgstr "སྒེའུ་ཁུང་མཐའ་སྒྲོམ་ཚུད་པ། (_B)"

msgid "Include the window border with the screenshot"
msgstr "བཅད་རིས་ཀྱི་ནང་སྒེའུ་ཁུང་མཐའ་སྒྲོམ་ཚུད་པ།"

msgid "Include the window manager border along with the screenshot"
msgstr "བཅད་རིས་ཀྱི་ནང་སྒེའུ་ཁུང་དོ་དམ་ཆས་ཀྱི་མཐའ་སྒྲོམ་ཚུད་པ།"

msgid "Interactively set options"
msgstr "སྤེལ་རེས་སྒྲིག་འགོད་ཀྱི་འདེམས་ཚན།"

msgid "Last save directory"
msgstr "ཐེངས་སྔོན་མར་ཉར་ཚགས་བྱས་པའི་དཀར་ཆག"

msgid "None"
msgstr "བདེ་འཇགས་སྲུང་སྐྱོང་མེད་པ།"

msgid "Overwrite existing file?"
msgstr "ཡོད་པའི་ཡིག་ཆ་སྣོན་དགོས་སམ།"

msgid "Print version information and exit"
msgstr "པར་གཞི་ཆ་འཕྲིན་པར་འདེབས་བྱས་ནས་ཕྱིར་འདོན།"

