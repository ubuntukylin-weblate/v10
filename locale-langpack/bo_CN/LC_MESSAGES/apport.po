msgid "%prog <report number>"
msgstr "%prog <སྙན་ཞུའི་ཨང་>"

msgid "%prog [options] [symptom|pid|package|program path|.apport/.crash file]"
msgstr ""
"%prog [འདེམས་ཚན་] [ནད་རྟགས|PID|མཉེན་ཆས་དུམ|བྱ་རིམ་དཀར་ཆག|.apport/.crash "
"file]"

msgid "&Cancel"
msgstr "ལེན་པ།(&C)"

msgid "&Confirm"
msgstr "གཏན་འཁེལ།(&C)"

msgid "&Done"
msgstr "ལེགས་འགྲུབ།(&D)"

msgid "&Examine locally"
msgstr "རང་གནས་ཞིབ་བཤེར།(&E)"

msgid "&Keep report file for sending later or copying to somewhere else"
msgstr ""
"སྙན་ཞུའི་ཡིག་ཆ་ཉར་ནས་ཅུང་ཙམ་དུ་གནས་གཞན་དུ་སྐུར་བའམ་འདྲ་བཟོ་བྱེད་པ།(&K)"

msgid "&Send report (%s)"
msgstr "སྐུར་བ་སྙན་ཞུ་(&S)(%s)"

msgid "&View report"
msgstr "སྙན་ཞུ་ལྟ་བ།(&V)"

msgid "(%i bytes)"
msgstr "(ཡིག་ཚིགས་%i )"

msgid "(binary data)"
msgstr "(གཉིས་གོང་འགྲིལ་ལུགས་ཀྱི་གཞི་གྲངས་)"

msgid "<big><b>Collecting problem information</b></big>"
msgstr "<big><b>གདན་དོན་ཆ་འཕྲིན་བསྡུ་པ།</b></big>"

msgid "<big><b>Sorry, an internal error happened.</b></big>"
msgstr "<big><b>ནང་ཁུལ་གྱི་ནོར་འཁྲུལ་བྱུང་བ།</b></big>"

msgid "<big><b>Uploading problem information</b></big>"
msgstr "<big><b>གནད་དོན་གྱི་ཆ་འཕྲིན་ཡར་སྐྱེལ་པ།</b></big>"

msgid "Add an extra tag to the report. Can be specified multiple times."
msgstr "སྙན་ཞུ་རུ་མཚོན་རྟགས་གཞན་སྣོན་པ། ཐེངས་མང་གཏན་འཁེལ་བྱ་ཆོག"

msgid ""
"After closing this message please click on an application window to report a"
" problem about it."
msgstr ""
"བརྡ་འཕྲིན་འདི་ཁ་བརྒྱབ་རྗེས། "
"གནད་དོན་ཞུ་དགོས་པའི་བྱ་རིམ་སྒེའུ་ཁུང་ལ་ཆིག་རྡེབ་བྱོས།"

msgid ""
"After the problem report has been sent, please fill out the form in the\n"
"automatically opened web browser."
msgstr ""

msgid ""
"An Ubuntu package has a file conflict with a package that is not a genuine "
"Ubuntu package."
msgstr ""
"མཉེན་ཆས་ཐུམ Ubuntuདང་དངོས་མིན་གྱི་མཉེན་ཆས་ཐུམ Ubuntu "
"བར་དུ་ཡིག་ཆ་འགལ་བ་བྱུང་།"

msgid "An error occurred while attempting to process this problem report:"
msgstr "གནད་དོན་སྙན་ཞུ་འབུལ་སྐབས་སུ་སྐྱོན་འབྱུང་བ།"

msgid "Apport"
msgstr "Apport"

msgid "Apport crash file"
msgstr "Apport ཐོར་ཞིག་གི་ཡིག་ཆ་"

msgid ""
"Authentication is required to collect system information for this problem "
"report"
msgstr "གནད་དོན་སྙན་ཞུ་བསྡུ་སྐབས་རྒྱུད་ཁོངས་ཆ་འཕྲིན་ལ་དབང་སྤྲོད་དགོས།"

msgid ""
"Build a temporary sandbox and download/install the necessary packages and "
"debug symbols in there; without this option it assumes that the necessary "
"packages and debug symbols are already installed in the system. The argument"
" points to the packaging system configuration base directory; if you specify"
" \"system\", it will use the system configuration files, but will then only "
"be able to retrace crashes that happened on the currently running release."
msgstr ""

msgid "Cache directory for packages downloaded in the sandbox"
msgstr "བྱེ་སྒམ་གྱི་མཉེན་ཆས་ཁུག་མའི་ལྷོད་གསོག་དཀར་ཆག་ཏུ་ཕབ།"

msgid "Cancel"
msgstr "འདོར་བ།"

msgid "Cancel and &ignore future crashes of this program version"
msgstr ""
"པར་གཞི་འདིའི་རྗེས་སུ་བྱུང་བའི་ཐོར་ཞིག་མཚམས་འཇོག་པ་དང་སྣང་མེད་དུ་གཏོང་།(&I)"

msgid ""
"Cannot connect to crash database, please check your Internet connection."
msgstr ""
"ཐོར་ཞིག་གཞི་གྲངས་མཛོད་དུ་འབྲེལ་མི་ཐུབ། ཁྱེད་ཀྱི་ "
"Internetཡི་འབྲེལ་མཐུད་ལ་ཞིབ་བཤེར་བྱོས།"

msgid "Cannot create report"
msgstr "སྙན་ཞུ་བཟོ་མི་ཐུབ།"

msgid "Choices:"
msgstr "འདེམ་པ།"

msgid "Click a window as a target for filing a problem report."
msgstr "སྒེའུ་ཁུང་ཅིག་ལ་ཆིག་རྡེབ་བྱས་ནས་གནད་དོན་སྙན་ཞུའི་དམིགས་ཡུལ་བྱ།"

msgid "Collect system information"
msgstr "རྒྱུད་ཁོངས་ཆ་འཕྲིན་བསྡུ་པ།"

msgid "Collecting Problem Information"
msgstr "གནད་དོན་ཆ་འཕྲིན་བསྡུ་བཞིན་པ།"

msgid "Collecting problem information"
msgstr "གནད་དོན་ཆ་འཕྲིན་བསྡུ་བཞིན་པ།"

msgid "Continue"
msgstr "མུ་མཐུད།"

msgid "Could not determine the package or source package name."
msgstr "ཐུམ་མམ་ཁུངས་ཨང་ཐུམ་གྱི་མིང་ཞིབ་བཤེར་བྱ་མི་ཐུབ།"

msgid "Crash report"
msgstr "ཐོར་ཞིག་གི་སྙན་ཞུ་"

msgid ""
"Create and use third-party repositories from origins specified in reports"
msgstr ""
"སྙན་ཞུ་ལས་བསྟན་པའི་ཁུངས་ཀྱིས་ཕྱོགས་གསུམ་པའི་མཉེན་ཆས་མཛོད་གསར་བཟོ་དང་བེད་སྤྱོད་བྱ།"

msgid "Destination directory exists and is not empty."
msgstr "དམིགས་ཚད་དཀར་ཆག་ཡོད་ཅིང་སྟོང་མིན།"

msgid ""
"Directory for unpacked packages. Future runs will assume that any already "
"downloaded package is also extracted to this sandbox."
msgstr ""
"ཐུམ་གྱི་དཀར་ཆག་བཙིར་འགྲོལ་བྱེད་པ། "
"རྗེས་ཀྱི་འཁོར་སྐྱོད་ནི་ཕབ་པའི་ཐུབ་ཚང་མ་བྱེ་སྒམ་འདིར་ལེན་པ་དབང་དུ་བཏང་།"

msgid ""
"Display retraced stack traces and ask for confirmation before sending them "
"to the crash database."
msgstr ""
"ལོག་པའི་དོས་ཀྱི་ཆ་འཕྲིན་ཤར། "
"དེ་ནི་ཐོར་ཞིག་གཞི་གྲངས་མཛོད་དུ་སྐུར་ཡོད་མེད་འདྲི་བ་དང་གཏན་འཁེལ་བྱོས།"

msgid "Do not put the new traces into the report, but write them to stdout."
msgstr "རྗེས་ཤུལ་གསར་བ་སྙན་ཞུ་རུ་འབྲི་མི་ཐུབ་ཀྱང་། ཚད་ལྡན་ཕྱིར་འདོན་དུ་ཕྲིས།"

msgid "Error"
msgstr "ནོར་འཁྲུལ།"

msgid "Error: %s"
msgstr "ནོར་འཁྲུལ། %s"

msgid "Error: %s is not an executable. Stopping."
msgstr "ནོར་འཁྲུལ། %s ནི་ལག་བསྟར་རུང་བའི་ཡིག་ཆ་མིན། མཚམས་འཇོག"

msgid ""
"File a bug report about a symptom. (Implied if symptom name is given as only"
" argument.)"
msgstr "ནད་རྟགས་ཤིག་ལ་དམིགས་ནས་bugཞུ་པ།"

msgid "File does not exist."
msgstr "ཡིག་ཆ་མེད་པ།"

msgid "Force Closed"
msgstr "བཙན་པོས་ཁ་རྒྱག"

msgid "Hide Details"
msgstr "ཞིབ་ཕྲིའ་ཆ་འཕྲིན་བོས།"

msgid "If you notice further problems, try restarting the computer."
msgstr "གལ་ཏེ་ཁྱེད་ཀྱིས་གནད་དོན་ཇེ་མང་མཐོང་ན། རྩིས་འཁོར་ཡང་བསྐྱར་མགོ་སློངས།"

msgid "Ignore future problems of this program version"
msgstr "རྗེས་སུ་བྱ་རིམ་པར་གཞི་འདི་གནད་དོན་སྣང་མེད་གཏོང་།"

msgid "Ignore future problems of this type"
msgstr "རྗེས་སུ་ནོར་འཁྲུལ་རིགས་འདི་སྣང་མེད་གཏོང་།"

msgid ""
"In bug filing mode, save the collected information into a file instead of "
"reporting it. This file can then be reported later on from a different "
"machine."
msgstr ""

msgid ""
"Information is being collected that may help the developers fix the problem "
"you report."
msgstr ""
"ཆ་འཕྲིན་བསྡུ་པ། "
"ཆ་འཕྲིན་འདི་དག་གིས་གསར་སྤེལ་མི་སྣ་ལ་ཁྱེད་ཀྱི་སྙན་ཞུའི་གནད་དོན་བཟོ་བཅོས་ལ་རོགས་བྱེད།"

msgid ""
"Install an extra package into the sandbox (can be specified multiple times)"
msgstr ""
"མཉེན་ཆས་ཐུམ་གཞན་དག་བྱེ་སྒམ་དུ་སྒྲིག་འཇུག་བྱ།(ཐེངས་མང་གཏན་འཁེལ་བྱ་ཐུབ།)"

msgid "Invalid PID"
msgstr "ནུས་མེད་ཀྱི་ PID"

msgid "Invalid problem report"
msgstr "ནུས་མེད་ཀྱི་གནད་དོན་སྙན་ཞུ་"

msgid "Launch a browser now"
msgstr "བཤར་ཆས་ཤིག་མགོ་སློངས།"

msgid "Leave Closed"
msgstr "ཁ་རྒྱག་གི་རྣམ་པ་གཏན་སྲུང་བྱེད།"

msgid "Memory exhaustion"
msgstr "ནང་གསོག་ཟད་ཚར།"

msgid "Network problem"
msgstr "དྲ་བའི་གནད་དོན།"

msgid "No additional information collected."
msgstr "ཆ་འཕྲིན་ཇེ་མང་བསྡུ་མི་ཐུབ།"

msgid "No package specified"
msgstr "གཏན་འཁེལ་གྱི་མཉེན་ཆས་ཐུམ་མེད།"

msgid "No pending crash reports. Try --help for more information."
msgstr ""
"ཐོར་ཞིག་གི་སྙན་ཞུ་མེད།  --helpཚོད་ལྟ་བྱས་ནས་ཆ་འཕྲིན་ཇེ་མང་འཐོབ་ཏུ་འཇུག"

msgid "OK"
msgstr "གཏན་འཁེལ"

msgid "OK to send these as attachments? [y/n]"
msgstr "ཞོར་ཆས་ལྟར་འདི་དག་སྐུར་བ་ཡིན་མིན་གཏན་འཁེལ་བྱེད།[y/n]"

msgid "Override report's CoreFile"
msgstr "སྙན་ཞུའི་ཡིག་ཆ་གཙོ་བོ་ཁེབས་པ།"

msgid "Override report's ExecutablePath"
msgstr "སྙན་ཞུའི་།ལག་བསྟར་འགྲོ་ལམ་ཁེབས་པ"

msgid "Override report's ProcMaps"
msgstr "སྙན་ཞུའི་འཕེལ་རིམ་རེའུ་མིག་ཁེབས་པ།"

msgid "Package %s does not exist"
msgstr "མཉེན་ཆས་ཐུམ %s མེད་པ།"

msgid "Package: %s"
msgstr "མཉེན་ཆས་ཐུམ་%s"

msgid "Password:"
msgstr "གསང་ཨང་།"

msgid ""
"Path to a file with the crash database authentication information. This is "
"used when specifying a crash ID to upload the retraced stack traces (only if"
" neither -g, -o, nor -s are specified)"
msgstr ""

msgid "Path to file (Enter to cancel):"
msgstr "ཡིག་ཆའི་འགྲོ་ལམ་(འབེབས་མཐེས་ལེན་པ།)："

msgid "Path to the duplicate sqlite database (default: no duplicate checking)"
msgstr ""
"SQLite གཞི་གྲངས་མཛོད་འགྲོ་ལམ་བསྐྱར་ཟློས།(སོར་བཞག བསྐྱར་དུ་ཞིབ་བཤེར་མི་བྱེད།)"

msgid "Permission denied"
msgstr ""

msgid "Please choose (%s):"
msgstr "འདེམ་རོགས།(%s)："

msgid "Please enter your account information for the %s bug tracking system"
msgstr "ནོར་འཚོལ་རྒྱུད་ཁོངས་ %sལ་ཁྱེད་ཀྱིས་རྩིས་ཐེམ་ནང་འཇུག་བྱེད་རོགས།"

msgid ""
"Please enter your password to access problem reports of system programs"
msgstr ""
"ཁྱེད་ཀྱིས་གསང་ཨང་ནང་འཇུག་སྟེ་རྒྱུད་ཁོངས་བྱ་རིམ་གྱི་གནད་དོན་སྙན་ཞུ་ལ་ལྟ་རོགས།"

msgid "Prepend timestamps to log messages, for batch operation"
