msgid "Adjusts the brightness of the image coming from the camera"
msgstr "བརྙན་ལེན་ཆས་ཀྱི་བརྙན་རིས་ཀྱི་དཀར་ཚད་ལེགས་སྒྲིག་བྱེད།"

msgid "Adjusts the contrast of the image coming from the camera"
msgstr "བརྙན་ལེན་ཆས་ཀྱི་བརྙན་རིས་ཀྱི་སྡུར་ཚད་ལེགས་སྒྲིག་བྱེད།"

msgid "Adjusts the hue (color tint) of the image coming from the camera"
msgstr ""
"བརྙན་ལེན་ཆས་ཀྱི་བརྙན་རིས་ཀྱི་མདོག་གདངས་ལེགས་སྒྲིག་བྱེད།(མདོག་གི་དཀར་ནག)"

msgid "Adjusts the saturation of the image coming from the camera"
msgstr "བརྙན་ལེན་ཆས་ཀྱི་བརྙན་རིས་ཀྱི་ཚད་ལོངས་ཚད་ལེགས་སྒྲིག་བྱེད།"

msgid "Are you sure you want to permanently delete the file?"
msgid_plural "Are you sure you want to permanently delete %d files?"
msgstr[0] "ཁྱེད་ཀྱིས་དངོས་གནས་ཡིག་ཆ་%d འདོར་འདོད་དམ།"
msgstr[1] "ཁྱེད་ཀྱིས་དངོས་གནས་ཡིག་ཆ་%d འདོར་འདོད་དམ།"

msgid "Brightness"
msgstr "གསལ་ཚད།"

msgid "Burst"
msgstr "བསྟུད་མར་པར་ལེན།"

msgid "Burst mode"
msgstr "བསྟུད་མར་པར་ལེན་པའི་རྣམ་པ།"

msgid "Camera device string indicator"
msgstr "པར་ཆས་སྒྲིག་ཆས་ཡིག་རྟགས་ཕྲེང་སྟོན་ཆས།"

msgid "Cancellable initialization not supported"
msgstr "བར་ཆད་རུང་བའི་ཐོག་མ་ཅན་དུ་སྒྱུར་བར་རྒྱབ་སྐྱོར་མི་བྱེད།"

msgid "Capture"
msgstr ""

msgid "Cheese"
msgstr "ཆེ་ཙི།"

msgid "Cheese Webcam Booth"
msgstr "ཆེ་ཙི་མགོ་ཆེ།"

msgid "Cheese Website"
msgstr "ཆའེ་ཙི་དྲ་ཚིགས།"

msgid ""
"Cheese uses your webcam to take photos and videos, applies fancy special "
"effects and lets you share the fun with others."
msgstr ""

msgid "Choose an Effect"
msgstr "ཐོབ་འབྲས་འདེམ་པ།"

msgid "Contrast"
msgstr "བསྡུར་ཚད།"

msgid "Could not delete %s"
msgstr "%s བསུབ་ཐབས་མེད།"

msgid "Could not move %s to trash"
msgstr "%sསྙིགས་སྒམ་དུ་སྤོ་མི་ཐུབ།"

msgid "Could not open %s"
msgstr "%s ཁ་ཕྱེ་ཐབས་མེད།"

msgid "Could not save %s"
msgstr "%sཉར་མི་ཐུབ།"

msgid "Countdown length"
msgstr "དུས་ཚོད་ལྡོག་རྩིས་ཀྱི་རིང་ཚད།"

msgid "DEVICE"
msgstr "སྒྲིག་ཆས།"

msgid ""
"Defines the path where the photos are stored. If empty, \"XDG_PHOTO/Webcam\""
" will be used."
msgstr ""
"འདྲ་པར་ཉར་བའི་འགྲོ་ལམ་སྟོད། གལ་ཏེ་སྟོང་བ་ཡིན་ན། “XDG_PHOTO/Webcam”སྤྱོད།"

msgid ""
"Defines the path where the videos are stored. If empty, \"XDG_VIDEO/Webcam\""
" will be used."
msgstr ""
"བརྙན་ཟློས་ཉར་བའི་འགྲོ་ལམ་སྟོད། གལ་ཏེ་སྟོང་བ་ཡིན་ན།“XDG_VIDEO/Webcam”སྤྱོད།"

msgid "Delay between photos (seconds)"
msgstr "འདྲ་པར་ཕྱིར་འགྱངས།(སྐར་ཆ།)"

msgid "Delete"
msgstr "སྤྱོད་མཁན་བསུབ་པ།"

msgid "Device"
msgstr "སྒྲིག་ཆས།"

msgid "Device capabilities not supported"
msgstr "སྒྲིག་ཆས་འདིའི་བྱེད་ནུས་ཚང་མ་རྒྱབ་སྐྱོར་མི་བྱེད།"

msgid "Device to use as a camera"
msgstr "བརྙན་མིག་ཏུ་བཀོལ་བའི་སྒྲིག་ཆས།"

msgid "Effects"
msgstr "ཕན་འབྲས།"

msgid "Fire _flash"
msgstr "འོད་འཚེར།(_F)"

msgid "Fire flash before taking a photo"
msgstr "པར་ལེན་པའི་སྔོན་དུ་འོད་འཚེར་བ།"

msgid "Hue"
msgstr "ཚོས་གཞི།"

msgid "If you delete an item, it will be permanently lost"
msgid_plural "If you delete the items, they will be permanently lost"
msgstr[0] "གལ་ཏེ་ཁྱེད་ཀྱིས་རྣམ་གྲངས་འདི་དག་འདོར་ན། དེ་དག་ཡལ་བ་ཡིན།"
msgstr[1] "གལ་ཏེ་ཁྱེད་ཀྱིས་རྣམ་གྲངས་འདི་དག་འདོར་ན། དེ་དག་ཡལ་བ་ཡིན།"

msgid "Image"
msgstr "བརྙན་རིས།"

msgid "Image brightness"
msgstr "བརྙན་རིས་དཀར་ཚད།"

msgid "Image contrast"
msgstr "བརྙན་རིས་བསྡུར་ཚད།"

msgid "Image hue"
msgstr "རི་མོའི་ཚོན་མདངས།"

msgid "Image saturation"
msgstr "བརྙན་རིས་ཚད་ལོངས་ཚད།"

msgid "Last selected effect"
msgstr "ཐེངས་སྔོན་མར་བདམས་པའི་ཕན་འབྲས།"

msgid "Leave fullscreen mode and go back to windowed mode"
msgstr "བརྙན་ཤེལ་ཡོངས་རྣམ་ལ་བྲལ་ནས་སྒེའུ་ཁུང་རྣམ་པར་ལོག།"

msgid "Move to _Trash"
msgstr "སྙིགས་སྒམ་དུ་སྤོ།(_T)"

msgid "Name of the installed effect that was selected last"
msgstr "ཐེངས་སྔོན་མར་བདམས་པའི་སྒྲིག་སྦྱོར་བྱས་པའི་ཕན་འབྲས་ཀྱི་གྲངས་ཀ"

msgid "Navigate to the next page of effects"
msgstr "ཐོབ་འབྲས་ཕྱི་མར་ཕྱོགས་ཁྲིད།"

msgid "Navigate to the previous page of effects"
msgstr "ཐོབ་འབྲས་སྔར་མར་ཕྱོགས་ཁྲིད།"

msgid "No Effect"
msgstr "ནུས་འབྲས་མེད།"

msgid "No device found"
msgstr "སྒྲིག་ཆས་མ་རྙེད་པ།"

msgid "No effects found"
msgstr "ཕན་འབྲས་མ་རྙེད་པ།"

msgid "Number of photos"
msgstr "འདྲ་པར་ཨང་གྲངས།"

msgid "Number of photos in burst mode"
msgstr "པར་བསྟུད་མར་ལེན་གྲངས།"

msgid "One or more needed GStreamer elements are missing: "
msgstr "GStreamer གཞི་རྒྱུ་གཅིག་གམ་མང་པོ་མི་འདང་།： "

msgid "Open"
msgstr "ཁ་ཕྱེ་བ།"

msgid "Output version information and exit"
msgstr "རྩིས་ཐེམ་གྱི་ཆ་འཕྲིན་ཕྱིར་གཏོང་ནས་ཕྱིར་ཐོན།"

msgid "P_references"
msgstr "ཐོག་མའི་འདེམས་ཚན།(_R)"

msgid "Photo"
msgstr "འདྲ་པར།"

msgid "Photo burst mode"
msgstr "བསྟུད་མར་པར་ལེན་པའི་རྣམ་པ།"

msgid "Photo height"
msgstr "འདྲ་པར་མཐོ་ཚད།"

msgid "Photo mode"
msgstr "རྐྱང་ལེན་རྣམ་པ།"

msgid "Photo path"
msgstr "འདྲ་པར་འགྲོ་ལམ།"

msgid "Photo resolution"
msgstr "འདྲ་པར་འབྱེད་ཕྱོད།"

msgid "Photo width"
msgstr "འདྲ་པར་ཞེང་ཚད།"

msgid "Preferences"
msgstr "མཐོ་རིམ།"

msgid "Record a Video"
msgstr "བརྙན་ཕབ།"

msgid "Record a video"
msgstr "བརྙན་ཕབ།"

msgid "Record a video using a webcam"
msgstr "དྲ་བའི་བརྙན་མིག་སྤྱད་ནས་བརྙན་ཐུང་དུམ་བུ་ཞིག་ཕབ་བཟོ་བྱེད་པ།"

msgid "Saturation"
msgstr "ཚད་ལོངས་ཚད།"

msgid "Save"
msgstr "བསྒྱུར་བཅོས་ཉར་ཚགས།"

msgid "Save File"
msgstr "ཡིག་ཆ་ཉར་ཚགས།"

msgid "Save _As…"
msgstr "གཞན་དུ་བཅོལ་བ།(_A)..."

msgid "Set to true to fire a flash before taking a photo"
msgstr "གལ་ཏེ་པར་ལེན་དུས་འོད་འཚེར་འདོད་ན། trueལ་བཀོད་རོགས།"

msgid "Set to true to show a countdown before taking a photo"
msgstr "གལ་ཏེ་པར་ལེན་དུས་དུས་ལྡོག་རྩིས་འདོད་ན། trueལ་བཀོད་རོགས།"

msgid "Shutter"
msgstr ""

msgid "Shutter sound"
msgstr "མགྱོགས་སྒོ་སྒྲ།"

msgid "Skip"
msgstr "མཆོང་བ།"

msgid "Skip all"
msgstr "ཚང་མ་སྣང་མེད་དུ་གཏོང་"

msgid "Start in fullscreen mode"
msgstr "བརྙན་ཡོངས་རྣམ་པས་མགོ་སློང་།"

msgid "Start in wide mode"
msgstr "བརྙན་ཡངས་རྣམ་པས་མགོ་སློང་།"

msgid "Stop recording"
msgstr "སྒྲ་ཕབ་མཚམས་འཇོག"

msgid "Stop taking pictures"
msgstr "པར་རྒྱག་མཚམས་འཇོག"

msgid "Take Multiple Photos"
msgstr "འདྲ་པར་མང་པོ་ལེན།"

msgid "Take a Photo"
msgstr "པར་རྒྱག་པ།"

msgid "Take a photo using a webcam"
msgstr "དྲ་བའི་བརྙན་མིག་སྤྱད་ནས་པར་རྒྱག་པ།"

msgid "Take multiple photos"
msgstr "འདྲ་པར་མང་པོ་ལེན།"

msgid ""
"Take multiple photos in quick succession with burst mode. Use the countdown "
"to give yourself time to strike a pose, and wait for the flash!"
msgstr ""

msgid "Take multiple photos using a webcam"
msgstr "དྲ་བའི་བརྙན་མིག་སྤྱད་ནས་པར་མང་པོ་རྒྱག་པ།"

msgid "Take photos and videos with your webcam, with fun graphical effects"
msgstr ""
"བརྙན་ལེན་ཆས་བཀོལ་ནས་པར་ལེན་པའམ་བརྙན་ཕབ་པ། "
"སྤྲོ་སྣང་ལྡན་པའི་བརྙན་རིས་ཕན་འབྲས་ཀྱང་སྣོན།"

msgid "The duration of the countdown before taking a photo, in seconds"
msgstr "པར་ལེན་སྔོན་གྱི་དུས་ལྡོག་རྩིས་སྐར་ཆ་གྲངས།"

msgid "The height of the image captured from the camera, in pixels"
msgstr "བརྙན་མིག་ལས་པར་རིས་ཀྱི་མཐོ་ཚད་གཅོད་པ། འཇལ་བྱེད་ནི་བརྙན་རྒྱུ་ཡིན།"

msgid "The height of the video captured from the camera, in pixels"
msgstr "བརྙན་མིག་ལས་བརྙན་ཟློས་ཀྱི་མཐོ་ཚད་གཅོད་པ། འཇལ་བྱེད་ནི་བརྙན་རྒྱུ་ཡིན།"

msgid ""
"The length of time, in milliseconds, to delay between taking each photo in a"
" burst sequence of photos. If the burst delay is less than the countdown "
"duration, the countdown duration will be used instead."
msgstr ""

msgid "The number of photos to take in a single burst."
msgstr "ཐེངས་གཅིག་ལ་བསྟུད་མར་བླངས་པའི་འདྲ་པར་གྱི་གྲངས་ཀ "

msgid ""
"The path to the device node which points to the camera, for example "
"/dev/video0"
msgstr "བརྙན་མིག་ཀྱི་སྒྲིག་ཆས་ཚིགས་ཚེག་འགྲོ་ལམ་ལ་སྟོན་པ། དཔེར་ན /dev/video0"

msgid "The width of the image captured from the camera, in pixels"
msgstr "བརྙན་མིག་ལས་པར་རིས་ཀྱི་མཐོ་ཚད་གཅོད་པ། འཇལ་བྱེད་ནི་བརྙན་རྒྱུ་ཡིན།"

msgid "The width of the video captured from the camera, in pixels"
msgstr "བརྙན་མིག་ལས་བརྙན་ཟློས་ཀྱི་མཐོ་ཚད་གཅོད་པ། འཇལ་བྱེད་ནི་བརྙན་རྒྱུ་ཡིན།"

msgid "There was an error playing video from the webcam"
