msgid "An internal error has occured, please contact the administrator."
msgstr "ནང་ཁུལ་གྱི་ནོར་འཁྲུལ་བྱུང་ན་དོ་དམ་པ་ལ་འབྲེལ་བ་གནང་རོགས།"

msgid "Credentials check failed, you may be unable to see all information."
msgstr "དཔང་ཡིག་ཞིབ་བཤེར་ཕམ་སོང་། ཁྱོད་ཀྱིས་ཕལ་ཆེར་ཆ་འཕྲིན་ཚང་མ་མཐོང་ཐབས་བྲལ།"

msgid "Error"
msgstr "ནོར་འཁྲུལ།"

msgid "Full-text search"
msgstr "རྩོམ་ཡིག་ཆ་ཚང་བཤེར་འཚོལ།"

msgid "Please specify the search criteria."
msgstr "བཤེར་འཚོལ་དྲས་རྒྱག་གཞི་དམིགས་འཛུགས་བྱེད་རོགས།"

msgid "This command may take a while to complete, please be patient..."
msgstr ""
"བཀའ་དེ་དུས་ཚོད་ངེས་ཅན་ཞིག་གི་ནང་དུ་འགྲུབ་སྲིད་པས་ངང་རྒྱུད་རིང་པོས་སྒུག་རོགས་གནང་།"

msgid "Title search"
msgstr "ཁ་བྱང་བཤེར་འཚོལ།"

msgid "Unknown command \"%s\" "
msgstr ""

msgid "Wiki search"
msgstr "Wikiབཤེར་འཚོལ།"

msgid "You must set a (long) secret string"
msgstr ""
"ཁྱེད་ཀྱིས་ངེས་པར་དུ་གསང་བའི་ཡིག་རྟགས་ཕྲེང་བ་(རིང་བ)ཞིག་སྒྲིག་འགོད་བྱ་དགོས།"
