msgstr ""

msgid "Touchpad button orientation"
msgstr "རེག་པང་མཐེབ་གནོན་གྱི་ཁ་ཕྱོགས།"

msgid "Touchpad enabled"
msgstr "རེག་པང་སྤྱོད་འགོ་རྩོམ་པ།"

