msgid "Applications can no longer access some of your Web Accounts"
msgstr ""
"ཉེར་སྤྱོད་བྱ་རིམ་གྱིས་ཁྱེད་རང་གི་Webརྩིས་ཐོའི་ཨང་གྲངས་ཁག་གཅིག་ལ་འཚམས་འདྲི་བྱེད་མི་ཐུབ།"

msgid "Applications can no longer access your %1 Web Account"
msgstr ""
"ཉེར་སྤྱོད་བྱ་རིམ་གྱིས་ཁྱེད་རང་གི་རྩིས་ཐོའི་ཨང་གྲངས་%1 "
"ལ་འཚམས་འདྲི་བྱེད་ཐབས་མེད།"

msgid ""
"As an additional security measure, please fill in the text from the picture "
"below:"
msgstr ""
"ཚད་བརྒལ་གྱི་བདེ་སྲུང་བྱེད་ཐབས་ཤིག་ཡིན་པའི་ཆ་ནས།གཤམ་གྱི་པར་རིས།་ཁྲོད་ཀྱི་ཡིག་ས"
"ྨར་འབྲི་རོགས།"

msgid ""
"Choose <b>Web Accounts</b> from the user menu to reinstate access to this "
"account."
msgstr ""
"སྤྱོད་མཁན་གྱི་འདེམས་བྱང་ཁྲོད་ནས་<b> Web "
"རྩིས་ཐོའི་</b>བདམས་ཏེ་རྩིས་ཐོའི་ལྟ་སྤྱོད་སོར་ལོག"

msgid "Done"
msgstr "ལེགས་འགྲུབ།"

msgid "Enter your credentials"
msgstr "ཁྱེད་རང་གི་དཔང་ཡིག་ནང་འཇུག་བྱོས།"

msgid "Enter your credentials to login"
msgstr "ཁྱེད་རང་གི་དཔང་ཡིག་ནང་འཇུག་བྱས་ནས་ཐོ་འགོད་བྱེད་དགོས།"

msgid "Password:"
msgstr "གསང་ཨང་།"

msgid "Previous authentication attempt failed. Please try again."
msgstr ""
"དཔང་དངོས་བདེན་པ་ཡིན་པའི་ར་སྤྲོད་བྱེད་མ་ཐུབ། ཡང་བསྐྱར་ཚོད་ལྟ་ཞིག་བྱེད་རོགས།"

msgid "Text from the picture:"
msgstr "པར་རིས།་ཁྲོད་ཀྱི་ཡིག་དེབ།"

msgid ""
"The authentication process is complete.\n"
"You may now close this dialog and return to the application."
msgstr ""

msgid "Username:"
msgstr "སྤྱོད་མཁན་གྱི་མིང་"

msgid "Web authentication"
msgstr "དྲ་ངོས་ཀྱི་དཔང་དངོས་བདེན་པ་ཡིན་པའི་ར་སྤྲོད།"

msgid "Web authentication for %1"
msgstr "1%གི་དྲ་ངོས་དཔང་དངོས་བདེན་པ་ཡིན་པའི་ར་སྤྲོད།"
