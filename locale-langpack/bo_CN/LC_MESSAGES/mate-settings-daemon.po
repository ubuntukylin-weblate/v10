msgstr "DPI"

msgid "Deactivate"
msgstr "འགོག་སྤྱོད།"

msgid "Desktop"
msgstr "ཅོག་ངོས།"

msgid "Disable touchpad while typing"
msgstr "ནང་འཇུག་སྐབས་རེག་པང་སྤྱད་མི་ཆོག"

msgid "Disabled"
msgstr "འགོག་སྤྱོད།"

msgid "Do not touch monitor configuration"
msgstr "འཆར་ཆས་སྡེབ་སྒྲིག་ལ་བཟོ་བཅོས་རྒྱག་མི་དགོས།"

msgid "Do you want to activate Slow Keys?"
msgstr "ཁྱོད་ཀྱིས་འགོར་མཐེབ་སྐུལ་སློང་བྱེད་འདོད་དམ།"

msgid "Do you want to activate Sticky Keys?"
msgstr "ཁྱོད་ཀྱིས་འབྱར་མཐེབ་སྐུལ་སློང་བྱེད་འདོད་དམ།"

msgid "Do you want to deactivate Slow Keys?"
msgstr "ཁྱོད་ཀྱིས་འགོར་མཐེབ་སྤྱོད་འགོག་བྱེད་འདོད་དམ།"

msgid "Do you want to deactivate Sticky Keys?"
msgstr "ཁྱོད་ཀྱིས་འགོར་མཐེབ་སྤྱོད་འགོག་བྱེད་འདོད་དམ།"

msgid "Do_n't activate"
msgstr "སྐུལ་སློང་མི་བྱེད།（N）"

msgid "Do_n't deactivate"
msgstr "སྤྱོད་འགོག་མི་བྱེད།（N）"

msgid "Does the display look OK?"
msgstr "མངོན་འཆར་རྒྱུན་ལྡན་རེད་དམ།"

msgid "Don't activate"
msgstr "སྐུལ་སློང་མི་བྱེད།"

msgid "Don't become a daemon"
msgstr "ལྟ་སྲུང་གི་འཕེལ་རིམ་དུ་མི་འགྱུར་བར་བྱ་དགོས།"

msgid "Don't deactivate"
msgstr "སྤྱོད་འགོག་མི་བྱེད།"

msgid "Don't show any warnings again"
msgstr "ཉེན་བརྡ་ཅི་ཡང་མི་འཆར་བ།"

msgid "Don't show any warnings again for this file system"
msgstr "ཡིག་ཆའི་རྒྱུད་ཁོངས་འདིར་ཐ་ཚིག་ཅི་ཡང་མི་འཆར།"

