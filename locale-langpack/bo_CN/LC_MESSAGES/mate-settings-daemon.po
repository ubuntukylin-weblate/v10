msgstr ""

msgid "Eject"
msgstr "སྒོར་བུད།"

msgid "Empty Trash"
msgstr "སྙིགས་སྒམ་གཙང་སེལ།"

msgid "Empty all of the items from the trash?"
msgstr "ཕྱིར་བསྡུའི་ས་ཚིགས་ཀྱི་རྣམ་གྲངས་ཚང་མ་གཙང་སེལ་བྱེད་དམ།"

msgid "Emptying the trash"
