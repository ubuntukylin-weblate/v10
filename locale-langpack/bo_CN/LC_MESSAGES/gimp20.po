msgstr "འདེམས་ཁུལ་མཐའ་ནས་བསྐོར་བའི་སྒྲོམ།"

msgid "Selection Editor"
msgstr "འདེམས་ཁུལ་གྱི་རྩོམ་སྒྲིག་ཆས།"

msgid "Selection Mask"
msgstr "འདེམས་ཁུལ་སོག་ལེབ།"

msgid "Selection criterion"
msgstr "འདེམས་ཁུལ་གྱི་ཚད་ལྡན།"

msgid "Selection from Path"
msgstr "བརྒྱུད་ལམ་ནས་འདེམས་ཁུལ་བཟོ་བ།"

msgid "Send messages to console instead of using a dialog"
msgstr "གླེང་སྒྲོམ་ནང་མིན་པར་འཛིན་སྟེགས་སྟེང་ཉེན་བརྡ་མངོན་པ།"

msgid "Sensitivity"
msgstr "ཚོར་སྐྱེན་ཚད།"

msgid "Sensitivity for brightness component"
msgstr "གསལ་ཚད་གསེས་ཚད་ཀྱི་ཚོར་སྐྱེན་ཚད།"

msgid "Sensitivity for red/green component"
msgstr "དམར / ལྗང་གསེས་ཚད་ཀྱི་ཚོར་སྐྱེན་ཚད།"

msgid "Sensitivity for yellow/blue component"
msgstr "སེར / སྔོན་གསེས་ཚད་ཀྱི་ཚོར་སྐྱེན་ཚད།"

msgid "Set Canvas Padding Color"
msgstr "རིས་གཞིའི་ལྕིབས་སྟན་གྱི་ཁ་དོག་སྒྲིག་འགོད་བྱེད་པ།"

msgid "Set Custom Canvas Padding Color"
msgstr "རང་བཟོས་རིས་གཞིའི་ལྕིབས་སྟན་གྱི་ཁ་དོག་སྒྲིག་འགོད་བྱེད་པ།"

msgid "Set Image Canvas Size"
msgstr "བརྙན་རིས་ཀྱི་རིས་གཞིའི་ཆེ་ཆུང་སྒྲིག་འགོད་བྱེད་པ།"

msgid "Set Image Print Resolution"
msgstr "བརྙན་རིས་པར་འདེབས་ཀྱི་འབྱེད་ཕྱོད་སྒྲིག་འགོད་བྱེད་པ།"

msgid "Set Item Exclusive Linked"
msgstr "རྣམ་གྲངས་ཀྱི་སྦྲེལ་མཐུད་གཅིག་པུ་དེ་སྒྲིག་འགོད་བྱེད་པ།"

msgid "Set Item Exclusive Visible"
msgstr "རྣམ་གྲངས་ཀྱི་མཐོང་རུང་གཅིག་པུ་སྒྲིག་འགོད་བྱེད་པ།"

msgid "Set Layer Boundary Size"
msgstr "རིས་རིམ་མཐའ་མཚམས་ཀྱི་ཆེ་ཆུང་སྒྲིག་འགོད་བྱེད་པ།"

msgid "Set a source image first."
msgstr "ཁུངས་བརྙན་རིས་སྔོན་ལ་སྒྲིག་འགོད་བྱེད་པ།"

msgid "Set layer or path as active"
msgstr "རིས་རིམ་མམ་ཡང་ན་བརྒྱུད་ལམ་དེ་བྱ་བྱེད་ལ་སྒྲིག་འགོད་བྱེད་པ།"

msgid "Set name from _text"
msgstr "ཡི་གེ་ཡིས་མིང་སྒྲིག་འགོད་བྱེད་པ། (_T)"

msgid "Set text layer attribute"
msgstr "ཡི་གེའི་རིས་རིམ་གཏོགས་གཤིས་སྒྲིག་འགོད་བྱེད་པ།"

msgid "Set the default Quick Mask color"
msgstr "སོར་བཞག་གི་མགྱོགས།མྱུར་སོག་པར་ཁ་དོག་སྒྲིག་འགོད་བྱེད་པ།"

msgid ""
"Sets an upper limit to the memory that is used per image to keep operations "
"on the undo stack. Regardless of this setting, at least as many undo-levels "
"as configured can be undone."
msgstr ""

msgid "Sets how many processors GIMP should try to use simultaneously."
msgstr ""
"GIMP ཡིས་སྒྲིག་དྲས་རྒྱག་ཆས་ག་ཚོད་མཉམ་དུ་བེད་སྤྱོད་ཐུབ་པའི་སྒྲིག་འགོད་བྱེད་པ།"

msgid "Sets the browser used by the help system."
msgstr "རོགས་རམ་རྒྱུད་ཁོངས་ཀྱིས་བེད་སྤྱད་པའི་མིག་བཤར་ཆས་སྒྲིག་འགོད་བྱེད་པ།"

msgid ""
"Sets the canvas padding color used if the padding mode is set to custom "
"color."
msgstr ""
"གལ་སྲིད་ལྕིབས་སྟན་རྣམ་པ་དེ་རང་བཟོས་ཁ་དོག་ལ་སྒྲིག་འགོ་བྱས་ཚེ་རིས་གཞིས་བེད་སྤྱད་པའི་ལྕིབས་སྟན་ཁ་དོག་སྒྲིག་འགོད་བྱེད་དགོས།"

msgid "Sets the default quick mask color."
msgstr "སྒྲིག་འགོད།"

msgid "Sets the dynamics search path."
msgstr "འགུལ་རྣམ་གྱི་བཤེར་འཚོལ་བརྒྱུད་ལམ་སྒྲིག་འགོད་བྱེད་པ།"

msgid ""
"Sets the folder for temporary storage. Files will appear here during the "
"course of running GIMP.  Most files will disappear when GIMP exits, but some"
" files are likely to remain, so it is best if this folder not be one that is"
" shared by other users."
msgstr ""

msgid "Sets the handedness for cursor positioning."
msgstr "འོད་རྟགས་གནས་ངེས་ཀྱི་ལག་པ་གཡས་གཡོན་གྱི་གོམས་གཤིས་ལ་སྒྲིག་འགོད་བྱེད་པ།"

msgid ""
"Sets the level of interpolation used for scaling and other transformations."
msgstr ""
"འཇུག་ཐང་རྩིས་རྒྱག་པའི་རིམ་པར་སྒྲིག་འགོད་བྱེད་པ། "
"དེ་སྐྱེད་སྐྱུང་དང་གཞན་གྱི་བརྗེ་བསྒྱུར་བཀོལ་སྤྱོད་ལ་བེད་སྤྱོད་པ།"

msgid "Sets the manner in which transparency is displayed in images."
msgstr "བརྙན་རིས་ནང་གི་ནང་གསལ་ཆ་ཤས་ཀྱི་མངོན་ཚུལ་སྒྲིག་འགོད་བྱེད་པ།"

msgid ""
"Sets the minimal number of operations that can be undone. More undo levels "
"are kept available until the undo-size limit is reached."
msgstr ""

msgid ""
"Sets the monitor's horizontal resolution, in dots per inch.  If set to 0, "
"forces the X server to be queried for both horizontal and vertical "
"resolution information."
msgstr ""

msgid ""
"Sets the monitor's vertical resolution, in dots per inch.  If set to 0, "
"forces the X server to be queried for both horizontal and vertical "
"resolution information."
msgstr ""

msgid "Sets the pixel format to use for mouse pointers."
msgstr "ཙིག་མདའི་སྟོན་མདས་བེད་སྤྱད་པའི་བརྙན་རྒྱུའི་རྣམ་གཞག་སྒྲིག་པ།"

msgid ""
"Sets the preview size used for layers and channel previews in newly created "
"dialogs."
msgstr ""
"གསར་བཟོ་བྱས་པའི་གླེང་སྒྲོམ་ནང་གི་རིས་རིམ་དང་རྒྱུ་ལམ་གྱི་སྔོན་ལྟའི་ཆེ་ཆུང་ལ་སྒྲིག་འགོད་བྱེད་པ།"

msgid "Sets the size of the checkerboard used to display transparency."
msgstr "ནང་གསལ་གྱི་མིག་མངས་དྲ་མིག་གི་ཆེ་ཆུང་མཚོན་པར་སྒྲིག་འགོད་བྱེད་པ།"

msgid ""
"Sets the size of the navigation preview available in the lower right corner "
"of the image window."
msgstr ""
"བརྙན་རིས་སྒེའུ་ཁུང་གཡས་འོག་ཟུར་གྱི་ཕྱོགས་ཁྲིད་སྔོན་ལྟའི་ཆེ་ཆུང་སྒྲིག་འགོད་བྱེད་པ།"

msgid "Sets the size of the previews in the Undo History."
msgstr "འདོར་བའི་ལོ་རྒྱུས་སྔོན་ལྟའི་ཆེ་ཆུང་སྒྲིག་འགོད་བྱེད་པ།"

msgid "Sets the size of the thumbnail shown in the Open dialog."
msgstr "ཁ་ཕྱེ་བའི་གླེང་སྒྲོམ་ནང་གི་བསྡུས་རིས་ཀྱི་ཆེ་ཆུང་ལ་སྒྲིག་འགོད་བྱེད་པ།"

msgid ""
"Sets the swap file location. GIMP uses a tile based memory allocation "
"scheme. The swap file is used to quickly and easily swap tiles out to disk "
"and back in. Be aware that the swap file can easily get very large if GIMP "
"is used with large images. Also, things can get horribly slow if the swap "
"file is created on a folder that is mounted over NFS.  For these reasons, it"
" may be desirable to put your swap file in \"/tmp\"."
msgstr ""

msgid "Sets the text to appear in image window status bars."
msgstr ""
"བརྙན་རིས་སྒེའུ་ཁུང་གི་རྣམ་པའི་ཚང་ནང་མངོན་པའི་ཡི་གེར་སྒྲིག་འགོད་བྱེད་པ།"

msgid "Sets the text to appear in image window titles."
msgstr "བརྙན་རིས་སྒེའུ་ཁུང་ཁ་བྱང་གིས་མངོན་པའི་ཡི་གེར་སྒྲིག་འགོད་བྱེད་པ།"

msgid "Sets the type of mouse pointers to use."
msgstr "བེད་སྤྱད་པའི་ཙིག་མདའ་སྟོན་མདའི་རིགས་གྲས་སྒྲིག་པ།"

msgid ""
"Sets whether GIMP should create previews of layers and channels. Previews in"
" the layers and channels dialog are nice to have but they can slow things "
"down when working with large images."
msgstr ""

msgid "Settings saved to '%s'"
msgstr "\"སྒྲིག་འགོད་དེ “%s” ལ་ཉར་ཚགས་བྱས་ཟིན་པ།"

msgid "Shape"
msgstr "བཟོ་ལྟ།"

msgid "Shape:"
msgstr "དབྱིབས།："

msgid "Shear"
msgstr "འཕྲད་འགྱུར།"

msgid "Shear Tool: Shear the layer, selection or path"
msgstr ""
"འཕྲད་འགྱུར་གྱི་ལག་ཆ།：འཕྲད་འགྱུར་རིས་རིམ་དང་། "
"འདེམས་ཁུལ་ལམ་ཡང་ན་བརྒྱུད་ལམ་བཅས།"

msgid "Shear magnitude _X:"
msgstr "འཕྲད་འགྱུར་ཚད། _X："

msgid "Shear magnitude _Y:"
msgstr "འཕྲད་འགྱུར་ཚད།_Y："

msgid "Shearing"
msgstr "འཕྲད་འགྱུར།"

msgid "Shortcut"
msgstr "མྱུར་མཐེབ།"

msgid "Shortcut \"%s\" is already taken by \"%s\" from the \"%s\" group."
msgstr "མྱུར་མཐེབ “%1$s” དེ་ཁྱུ་ཚོ “%3$s” ཡི “%2$s” ཡིས་བཅད་བཟུང་བྱས་པ།"

msgid "Shortcut: "
msgstr "མྱུར་མཐེབ།："

msgid "Show _brush outline"
msgstr "རིས་པིར་སྤྱི་ཁོག་མངོན་པ། (_B)"

msgid "Show _foreground & background color"
msgstr "མདུན་ལྗོངས་ཁ་དོག་དང་རྒྱབ་ལྗོངས་ཁ་དོག་མངོན་པ། (_F)"

msgid "Show _guides"
msgstr "ཟུར་ལྟའི་ཐིག་མངོན་པ། (_G)"

msgid "Show _layer boundary"
msgstr "རིས་རིམ་མཐའ་མཚམས་མངོན་པ། (_L)"

msgid "Show _menubar"
msgstr "འདེམ་བྱང་སྡེ་འཆར་བ།(_M)"

msgid "Show _rulers"
msgstr "ཚད་ཁྲེ་མངོན་པ། (_R)"

msgid "Show _tooltips"
msgstr "ལག་ཆ་གསལ་བརྡ་མངོན་པ། (_T)"

msgid "Show a preview of the transformed image"
msgstr "བརྗེ་བསྒྱུར་བརྙན་རིས་ཀྱི་སྔོན་ལྟ་མངོན་པ།"

msgid "Show a tooltip when the pointer hovers over an item."
msgstr "སྟོན་མདའ་དེ་རྣམ་གྲངས་སྟེང་ལྡིང་འཁོར་བྱེད་སྐབས་ལག་ཆའི་སྣེ་སྟོན་མངོན་པ།"

msgid "Show active _brush, pattern & gradient"
msgstr "བྱ་བྱེད་རིས་པིར་དང་། དཔེ་རིས་དང་རིམ་འགྱུར་བཅས་མངོནཔ། (_B)"

msgid "Show active _image"
msgstr "བྱ་བྱེད་བརྙན་རིས་མངོན་པ། (_I)"

msgid "Show gri_d"
msgstr "དྲ་མིག་མངོན་པ། (_D)"

msgid "Show help _buttons"
msgstr "རོགས་རམ་མཐེབ་གནོན་མངོན་པ། (_B)"

msgid "Show image preview"
msgstr "བརྙན་རིས་སྔོན་ལྟ་མངོན་པ།"

msgid "Show image size"
msgstr "བརྙན་རིས་ཆེ་ཆུང་མངོན་པ།"

msgid "Show license information and exit"
