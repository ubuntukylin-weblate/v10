msgid "Kylin file protection box"
msgstr "ཆི་ལིན་ཡིག་ཆ་སྲུང་སྐྱོང་སྒམ།"

msgid "about"
msgstr "འབྲེལ་ཡོད།"

msgid "Operating environment: kylin (V10)"
msgstr "འཁོར་སྐྱོད་ཁོར་ཡུག་ནི་དགུ་ཚིགས་ཆི་ལིན་བཀོལ་སྤྱོད་མ་ལག (V10)་ཡིན།"

msgid "Secure kernel (3.0.0) and above"
msgstr "བདེ་འཇགས་ནང་ཉིང (3.0.0)ཡན"

msgid "Protective box kit (3.0.8) and above"
msgstr "སྲུང་སྐྱོབ་སྒམ་ཕྲེང་(3.0.8) ཡན"

msgid "Software version: %1"
msgstr "མཉེན་ཆས་པང་གཞི: %1"

msgid "All"
msgstr "མཉེན་ཆས་ཚང་མ།"

msgid "Yes"
msgstr "ཡིན།"

msgid "No"
msgstr "མིན།"

msgid "Personal creation"
msgstr "སྒེར་གྱིས་འཛུགས་པ"

msgid "Shared by others"
msgstr "གཞན་ལ་མཉམ་སྤྱོད།"

msgid "%1 Shared"
msgstr "%1མཉམ་སྤྱོད"

msgid "Created by myself"
msgstr "རང་གིས་འཛུགས་པ།"

msgid "File protection box"
msgstr "ཡིག་ཆ་སྲུང་སྐྱོབ་སྒམ"

msgid ""
"Provide data security protection, support isolation hiding, encryption "
"protection and sharing of personal data"
msgstr ""
"གཞི་གྲངས་བདེ་འཇགས་སྲུང་སྐྱོབ་མཁོ་འདོན་བྱས་ནས་རང་གི་གཞི་གྲངས་ཀྱི་ལོགས་བཀར་སྦས་"
"སྐུང་དང་། གསང་རྒྱ་སྲུང་སྐྱོབ། མཉམ་སྤྱོད་ནུས་པར་སྲུང་སྐྱོབ་བྱེད།"

msgid "All protective boxes"
msgstr "ཡིག་ཆ་སྒ་ཚང་མ།"

msgid "New"
msgstr "གསར་འཛུགས།"

msgid "Delete"
msgstr "བསུབ་པ།"

msgid "Password setting"
msgstr "གསང་གྲངས་སྒྲིག་འགོད།"

msgid "Sharing settings"
msgstr "མཉམ་སྤྱོད་སྒྲིག་འགོད།"

msgid "Millet porridge: Kylin"
msgstr "འབྲས་སེར:ཆི་ལིན"

msgid "Shared"
msgstr "མཉམ་སྤྱོད་བྱས་ཟིན་པ།"

msgid "Encrypted"
msgstr "གསང་བསྡམས།"

msgid "Please select the protective box to be deleted! "
msgstr "ཁྱེད་ཀྱིས་བསུབ་དགོས་པའི་ཉེན་སྲུང་སྒམ་འདེམས་རོགས།! "

msgid "Unable to delete, please unmount the protective box first \"%1\" !"
msgstr "བསུམ་མི་ཐུབ། ཁྱེད་ཀྱིས་སྔོན་སྲུང་སྐྱོབ་སྒམ \"%1\"་ལྷུ་གཏོང་གནང་རོགས།!"

msgid ""
"The protection box is encrypted, please enter the password for "
"authentication"
msgstr "སྲུང་སྐྱོབ་སྒམ་ལ་གསང་རྒྱ་བཏབ། ཁྱེད་ཀྱིས་གསང་གྲངས་དཔང་སྤྲོད་གནང་རོགས།"

msgid ""
"The delete operation is irreversible, are you sure to delete the protective "
"box \"%1\" ?"
msgstr "བསུབ་པ་ཕྱིར་ལྡོག་མེད། སྲུང་སྐྱོབ་སྒམ་\"%1\" བསུབ་ཁོ་ཐག་ཡིན་ནམ?"

msgid "Protective box \"%1\" failed to delete: %2"
msgstr "སྲུང་སྐྱོབ་སྒམ\"%1\" བསུབ་མ་ཐུབ།"

msgid "Please select the protective box that requires password setting !"
msgstr "ཁྱེད་ཀྱིས་གསང་གྲངས་འགོད་དགོས་པའི་སྲུང་སྐྱོབ་སྒམ་འདེམས་རོགས!"

msgid "Please select the protective box that requires password setting ! "
msgstr "ཁྱེད་ཀྱིས་གསང་གྲངས་འགོད་དགོས་པའི་སྲུང་སྐྱོབ་སྒམ་འདེམས་རོགས! "

msgid ""
"Unable to set the password, please unmount the protection box first \"%1\" !"
msgstr "གསང་གྲངས་འཇོག་མི་ཐུབ་པས། སྔོན་ལ་སྲུང་སྐྱོབ་སྒམ\"%1\"ལྷུ་གཏོང་གནང་རོགས !"

msgid "Please select the protective box that needs to be shared! "
msgstr "ཁྱེད་ཀྱིས་མཉམ་སྤྱོད་དགོས་པའི་སྲུང་སྐྱོབ་སྒམ་འདེམས་རོགས! "

msgid "Unable to set up sharing, please unmount the protection box first \"%1\" !"
msgstr ""
"མཉམ་སྤྱོད་བྱ་ཐབས་མེད་པས་སྔོན་ལ་སྲུང་སྐྱོབ་སྒམ་ \"%1\" ལྷུ་གཏོར་གནང་རོགས!"

msgid "Failed to load shared user list! "
msgstr "མཉམ་སྤྱོད་བཀོལ་མཁན་སྡེ་སྣོན་འཇུག་བྱ་མ་ཐུབ! "

msgid "Protective box: %1 (from %2)"
msgstr "སྲུང་སྐྱོབ་སྒམ: %1 (ཡོང་ས %2)"

msgid "Protective box: %1"
msgstr "སྲུང་སྐྱོབ་སྒམ: %1"

msgid "Mounted"
msgstr "འགེལ་འཇུག་བྱས།"

msgid "Protective box: %1 item"
msgstr "སྲུང་སྐྱོབ་སྒམ: %1སྡེ"

msgid "Please select the protective box to be mounted! "
msgstr "ཁྱེད་ཀྱིས་འགེལ་འཇུག་གི་སྲུང་སྐྱོབ་སྒམ་འདེམས་རོགས! "

msgid "Please select the protective box to be Unmount! "
msgstr "ཁྱེད་ཀྱིས་ལྷུ་གཏོར་དགོས་པའི་སྲུང་སྐྱོབ་སྒམ་འདེམས་རོགས! "

msgid "Protective box \"%1\" Unmount failed: %2"
msgstr "སྲུང་སྐྱོབ་སྒམ \"%1\"ལྷུ་གཏོང་མ་ཐུབ: %2"

msgid "Please select the protective box that needs to be renamed! "
msgstr "ཁྱེད་ཀྱིས་མིང་འདོགས་དགོས་པའི་སྲུང་སྐྱོབ་སྒམ་འདེམས་རོགས། "

msgid "Unable to rename operation, please unmount protective box first \"%1\" !"
msgstr ""
"ཡང་བསྐྱར་མིང་འདོགས་མི་ཐུབ་པས་སྔོན་ལ་སྲུང་སྐྱོབ་སྒམ་\"%1\"ལྷུ་གཏོར་གནང་རོགས !"

msgid ""
"The current system does not support kylin file protection box related "
"functions"
msgstr ""
"མིག་སྔའི་མ་ལག་གིས་ཆི་ལིན་ཡིག་ཆ་སྲུང་སྐྱོབ་སྒམ་གྱི་འབྲེལ་ཡོད་ནུས་པ་རྒྱབ་སྐྱོར་"
"མི་བྱེད།"

msgid "Invalid username"
msgstr "ནུས་མེད་བཀོལ་མཁན་མིང་།"

msgid "Username is too long"
msgstr "བཀོལ་མཁན་མིང་རིང་དྲགས།"

msgid "Kylin file protection box system environment is not initialized"
msgstr "ཆི་ལིན་ཡིག་ཆ་སྲུང་སྐྱོབ་སྒམ་མ་ལག་གི་ཁོར་ཡུག་འགོ་བཙུགས་མེད།"

msgid "The system configuration is corrupted"
msgstr "མ་ལག་སྒྲིག་བཀོད་བརླག་སོང་།"

msgid "System configuration failed"
msgstr "མ་ལག་སྒྲིག་བཀོད་མ་བྱུང་།"

msgid "The protection box configuration is damaged"
msgstr "སྲུང་སྐྱོབ་སྒམ་སྒྲིག་བཀོད་བརླག་ཟིན།"

msgid "Password length exceeds limit"
msgstr "གསང་གྲངས་ཀྱི་རིང་ཐུང་ཚད་ལས་བརྒལ།"

msgid "The current system does not support encryption"
msgstr "མིག་སྔའི་མ་ལག་གིས་གསང་རྒྱ་ནུས་པ་འདོན་མི་ཐུབ།"

msgid "The protective box is mounted"
msgstr "སྲུང་སྐྱོབ་སྒམ་འགེལ་འཇུག་བྱས་ཚར།"

msgid "The protective box is decrypted"
msgstr "སྲུང་སྐྱོབ་སྒམ་གསར་གཏོར་བྱས་ཚར།"

msgid "Failed to delete protective box"
msgstr "སྲུང་སྐྱོབ་སྒམ་བསུབ་མ་ཐུབ།"

msgid "Failed to mount the protection box"
msgstr "སྲུང་སྐྱོབ་སྒམ་འགེལ་འཇུག་མ་ཐུབ།"

msgid "Data restore failed"
msgstr "གཞི་གྲངས་སོར་ཆུད་མ་ཐུབ།"

msgid "Protection box unmount failed"
msgstr "སྲུང་སྐྱོབ་སྒམ་འགེལ་འཇུག་མ་ཐུབ།"

msgid "The protective box is not mounted"
msgstr "སྲུང་སྐྱོབ་སྒམ་འགེལ་འཇུག་མ་ཐུབ།"

msgid "The protection box is not encrypted"
msgstr "སྲུང་སྐྱོབ་སྒམ་གསང་གྲངས་མེད།"

msgid "Wrong password"
msgstr "གསང་ཨང་ནོར་འཁྲུལ།"

msgid "Invalid protection box name"
msgstr "སྲུང་སྐྱོབ་སྒམ་གྱི་མིང་ནུས་མེད་ཡིན།"

msgid "Unknown mistake"
msgstr "མ་ཤེས་པའི་ནོར་འཁྲུལ།"

msgid "name"
msgstr "མིང་།"

msgid "Whether to encrypt"
msgstr "གསང་གྲངས་འཇོག་མིན།"

msgid "Whether to share"
msgstr "མཉམ་སྤྱོད་བྱ་མིན།"

msgid "From"
msgstr "ཡོང་ས་ནི།"

msgid "           name"
msgstr "           མིང་།"

msgid "  Whether to encrypt"
msgstr "  གསང་གྲངས་འཇོག་མིན"

msgid "  Whether to share"
msgstr "  མཉམ་སྤྱོད་བྱ་མིན།"

msgid "  Owner"
msgstr "  བདག་འཛིན་མཁན།"

msgid "Change password settings"
msgstr "གསང་གྲངས་བརྗེ་བ།"

msgid "Change method: "
msgstr "བརྗེ་བའི་རྣམ་པ་ནི: "

msgid "Change Password"
msgstr "གསང་གྲངས་བཟོ་བཅོས།"

msgid "Unprotect"
msgstr "སྲུང་སྐྱོབ་མེད།"

msgid "Current password"
msgstr "མིག་སྔའི་གསང་གྲངས།"

msgid "New password"
msgstr "གསང་གྲངས་གསར་པ།"

msgid "New key confirmation"
msgstr "གསང་གྲངས་གསར་པ་གཏན་འཁེལ།"

msgid "Confirm"
msgstr "གཏན་འཁེལ།"

msgid "Cancel"
msgstr "འདོེར་བ།"

msgid ""
"The protection box whose password needs to be changed is not selected, "
"please select and modify again!  "
msgstr ""
"གསང་གྲངས་བཅོས་དགོས་པའི་སྲུང་སྐྱོབ་སྒམ་འདེམས་མེད་པས། "
"གདམས་ནས་ཡང་བསྐྱར་བཟོ་བཅོས་གནང་རོགས!  "

msgid "The original password cannot be empty"
msgstr "གདོད་མའི་གསང་གྲངས་སྟོང་པ་ཡིན་མི་རུང་།"

msgid "New password cannot be empty"
msgstr "གསང་གྲངས་གསར་པ་སྟོང་པ་ཡིན་མི་རུང་།"

msgid "New password confirmation cannot be empty"
msgstr "གསང་གྲངས་གསར་པ་སྟོང་པ་མིན་པ་གཏན་འཁེལ།"

msgid "The new password does not match the confirmed password"
msgstr "གསང་གྲངས་གསར་པ་དང་གཏན་འཁེལ་གྱི་གསང་གྲངས་གཅིག་མཐུན་མིན།"

msgid "Protective box \"%1\" Password modification failed: %2"
msgstr "སྲུང་སྐྱོབ་སྒམ \"%1\" གསང་གྲངས་བཅོས་མ་ཐུབ: %2"

msgid "Confirm password cannot be empty"
msgstr "གསང་གྲངས་སྟོང་པ་མིན་པ་གཏན་འཁེལ།"

msgid "Protective box \"%1\" Password unlock failed: %2"
msgstr "སྲུང་སྐྱོབ་སྒམ \"%1\" གསང་གྲངས་གཏོར་མ་ཐུབ: %2"

msgid "Create a protective box"
msgstr "སྲུང་སྐྱོབ་སྒམ་གསར་འཛུགས།"

msgid "Path"
