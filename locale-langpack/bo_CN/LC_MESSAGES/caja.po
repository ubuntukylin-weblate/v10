msgid ""
"The folder \"%B\" cannot be copied because you do not have permissions to "
"read it."
msgstr "ཡིག་ཁུག“%B”འདྲ་བཟོ་མི་ཐུབ། ཁྱེད་ལ་དེ་ཀློག་པའི་དབང་ཚད་མེད་པའི་རྐྱེན།"

msgid ""
"The folder \"%B\" cannot be deleted because you do not have permissions to "
"read it."
msgstr "ཡིག་ཁུག“%B”འདོར་མི་ཐུབ། ཁྱེད་ལ་དེ་ཀློག་པའི་དབང་ཚད་མེད་པའི་རྐྱེན།"

msgid ""
"The folder \"%B\" cannot be handled because you do not have permissions to "
"read it."
msgstr ""
"ཡིག་ཁུག“%B”སྒྲིག་གཅོད་བྱ་མི་ཐུབ། ཁྱེད་ལ་དེ་ཀློག་པའི་དབང་ཚད་མེད་པའི་རྐྱེན།"

msgid "The folder \"%s\" cannot be opened on \"%s\"."
msgstr "ཡིག་ཁུག“%s”དེ“%s”རུ་ཁ་འབྱེད་མི་ཐུབ།"

msgid "The folder contents could not be displayed."
msgstr "ཡིག་ཁུག་ནང་དོན་མངོན་མི་ཐུབ།"

msgid "The font description used for the icons on the desktop."
msgstr "ཅོག་ངོས་སུ་རིས་རྟགས་ཞིབ་བརྗོད་སྤྱོད་པའི་ཡིག་གཟུགས།"

msgid ""
"The format of file dates. Possible values are \"locale\", \"iso\", and "
"\"informal\"."
msgstr ""
"ཡིག་ཆའི་དུས་ཀྱི་རྣམ་གཞག་འདེམས་རུང་གི་ཚད་ནི་“locale”དང་།“iso”།“informal”ཡིན།"

msgid "The geometry string for a navigation window."
msgstr "ཕྱོགས་ཁྲིད་སྒེའུ་ཁུང་གི་དབྱིབས་ཀྱི་ཡིག་རྟགས་ཕྲེང་།"

msgid "The group could not be changed."
msgstr "ཚོ་པ་བཅོས་མི་ཐུབ།"

msgid "The group of the file."
msgstr "ཡིག་ཆའི་ཚོ་ཁག"

msgid "The history location doesn't exist."
msgstr "ལོ་རྒྱུས་ཀྱི་གནས་ས་མེད་པ།"

msgid "The icon view encountered an error while starting up."
msgstr "རིས་རྟགས་མཐོང་རིས་མགོ་སློང་སྐབས་སུ་ནོར་འཁྲུལ་བྱུང་།"

msgid "The icon view encountered an error."
msgstr "རིས་རྟགས་མཐོང་རིས་ནོར་འཁྲུལ་བྱུང་།"

msgid "The item cannot be restored from trash"
msgstr "རྣམ་གྲངས་སྙིགས་སྒམ་ལས་སླར་གསོ་མི་ཐུབ།"

msgid "The item could not be renamed."
msgstr "རྣམ་གྲངས་མིང་བསྐྱར་འདོགས་བྱ་མི་ཐུབ།"

msgid "The list view encountered an error while starting up."
msgstr "རེའུ་མིག་མཐོང་རིས་མགོ་སློང་སྐབས་སུ་ནོར་འཁྲུལ་ལ་འཕྲད།"

msgid "The list view encountered an error."
msgstr "རེའུ་མིག་མཐོང་རིས་ནོར་འཁྲུལ་ལ་འཕྲད།"

msgid "The location \"%s\" does not exist."
msgstr "གནས་ས“%s”མེད་པ།"

msgid "The location cannot be displayed with this viewer."
msgstr "གནས་ས་ལྟ་ཆས་ཀྱིས་མངོན་མི་ཐུབ།"

msgid "The location is not a folder."
msgstr "གནས་ས་ནི་ཡིག་ཁུག་མིན།"

msgid "The location of the file."
msgstr "ཡིག་ཆའི་གནས་ས།"

msgid "The media contains digital photos."
msgstr "བར་རྫས་སུ་ཨང་ཀིའི་འདྲ་པར་འདུས།"

msgid "The media contains software."
msgstr "བར་རྫས་ལ་མཉེན་ཆས་ཚུད། "

msgid "The media has been detected as \"%s\"."
msgstr "བར་རྫས་“%s”ལ་ཞིབ་དཔྱད་བྱེད།"

msgid "The mime type of the file."
msgstr "ཡིག་ཆའི MIME རིགས་གྲས།"

msgid ""
"The name \"%s\" is already used in this folder. Please use a different name."
msgstr "ཡིག་ཁུག་འདིའི་ནང་དུ་%sཞེས་པའི་མིང་བཀོལ་ཟིན། མིང་གཞན་ཞིག་སྤྱོད་རོགས། "

msgid ""
"The name \"%s\" is not valid because it contains the special character "
"\"\\/:*?\"<>\". Please use a different name."
msgstr ""
"མིང་“%s”ནུས་མེད་ཡིན། རྒྱུ་མཚན་ནི་དེའི་ནང་དུ་དམིགས་བསལ་མཚོན་རྟགས་“\\ / : * ? "
"\" < > |”འདུས། ཁྱེད་ཀྱིས་མིང་གཞན་བཀོལ་སྤྱོད་བྱོས།"

msgid "The name \"%s\" is not valid. Please use a different name."
msgstr "མིང་“%s”ནུས་མེད། མིང་གཞན་སྤྱོད་རོགས།"

msgid "The name and icon of the file."
msgstr "ཡིག་ཆའི་མིང་དང་རིས་རྟགས།"

msgid "The owner could not be changed."
msgstr "བདག་པོ་བསྒྱུར་མི་ཐུབ། "

msgid "The owner of the file."
msgstr "ཡིག་ཆའི་བདག་པོ། "

msgid "The path is empty"
msgstr "འགྲོ་ལམ་སྟོང་པ་རེད།"

msgid "The permissions could not be changed."
msgstr "དབང་ཆ་བཅོས་མི་ཐུབ། "

msgid "The permissions of \"%s\" could not be determined."
msgstr "%sཡི་དབང་ཚད་གཏན་འཁེལ་བྱེད་མི་ཐུབ། "

msgid "The permissions of the file, in octal notation."
msgstr "ཡིག་ཆའི་དབང་ཚད་བརྒྱད་གོང་འགྲིལ་ལུགས་ཀྱིས་མངོན།"

msgid "The permissions of the file."
msgstr "ཡིག་ཆའི་དབང་ཚད། "

msgid "The permissions of the selected file could not be determined."
msgstr "བདམས་པའི་ཡིག་ཆའི་དབང་ཚད་གཏན་མི་འཁེལ།"

msgid ""
"The position of the opposite end of the selection from the cursor in chars."
msgstr "གདམ་ཁུལ་སྣེ་གཞན་གྱི་གནས་ས་འོད་རྟགས་ནས་ཡིག་རྟགས་ལྟར་གྲངས་བགྲང་།"

msgid "The selection rectangle"
msgstr "གྲུ་བཞི་ནར་མོའི་འདེམས་ཁུལ།"

msgid "The server at \"%s\" cannot be found."
msgstr "“%s”རུ་གནས་པའི་ཞབས་ཞུ་ཆས་མི་རྙེད།"

msgid "The side pane view to show in newly opened windows."
msgstr "ཁ་གསར་བར་ཕྱེ་བའི་སྒེའུ་ཁུང་དུ་མཐའ་ཟུར་སྡེའི་མཐོང་རིས་མངོན།"

msgid "The size of the file."
msgstr "ཡིག་ཆའི་ཆེ་ཆུང་། "

msgid ""
"The software will run directly from the medium \"%s\". You should never run software that you don't trust.\n"
"\n"
"If in doubt, press Cancel."
msgstr ""
"མཉེན་ཆས་ཐད་ཀར་བར་རྫས“%s”ཁྲོད་འཁོར་སྐྱོད་བྱེད་སྲིད།\n"
"\n"
"ཁྱོད་ཀྱིས་ཡིད་ཆེས་མེད་པའི་མཉེན་ཆས་འཁོར་སྐྱོད་མ་བྱེད། "
"གལ་ཏེ་ཁྱེད་ལ་དོགས་པ་ཡོད་ན། འདོར་བ་སྣོན་རོགས།"

msgid "The source file would be overwritten by the destination."
msgstr "ཁུངས་ཡིག་ཆ་དམིགས་ཡུལ་གྱིས་ཁེབས་སྲིད།"

msgid "The target doesn't support symbolic links."
msgstr "འདྲུད་པའི་དམིགས་ཡུལ་གྱིས་མཚོན་རྟགས་སྦྲེལ་མཐུད་རྒྱབ་སྐྱོར་མི་བྱེད།"

msgid "The text of the label."
msgstr "གདོང་འཛར་སྟེང་གི་ཡི་གེ"

msgid "The type of the file."
msgstr "ཡིག་ཆའི་རིགས། "

msgid "There is %S available, but %S is required."
msgstr "མིག་སྔར་S%ལས་མེད་མོད།འོན་ཀྱང་S%དགོས། "

msgid "There is no \"%s\" in this folder. Perhaps it was just moved or deleted?"
msgstr "ཡིག་ཁུག་འདིར་“%s”མེད། དེ་ད་སྤོ་བའམ་འདོར་སྲིད།"

msgid "There is no application installed for %s files"
msgstr "%s ཡིག་ཆ་རིགས་གྲས་ལ་ཉེར་སྤྱོད་བྱ་རིམ་སྒྲིག་འཇུག་བྱེད་མེད།"

msgid ""
"There is no application installed for %s files.\n"
"Do you want to search for an application to open this file?"
msgstr ""
"ཡིག་ཆའི་རིགས %sལ་ཉེར་སྤྱོད་བྱ་རིམ་ནང་འཇུག་བྱས་མེད།\n"
"ཁྱོད་ཀྱིས་ཡིག་ཆ་འདི་ཁ་འབྱེད་པའི་ཉེར་སྤྱོད་བྱ་རིམ་འཚོལ་དགོས་སམ།"

msgid "There is no files which you want"
msgstr "འཚོལ་བཤེར་ཆ་རྐྱེན་དང་འཚམ་པའི་ཚན་པ་མེད།"

msgid ""
"There is not enough space on the destination. Try to remove files to make "
"space."
msgstr ""
"དམིགས་ཚད་ཀྱི་གནས་སར་བར་སྟོང་འདང་ངེས་ཤིག་མེད། "
"ཁྱོད་ཀྱིས་ཡིག་ཆ་འགའ་བསུབ་ནས་བར་སྟོང་ལྷོད་པར་་ཚོད་ལྟ་བྱེད་རོགས། "

msgid "There is nothing on the clipboard to paste."
msgstr "དྲས་པང་ནང་དུ་སྦྱར་ཆོག་པའི་དངོས་པོ་མེད། "

msgid "There was an error copying the file into %F."
msgstr "ཡིག་ཆ་ %Fལ་འདྲ་བཟོ་བྱེད་སྐབས་ནོར་བྱུང་།"

msgid "There was an error creating the directory in %F."
msgstr " %Fནང་དུ་དཀར་ཆག་གསར་བཟོ་བྱེད་སྐབས་ནོར་བྱུང་།"

msgid "There was an error creating the folder \"%B\"."
msgstr "ཡིག་ཁུག“%B”གསར་བཟོ་བྱེད་སྐབས་ནོར་བྱུང་།"

msgid "There was an error creating the symlink in %F."
msgstr " %Fནང་དུ་མཚོན་རྟགས་འབྲེལ་བ་གསར་བཟོ་བྱེད་དུས་ནོར་བྱུང་།"

msgid "There was an error deleting %B."
msgstr " %B འདོར་སྐབས་ནོར་འཁྲུལ་བྱུང་།"

msgid "There was an error displaying help."
msgstr "རོགས་རམ་བྱེད་སྐབས་ཀྱི་ནོར་འཁྲུལ་འཆར་བ།"

msgid ""
"There was an error displaying help: \n"
"%s"
msgstr ""
"རོགས་རམ་འཆར་སྐབས་ནོར་འཁྲུལ་བྱུང་།\n"
"%s"

msgid "There was an error getting information about \"%B\"."
msgstr "“%B”སྐོར་གྱི་ཆ་འཕྲིན་ཐོབ་སྐབས་སུ་ནོར་འཁྲུལ་བྱུང་།"

msgid "There was an error getting information about the destination."
msgstr "དམིགས་ཡུལ་སྐོར་གྱི་ཆ་འཕྲིན་ཐོབ་སྐབས་སུ་ནོར་འཁྲུལ་བྱུང་།"

msgid ""
"There was an error getting information about the files in the folder \"%B\"."
msgstr "ཡིག་ཁུག“%B”ནང་གི་ཡིག་ཆའི་ཆ་འཕྲིན་ཐོབ་སྐབས་སུ་ནོར་འཁྲུལ་བྱུང་།"

msgid "There was an error launching the application."
msgstr "ཉེར་སྤྱོད་བྱ་རིམ་མགོ་སློང་སྐབས་ནོར་འཁྲུལ་བྱུང་།"

msgid "There was an error moving the file into %F."
msgstr "ཡིག་ཆ་ %Fལ་སྤོ་སྐབས་ནོར་འཁྲུལ་བྱུང་།"

msgid "There was an error reading the folder \"%B\"."
msgstr "ཡིག་ཁུག“%B”ཀློག་སྐབས་ནོར་འཁྲུལ་བྱུང་།"

msgid "There was an internal error trying to search for applications:"
msgstr ""
"ཉེར་སྤྱོད་བྱ་རིམ་བཤེར་འཚོལ་ཚོད་ལྟ་བྱེད་སྐབས་ནང་ཁུལ་གྱི་ནོར་འཁྲུལ་བྱུང་།："

msgid "These files are on a Photo CD."
