msgid "Add input method"
msgstr "ནང་འཇུག་ཐབས་སྣོན་པ།"

msgid "Addon"
msgstr "སྒྲིག་ལྷུ་ཟུར་སྣོན།"

msgid "Advance"
msgstr "མཐོ་རིམ།"

msgid "Appearance"
msgstr "ཕྱི་ཚུལ།"

msgid "Available Input Method"
msgstr "སྤྱོད་རུང་གི་ནང་འཇུག་ཐབས།"

msgid "Cannot load currently used user interface info"
msgstr "མིག་སྔའི་སྤྱོད་མཁན་འཆར་ངོས་ཀྱི་ཆ་འཕྲིན་འཇུག་མི་ཐུབ།"

msgid "Clear font setting"
msgstr "ཡིག་གཟུགས་ཀྱི་སྒྲིག་འགོད་སེལ་བ།"

msgid "Configure"
msgstr " སྒྲིག་འགོད།"

msgid "Current Input Method"
msgstr "མིག་སྔའི་ནང་འཇུག་ཐབས།"

msgid "Default"
msgstr "སོར་བཞག"

msgid "Default keyboard layout"
msgstr "སོར་བཞག་གི་མཐེབ་གཞོང་བཀོད་པ།"

msgid "Didn't install related component."
msgstr "སྒྲིག་འཇུག་མེད་པའི་འབྲེལ་ཡོད་སྒྲིག་ལྷུ།"

msgid "Empty"
msgstr "སྟོང་པ།"

msgid "Global Config"
msgstr "ཁྱོན་ཡོངས་སྡེབ་སྒྲིག"

msgid "Input Method"
msgstr "འཇུག་ཐབས།"

msgid "Input Method Configuration"
msgstr "ནང་འཇུག་ཐབས་སྡེབ་སྒྲིག"

msgid "Input Method Default"
msgstr "ནང་འཇུག་ཐབས་སོར་བཞག"

msgid "Input method settings:"
msgstr "ནང་འཇུག་ཐབས་སྒྲིག་འགོད།："

msgid "Keyboard layout to use when no input window:"
msgstr "ནང་འཇུག་སྒེའུ་ཁུང་མེད་སྐབས་སྤྱོད་པའི་མཐེབ་གཞོང་བཀོད་པ།:"

msgid "Keyboard layout:"
msgstr "མཐེབ་གཞོང་བཀོད་པ།："

msgid "Language"
msgstr "སྐད་བརྡ།"

msgid "No configuration option for %s."
msgstr "%s སྡེབ་སྒྲིག་འདེམས་ཚན་མེད།"

msgid "Only Show Current Language"
msgstr "མིག་སྔའི་སྐད་བརྡ་ཁོ་ན་མངོན།"

msgid "Other"
msgstr "གཞན་དག"

msgid "Please press the new key combination"
msgstr "མཐེབ་གནོན་འདུས་སྒྲིག་གསར་པ་གནོན་རོགས།"

msgid "Search Addon"
msgstr "ཟུར་སྣོན་སྒྲིག་ལྷུ་འཚོལ་བཤེར།"

msgid "Search Input Method"
msgstr "ནང་འཇུག་ཐབས་འཚོལ་བཤེར།"

msgid "Show Advance Option"
msgstr "མཐོ་རིམ་འདེམས་ཚན་མངོན་པ།"

msgid ""
"The first input method will be inactive state. Usually you need to put "
"<b>Keyboard</b> or <b>Keyboard - <i>layout name</i></b> in the first place."
msgstr ""

msgid "Unknown"
msgstr "མི་ཤེས།"

msgid "Unspecified"
msgstr "དམིགས་འཛུགས་བྱས་མེད་པ།"

msgid "_Cancel"
msgstr " དོར་བ།"

msgid "_OK"
msgstr "གཏན་འཁེལ།"
