msgid "\"%s\" is not a valid value for focus attribute"
msgstr "%sནི་focusངོ་བོའི་ནུས་ལྡན་གྱི་ཐང་མིན།"

msgid "\"%s\" is not a valid value for resize attribute"
msgstr "%sནི་resizeངོ་བོའི་ནུས་ལྡན་གྱི་གྲངས་ཐང་མིན།"

msgid "\"%s\" is not a valid value for state attribute"
msgstr "s%ནི་Sateངོ་བོའི་ནུས་ལྡན་གྱི་གྲངས་ཐང་མིན།"

msgid ""
"\"version\" attribute cannot be used in metacity-theme-1.xml or metacity-"
"theme-2.xml"
msgstr ""
"versionགཏོགས་གཤིས་དེ་metacity-theme-1.xmlདང་metacity-"
"theme-2.xmlལ་སྤྱོད་མི་རུང་།"

msgid "%d x %d"
msgstr ""

msgid "%s %s"
msgstr "\"%s %s\""

msgid "%s (on %s)"
msgstr "\"%s། ( %sལ)\""

msgid "<%s> geometry \"%s\" has not been defined"
msgstr "\"<%s> དབྱིབས་རྩིས་བཀོད་པ“%s”ད་དུང་མཚན་ཉིད་བཞག་མེད།\""

msgid "<%s> must specify either a geometry or a parent that has a geometry"
msgstr ""
"<%s>ངེས་པར་དུ་དབྱིབས་རྩིས་ཀྱི་བཀོད་པ་ཞིག་གམ་དབྱིབས་རྩིས་ཀྱི་བཀོད་པ་ཡོད་པའི་ཕ་རིམ་ཞིག་དམིགས་འཛུགས་བྱེད་དགོས།"

msgid "<%s> name \"%s\" used a second time"
msgstr "ཡང་བསྐྱར་<%s> མིང་གི་%sབེད་སྤྱོད་བྱས་པ།"

msgid "<%s> parent \"%s\" has not been defined"
msgstr "<%s> ཕ་རིམ་པའི%sལ་ད་དུང་མཚན་ཉིད་བཞག་མེད།"

msgid "<%s> specified twice for this theme"
msgstr "དེར་བརྟེན་བརྗོད་དོན་གཙོ་བོས་<%s>ཐེངས་གཉིས་ལ་དམིགས་འཛུགས་བྱས།"

msgid ""
"<button function=\"%s\" state=\"%s\" draw_ops=\"whatever\"/> must be "
"specified for this frame style"
msgstr ""

msgid "A style called \"%s\" has not been defined"
msgstr "“%s”ཉམས་འགྱུར་ལ་ད་དུང་མཚན་ཉིད་བཞག་མེད་པ།"

msgid "Activate the window menu"
msgstr "སྒེའུ་ཁུང་འདེམས་བྱང་གྲུང་སློང་།"

msgid "Alpha must be between 0.0 (invisible) and 1.0 (fully opaque), was %g\n"
msgstr ""
"Alhaཐང་ནི་ངེས་པར་དུ་0.0（མཐོང་ཐབས་བྲལ་པ）ནས་1.0（ཕྱི་གསལ་ནང་གསལ་གཏན་ནས་མིན་པ）བར་དང་།ད་ལྟ་ནི་%gཡིན།\n"

msgid "Alpha value \"%s\" in blended color is not between 0.0 and 1.0"
msgstr "མདོག་འདྲེས་མའིAlPaཐང་གི“%s”ནི་0.0ནས་1.0བར་མེད།"

msgid "Angle must be between 0.0 and 360.0, was %g\n"
msgstr "ཟུར་ཚད་ཐང་ངེས་པར་དུ་0.0ནས་360.0བར་ཡིན་དགོས་པ་དང་།ད་ལྟ་ནི་ %gཡིན།\n"

msgid ""
"Another compositing manager is already running on screen %i on display "
"\"%s\"."
msgstr ""
"DISPLAY %2$s ཡི་བརྙན་ཡོལ་%1$i "
"ཐོག་ཏུ་སྒེའུ་ཁུང་གི་དོ་དམ་ཆས་གཞན་པ་ཞིག་འཁོར་སྐྱོད་བྱེད་བཞིན་འདུག"

msgid "Application is not responding."
msgstr "ཉེར་སྤྱོད་བྱ་རིམ་དང་ལེན་མ་བྱས།"

msgid "Aspect ratio \"%s\" is unknown"
msgstr "ཞེང་ཚད་བསྡུར་“%s”ངེས་མེད།"

msgid "Attach modal dialogs"
msgstr "གཞན་བརྟེན་དཔེ་རྣམ་གླེང་སྒྲོམ།"

msgid "Attribute \"%s\" is invalid on <%s> element in this context"
msgstr ""

msgid "Attribute \"%s\" repeated twice on the same <%s> element"
msgstr ""

msgid "Auto maximize nearly monitor sized windows"
msgstr "འཆར་ཆས་ཆེ་ཆུང་གི་སྒེའུ་ཁུང་དང་ཆེས་ཉེ་བར་རང་འགུལ་ཆེར་སྐྱེད་བྱེད་པ།"

msgid "Bad version specification '%s'"
msgstr "པར་གཞི་%sརྣམ་གཞག་ནོར་འཁྲུལ།"

msgid "Bell event"
msgstr "དྲིལ་དབྱངས་དོན་རྐྱེན།"

msgid ""
"Blend format is \"blend/bg_color/fg_color/alpha\", \"%s\" does not fit the "
"format"
msgstr ""
"མཉམ་བསྲེས་རྣམ་གཞག་ནིblend/bgoncolor/fg-color/alphདང་%s "
"རྣམ་གཞག་འདི་དང་མི་མཐུན།"

msgid "Boolean values must be \"true\" or \"false\" not \"%s\""
msgstr "པུར་ཐང་ནི་ངེས་པར་དུ་ངོ་མ་དང་ཡང་ན་རྫུན་མ་ཡིན་དགོས།%sཡིན་མི་རུང་།"

msgid "Border \"%s\" is unknown"
msgstr "མཐའ་སྒྲོམ་“%s”ངེས་མེད།"

msgid "Built-in display"
msgstr "ནང་སྒྲིག་བརྙན་ཤེལ།"

msgid "Button aspect ratio %g is not reasonable"
msgstr "མཐེབ་གནོན་ཞེང་ཚད་བསྡུར་%g ལུགས་མཐུན་མིན་པ།"

msgid "Button function \"%s\" does not exist in this version (%d, need %d)"
msgstr ""
"མཐེབ་གནོན་རྟེན་འབྱུང་གྲངས་“%s”དེ་པར་གཞི་འདིའི་（%dདང་།དགོས་པ་ནི་%d）ཁྲོད་མེད་པ།"

msgid ""
"Can't have a two draw_ops for a <button> element (theme specified a draw_ops"
" attribute and also a <draw_ops> element, or specified two elements)"
msgstr ""

msgid ""
"Can't have a two draw_ops for a <menu_icon> element (theme specified a "
"draw_ops attribute and also a <draw_ops> element, or specified two elements)"
msgstr ""

msgid ""
"Can't have a two draw_ops for a <piece> element (theme specified a draw_ops "
"attribute and also a <draw_ops> element, or specified two elements)"
msgstr ""

msgid "Cancel tab popup"
msgstr "Tabརེས་འཁོར་འཕར་ཐོན་དོར་བ།"

msgid ""
"Cannot specify both \"button_width\"/\"button_height\" and \"aspect_ratio\" "
"for buttons"
msgstr ""
"དུས་མཉམ་དུ་དམིགས་འཛུགས་བྱེད་ཐབས་བྲལ་པའི་མཐེབ་ནི་buomonnwidt "
"hདང་།buomonononheigh tapectratioབཅས་རེད།"

msgid "Close window"
msgstr " སྒེའུ་ཁུང་ཁ་རྒྱག་པ།"

msgid "Constant \"%s\" has already been defined"
msgstr "རྒྱུན་གྲངས“%s”ལ་མཚན་ཉིད་བཞག་ཟིན།"

msgid ""
"Coordinate expression contained unknown operator at the start of this text: "
"\"%s\""
msgstr ""
"གནས་ཚད་མཚོན་ཚུལ་ལ་ཡིག་དེབ་འདི་འགོ་ཚུགས་སར་ངེས་མེད་ཀྱི་རྩིས་རྒྱག་མཚོན་རྟགས་འདུས་པ།：“%s”"

msgid "Coordinate expression contains character '%s' which is not allowed"
msgstr "གནས་ཚད་མཚོན་ཚུལ་ནང་དུ་མི་ཆོག་པའི་ཡིག་རྟགས“%s”ཚུད་ཡོད།"

msgid ""
"Coordinate expression contains floating point number '%s' which could not be"
" parsed"
msgstr "གནས་ཚད་མཚོན་ཚུལ་ནང་དབྱེ་ཞིབ་བྱེད་ཐབས་བྲལ་པའི་འཕྱོ་ཚེག་གྲངས་%sཚུད་ཡོད།"

msgid "Coordinate expression contains integer '%s' which could not be parsed"
msgstr "གནས་ཚད་མཚོན་ཚུལ་ནང་དབྱེ་ཞིབ་བྱེད་ཐབས་བྲལ་པའི་ཧྲིལ་གྲངས་%sཚུད་ཡོད།"

msgid "Coordinate expression doesn't seem to have any operators or operands"
msgstr "གནས་ཚད་མཚོན་ཚུལ་ལ་རྩིས་རྒྱག་རྟགས་སམ་བཀོལ་སྤྱོད་གྲངས་གང་ཡང་མེད་པ་འདྲ།"

msgid "Coordinate expression ended with an operator instead of an operand"
msgstr ""
"གནས་ཚད་མཚོན་ཚུལ་ནི་རྩིས་རྒྱག་རྟགས་ཀྱིས་མཇུག་སྡུད་པ་ལས་བཀོལ་སྤྱོད་གྲངས་ཀྱིས་མཇུག་སྡུད་པ་མ་ཡིན།"

msgid "Coordinate expression had a close parenthesis with no open parenthesis"
msgstr "གནས་ཚད་མཚོན་ཚུལ་ལ་ཟུམ་པའི་གུག་རྟགས་ཤིག་ཡོད་པ་ལས་ཕྱེ་པའི་གུག་རྟགས་མེད།"

msgid ""
"Coordinate expression had an open parenthesis with no close parenthesis"
msgstr "གནས་ཚད་མཚོན་ཚུལ་ལ་ཕྱེ་བའི་གུག་རྟགས་ཤིག་ཡོད་པ་ལས་ཟུམ་པའི་གུག་རྟགས་མེད།"

msgid "Coordinate expression had an operand where an operator was expected"
msgstr ""
"གནས་ཚད་མཚོན་ཚུལ་གྱི་བཀོལ་སྤྱོད་གྲངས་ག་གེ་མོ་ཞིག་ནི་རྩིས་རྒྱག་རྟགས་ཡིན་དགོས།"

msgid "Coordinate expression had unknown variable or constant \"%s\""
msgstr "གནས་ཚད་མཚོན་ཚུལ་ལ་ངེས་མེད་ཀྱི་འགྱུར་ཚད་དམ་ཡང་ན་རྒྱུན་གྲངས་%sཡོད།"

msgid "Coordinate expression has an operator \"%s\" where an operand was expected"
msgstr "གནས་ཚད་མཚོན་ཚུལ་གྱི་རྩིས་རྒྱག་རྟགས་%sནི་བཀོལ་སྤྱོད་གྲངས་ཤིག་ཡིན་དགོས།"

msgid ""
"Coordinate expression has operator \"%c\" following operator \"%c\" with no "
"operand in between"
msgstr ""
"གནས་ཚད་མཚོན་ཚུལ་གྱི་རྩིས་རྒྱག་རྟགས“%c”ཡི་རྗེས་སུ་རྩིས་རྒྱག་རྟགས་“%c”འབྲངས་ཡོད་ཅིང་།བར་དུ་བཀོལ་སྤྱོད་གྲངས་ཀ་མེད།"

msgid "Coordinate expression parser overflowed its buffer."
msgstr "གནས་ཚད་མཚོན་ཚུལ་དེའི་ལྷོད་གཏོང་ཁུལ་ནས་ཕྱིར་ལུད་པ།"

msgid "Coordinate expression results in division by zero"
msgstr "གནས་ཚད་མཚོན་ཚུལ་གྱིས་ཐིག་ལེ་ཡིས་བགོད་བྱེད་བྱས་པ།"

msgid ""
"Coordinate expression tries to use mod operator on a floating-point number"
msgstr ""
"གནས་ཚད་མཚོན་ཚུལ་གྱིས་འཕྱོ་ཚེག་གྲངས་modརྩིས་རྒྱག་རྟགས་སྤྱོད་རྩིས་བྱེད་པ།"

msgid "Coordinate expression was empty or not understood"
msgstr "གནས་ཚད་མཚོན་ཚུལ་སྟོང་པའམ་གོ་བ་ལེན་ཐབས་བྲལ་བ།"

msgid ""
"Could not find a theme! Be sure %s exists and contains the usual themes.\n"
msgstr ""
"བརྗོད་སྙིང་རྙེད་ཐབས་བྲལ། "
"ཁྱེད་རང་གིས་%sགནས་པ་མ་ཟད་རྒྱུན་གཏན་གྱི་བརྗོད་སྙིང་ཡོད་པར་དམིགས་འཛུགས་གནང་རོགས།"
" \n"

msgid "Could not parse \"%s\" as a floating point number"
msgstr "%sདེ་གཡེང་ཚེག་གྲངས་ལ་དབྱེ་ཞིབ་བྱེད་ཐབས་བྲལ།"

msgid "Could not parse \"%s\" as an integer"
msgstr "%sཧྲིལ་གྲངས་སུ་དབྱེ་ཞིབ་བྱེད་ཐབས་བྲལ།"

msgid "Could not parse alpha value \"%s\" in blended color"
msgstr "མདོག་འདྲེས་མའི་ཁྲོད་ཀྱི་Alhaཐང“%sལ་དབྱེ་ཞིབ་བྱེད་ཐབས་བྲལ།"

msgid "Could not parse color \"%s\""
