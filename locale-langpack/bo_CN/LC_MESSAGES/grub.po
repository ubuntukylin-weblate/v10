msgid " - Total size unknown"
msgstr "--སྤྱི་འབོར་ངེས་མེད།"

msgid "%ds"
msgstr "%d སྐར་ཆ།"

msgid "%ds remaining."
msgstr "སྐར་ཆ་ %d ལྷག"

msgid ""
"%s appears to contain a %s filesystem which isn't known to reserve space for"
" DOS-style boot.  Installing GRUB there could result in FILESYSTEM "
"DESTRUCTION if valuable data is overwritten by grub-setup (--skip-fs-probe "
"disables this check, use at your own risk)"
msgstr ""

msgid ""
"%s appears to contain a %s partition map which isn't known to reserve space "
"for DOS-style boot.  Installing GRUB there could result in FILESYSTEM "
"DESTRUCTION if valuable data is overwritten by grub-setup (--skip-fs-probe "
"disables this check, use at your own risk)"
msgstr ""

msgid "%s, with Linux %s"
msgstr "\"%s，Linux %s\""

msgid "%s, with Xen %s and Linux %s"
msgstr "\"%s，Xen %s དང་ Linux %s\""

msgid "%s, with kFreeBSD %s"
msgstr "\"%s，kFreeBSD %s\""

msgid "%s, with kernel %s (via %s)"
msgstr "%s，%s ནང་སྙིང་།( %sབརྒྱུད་ཟིན།)"

msgid "%s: error:"
msgstr "%s：ནོར་འཁྲུལ།"

msgid "%s: info:"
msgstr "%s：ཆ་འཕྲིན།"

msgid "- Last modification time %d-%02d-%02d %02d:%02d:%02d %s"
msgstr "\"- ཉེ་ལམ་བཟོ་བཅོས %d-%02d-%02d %02d:%02d:%02d %s\""

msgid "-l | -r | [-s] grubdev osdisk."
msgstr "\"-l | -r | [-s] grubསྒྲིག་ཆས། རྒྱུད་ཁོངས་སྡུད་སྡེར།\""

msgid "ADDR"
msgstr " ཤག་གནས།"

msgid "ADDR VALUE [MASK]"
msgstr "ཤག་གནས། ཐང་། [གབ་ཨང་།]"

msgid "ADDR1,MASK1[,ADDR2,MASK2[,...]]"
msgstr "ཤག་གནས1,གབ་ཨང་1[,ཤག་གནས2,གབ་ཨང་2[,...]]"

msgid "ASCII"
msgstr "ASCII"

msgid "Accept DOS-style CR/NL line endings."
msgstr "DOS ཉམས་འགྱུར་གྱི་ CR/NL ཕྲེང་གི་འཇུག་སྒྲིལ་དང་ལེན་བྱེད་པ།"

msgid "Active input terminals:"
msgstr "བྱེད་སྒོའི་ནང་འཇུག་མཐའ་སྣེ།"

msgid "Active output terminals:"
msgstr "བྱེད་སྒོའི་ཕྱིར་གཏོང་མཐའ་སྣེ།"

msgid "Advanced options for %s"
msgstr "%s མཐོ་རིམ་གདམ་གསེས།"

msgid "Advanced options for %s (with Xen hypervisor)"
msgstr ""
"%s མཐོ་རིམ་གདམ་གསེས།（Xen རྟོག་བཟོས་འཕྲུལ་འཁོར་་དོ་དམ་བྱ་རིམ་སྤྱད་ཡོད།）"

msgid "Ask for file name to reboot from."
msgstr "དམིགས་འཛུགས་བྱས་པའི་ཡིག་ཆ་ནས་འགོ་རྩོམ་པ། "

msgid ""
"Attempting to install GRUB to a disk with multiple partition labels or both "
"partition label and filesystem.  This is not supported yet."
msgstr ""

msgid ""
"Attempting to install GRUB to a partitionless disk or to a partition.  This "
"is a BAD idea."
msgstr ""
"GRUBཁུལ་མ་ཕྱེ་བའི་སྡུད་སྡེར་རམ་བགོས་ཁུལ་ཞིག་ཏུ་སྒྲིག་འཇུག་བྱེད་པར་ཚོད་ལྟ་བྱེད་བཞིན་ཡོད།"
" འདི་ནི་བསམ་ཚུལ་ངན་པ་ཞིག་རེད། "

msgid "Available input terminals:"
msgstr "ནང་འཇུག་མཐའ་སྣེ་སྤྱད་ཆོག："

msgid "Available output terminals:"
msgstr "ནང་འཇུག་མཐའ་སྣེ་སྤྱད་ཆོག："

msgid "BLOCK"
msgstr "དུམ་བུ།"

msgid "Base directory for hash list."
msgstr "ཧ་ཞིའི་རེའུ་མིག་གི་ཡིག་ཆ་ཡོད་སའི་དཀར་ཆག "

msgid "Boot an operating system."
msgstr "བཀོལ་སྤྱོད་རྒྱུད་ཁོངས་ལ་སྣེ་ཁྲིད་བྱ་དགོས། "

msgid "Boot into single mode."
msgstr "སྤྱོད་མཁན་རྐྱང་པའི་རྣམ་པའི་ཐོག་ནས་སྤེལ་འགོ་ཚུགས།"

msgid "Boot with debug messages."
msgstr "ཚོད་སྒྲིག་ཆ་འཕྲིན་མངོན་འཆར། "

msgid "Boot with verbose messages."
msgstr "འཁོར་སྐྱོད་སྐབས་ཆ་འཕྲིན་ཆ་ཚང་མངོན་གྱི་ཡོད། "

msgid "Booting a command list"
msgstr "བཀའ་བརྡའི་རེའུ་མིག་ལྟར་ཁྲིད་སྟོན་བྱེད་བཞིན་ཡོད།"

msgid "Change configured devices."
msgstr "དེ་སྔོན་སྡེབ་སྒྲིག་བྱས་པའི་སྒྲིག་ཆས་ལ་བཟོ་བཅོས་རྒྱག་པ། "

msgid "Check Alt key."
msgstr "Altམཐེབ་ཞིབ་བཤེར། "

msgid "Check Control key."
msgstr "Comlམཐེབ་ཞིབ་བཤེར། "

msgid "Check Shift key."
msgstr "Shiftམཐེབ་ཞིབ་བཤེར། "

msgid "Check for CPU features."
msgstr "CPUཁྱད་གཤིས་ཞིབ་བཤེར། "

msgid "Check key modifier status."
msgstr "བརྒྱན་མཐེབ་ཀྱི་རྣམ་པ་ཞིབ་བཤེར། "

msgid "Clear the screen."
msgstr "ཡོལ་བཤེར། "

msgid "Commands:"
msgstr " བཀའ་བརྡ།："

msgid "Compare FILE with local file LOCAL."
msgstr "དམིགས་འཛུགས་བྱས་པའི་ཡིག་ཆ་དང་རང་སའི་ཡིག་ཆ་བསྡུར་བ། "

msgid "Compare two files."
msgstr "ཡིག་ཆ་གཉིས་གཤིབ་བསྡུར་བྱེད་པ། "

msgid "Compute or check hash checksum."
msgstr "ཧ་ཞིས་དག་བཤེར་སྡོམ་ལ་དག་བཤེར་དང་རྩིས་རྒྱག་པ། "

msgid "Configure serial port."
msgstr "ཕྲེང་གི་མཐུད་ཁ་སྡེབ་སྒྲིག་བྱེད་པ། "

msgid "Continue loops"
msgstr "མུ་མཐུད་རྒྱུན་འཁོར།"

msgid "Copy FILE to local file LOCAL."
msgstr "ཡིག་ཆ་འདྲ་བཟོ་བྱས་ནས་རང་སའི་གནས་སུ་སྦྱོར་བ། "

msgid "Create a blank environment block file."
msgstr "ཁོར་ཡུག་དུམ་བུའི་ཡིག་ཆ་སྟོང་པ་ཞིག་བཟོ་དགོས། "

msgid "DEVICE"
msgstr "སྒྲིག་ཆས།"

msgid "DEVICE [PARTITION[+/-[TYPE]]] ..."
msgstr "སྒྲིག་ཆས།[བགོས་ཁུལ།[+/-[རིགས་གྲས།]]]"

msgid "DEVICE must be an OS device (e.g. /dev/sda)."
msgstr ""
"དམིགས་འཛུགས་བྱས་པའི་སྒྲིག་ཆས་ནི་ངེས་པར་དུ་བཀོལ་སྤྱོད་རྒྱུད་ཁོངས་གི་སྒྲིག་ཆས་ཤིག་ཡིན་དགོས།"
" (དཔེར་ན། /dev/sda)"

msgid "DEVICE_NAME"
msgstr "སྒྲིག་ཆས་ཀྱི་མིང་།"

msgid "DIR"
msgstr "DIR"

msgid "DIRECTORY [OSBundleRequired]"
msgstr "དཀར་ཆག[OSBundleRequired]"

msgid "Debug tool for filesystem driver."
msgstr "ཡིག་ཆའི་རྒྱུད་ཁོངས་སྐུལ་བྱེད་ཚོད་སྒྲིག་ཡོ་བྱད། "

msgid "Define a submenu."
msgstr "བུའི་འདེམས་བྱང་ཞིག་ལ་མཚན་ཉིད་ཞོག"

msgid "Delete variables."
msgstr "འགྱུར་ཚད་བསུབ་པ། "

msgid "Determine driver."
msgstr "དམིགས་འཛུགས་འཁོར་སྐུལ། "

msgid "Determine filesystem UUID."
msgstr "དམིགས་འཛུགས་ཡིག་ཆའི་རྒྱུད་ཁོངས་UUID "

msgid "Determine filesystem label."
msgstr "དམིགས་འཛུགས་བྱས་པའི་ཡིག་ཆའི་རྒྱུད་ཁོངས་པོད་རྟགས། "

msgid "Determine filesystem type."
msgstr "དམིགས་འཛུགས་ཡིག་ཆའི་རྒྱུད་ཁོངས་རིགས་གྲས།"

msgid "Determine partition map type."
msgstr "དམིགས་འཛུགས་བགོས་ཁུལ་རེའུ་མིག་གི་རིགས་གྲས། "

msgid "Device %s:"
msgstr "སྒྲིག་ཆས་ %s"

msgid "Disable ACPI."
msgstr "ACPIབཀོལ་མི་ཆོག "

msgid "Disable SMP."
msgstr "SMPབཀོལ་མི་ཆོག "

msgid "Disable all boot output."
msgstr "ཁྲིད་སྟོན་དང་ཕྱིར་གཏོང་ཡོད་ཚད་བྱས་མི་ཆོག "

msgid "Disable/enable SMART (0/1)."
msgstr "སྤྱོད་མི་ཆོག/སྤྱོད་འགོ་རྩོམ SMART། (0/1)"

msgid "Display a line of text."
msgstr "ཡིག་དེབ་འཆར་བ།"

msgid "Display blocklist of FILE."
msgstr "དམིགས་འཛུགས་བྱས་པའི་ཡིག་ཆའི་དུམ་བུའི་རེའུ་མིག་འཆར་བ།"

msgid "Display output on all consoles."
msgstr "ཚོད་འཛིན་སྟེགས་བུ་ཚང་མ་ནས་ཕྱིར་འདོན་འཆར་བ། "

msgid "Display the usage of this command and exit."
msgstr "བཀའ་འདིའི་སྤྱོད་སྟངས་ཤར་ཞིང་ཕྱིར་འཐེན་བྱེད་པ། "

msgid "Display this help and exit."
msgstr "རོགས་རམ་འདི་ཤར་ནས་ཕྱིར་འཐེན་བྱེད་པ། "

msgid "Do not output the trailing newline."
msgstr "མཇུག་ཏུ་སྟོང་ཕྲེང་ལྷག་མ་ཕྱིར་གཏོང་མི་བྱེད། "

msgid "Do not print messages."
msgstr "ཁུ་སིམ་རྣམ་པ། "

msgid "Do not probe any floppy drive."
msgstr "མཉེན་སྡེར་སྐུལ་ཆས་གང་ལ་ཡང་ཞིབ་བཤེར་མི་བྱེད། "

msgid "Do not use APM to halt the computer."
msgstr "PMསྤྱད་ནས་རྩིས་འཁོར་སྒོ་མི་རྒྱག་པ། "

msgid "Do nothing, successfully."
msgstr "བཀོལ་སྤྱོད་མ་བྱས་པར་ལེགས་འགྲུབ། "

msgid "Do nothing, unsuccessfully."
msgstr "བཀོལ་སྤྱོད་མ་བྱས་པར་ཕམ་པ། "

msgid "Don't display boot diagnostic messages."
msgstr "གླེང་མོལ་ཁྲིད་སྟོན་བྱེད་པའི་ཆ་འཕྲིན་མངོན་གྱི་མེད། "

msgid "Don't load host tables specified by comma-separated list."
msgstr ""
"དམིགས་འཛུགས་བྱས་པའི་གཙོ་འཁོར་རེའུ་མིག་ནང་དུ་མི་འགོད། (མང་བ་ཚེག་གིས་བགར་ཡོད)"

msgid "Don't reboot, just halt."
msgstr "ཐད་ཀར་ཁ་རྒྱག་པ། "

msgid "Don't stop after first error."
msgstr "ཐེངས་དང་པོར་ནོར་འཁྲུལ་བྱུང་དུས་མཚམས་མི་འཇོག "

msgid "ENVVAR"
msgstr "ཁོར་ཡུག་འགྱུར་ཚད།"

msgid "ENVVAR [ENVVAR] ..."
msgstr "ཁོར་ཡུག་འགྱུར་ཚད[ཁོར་ཡུག་འགྱུར་ཚད]"

msgid "ESC at any time exits."
msgstr "ག་དུས་ཡིན་ཡང་ESCམནན་ན་ཚང་མ་ཕྱིར་འབུད་ཐུབ། "

msgid "EXPRESSION"
msgstr "མཚོན་ཚུལ།"

msgid "EXPRESSION ]"
msgstr "མཚོན་ཚུལ།]"

msgid ""
"Embedding is not possible.  GRUB can only be installed in this setup by "
"using blocklists.  However, blocklists are UNRELIABLE and their use is "
"discouraged."
msgstr ""

msgid "Enable interpretation of backslash escapes."
msgstr "ལྡོག་པའི་གསེག་གདང་མཆེད་དོན་སྤྱོད་འགོ་ཚུགས་པ།"

msgid "Enter in KDB on boot."
msgstr "འགོ་བསླངས་རྗེསKDBནང་འཛུལ་གྱི་ཡོད། "

msgid "Enter normal mode."
msgstr "སྤྱིར་བཏང་གི་རྣམ་པར་འཛུལ་གྱི་ཡོད། "

msgid "Enter password: "
msgstr "གསང་རྟགས།"

msgid "Enter username: "
