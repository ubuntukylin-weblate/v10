msgid "%s (%s)"
msgstr "%s (%s)"

msgid "<small><b>Action:</b></small>"
msgstr "<small><b>འགུལ་སྟངས།：</b></small>"

msgid "<small><b>Vendor:</b></small>"
msgstr "<small><b>འདོན་མཁན།：</b></small>"

msgid "<small><b>_Details</b></small>"
msgstr "<small><b>གནས་ཚུལ་ཞིབ་ཕྲ།(_D)</b></small>"

msgid ""
"An application is attempting to perform an action that requires privileges. "
"Authentication as one of the users below is required to perform this action."
msgstr ""

msgid ""
"An application is attempting to perform an action that requires privileges. "
"Authentication as the super user is required to perform this action."
msgstr ""
"བྱ་རིམ་ཞིག་གིས་ཁྱད་དབང་དགོས་པའི་འགུལ་སྟངས་ཤིག་ལག་བསྟར་བྱེད་ཚོད་བྱེད་པ། "
"དབང་སྤྲད་ནས་རིམ་འདས་སྤྱོད་མཁན་བྱས་ནས་འགུལ་སྟངས་འདི་ལག་བསྟར་བྱ།"

msgid ""
"An application is attempting to perform an action that requires privileges. "
"Authentication is required to perform this action."
msgstr ""
"བྱ་རིམ་ཞིག་གིས་ཁྱད་དབང་དགོས་པའི་འགུལ་སྟངས་ཤིག་ལག་བསྟར་བྱེད་ཚོད་བྱེད་པ། "
"དབང་སྤྲད་ནས་འགུལ་སྟངས་འདི་ལག་བསྟར་བྱ།"

msgid "Authenticate"
msgstr "ར་སྤྲོད།"

msgid "Authentication dialog was dismissed by the user"
msgstr "སྤྱོད་མཁན་གྱིས་དཔང་སྤྲོད་གླེང་སྒྲོམ་ཁ་རྒྱག"

msgid "Click to edit %s"
msgstr "ཆིག་རྡེབ་ནས་རྩོམ་སྒྲིག %s"

msgid "Click to open %s"
msgstr "ཆིག་རྡེབ་ནས་ཁ་འབྱེད། %s"

msgid "Select user..."
msgstr "སྤྱོད་མཁན་འདེམ་པ།..."

msgid "Your authentication attempt was unsuccessful. Please try again."
msgstr "ར་སྤྲོད་ཕམ་སོང་པས་བསྐྱར་ཚོད་བྱོས།"

msgid "_Authenticate"
msgstr "དབང་སྤྲོད།(_A)"

msgid "_Password for %s:"
msgstr "%s ཡི་གསང་ཨང་།(_P)："

msgid "_Password:"
msgstr "གསང་ཨང་།(_P)："
